function connect_iol_v_gs_addr_input
(
unsigned int tx, 
unsigned int region_index, 
unsigned int ty_iol, 
string dev_iol, 
string dev_wlsr,
unsigned int flag,  // for iol SITE tx
unsigned int flag_dev  
)
{
    string iol = dev_iol ;
    string wlsr = dev_wlsr ;
    
    int sx = tx * NUM_GRID_Y + 2;   

    int sx_iol = (flag == 0) ? sx : sx - 2 ;

    int Snum_region = region_index * NUM_TILES_REGION * NUM_GRID_Y ;

    int sy = ( MID_TILE_REGION -1 ) * NUM_GRID_Y + Snum_region ;

    int sy_iol = (flag_dev==0) ? ty_iol * NUM_GRID_Y + Snum_region : ty_iol * NUM_GRID_Y;

    if(flag_dev==0)
    {
    connect 
      (
          <pin GLOGEN of <device *iol @ [sx_iol,sy_iol]>>  => <wire GLOGEN_OUT of <device *wlsr @ [sx,sy]>>,
          <pin GRS_N of <device *iol @ [sx_iol,sy_iol]>>   => <wire BUS_GSEB_OUT[0] of <device *wlsr @ [sx,sy]>>
      );
    }
    connect 
      (
          <pin ADDR of <device *iol @ [sx_iol,sy_iol]>>    => <wire ADDR[23:22] of <device *wlsr @ [sx,sy]>>
      );        


}// end of function connect_iol_v_gs_addr_input
