function connect_MASK_CLM2BL
(
    unsigned int rx,          
    unsigned int tx,          
    unsigned int tx_blsr,          
    string blsr1,
    unsigned int flag_lr,  //blsr right or left          
    unsigned int flag_ud_r            
)
