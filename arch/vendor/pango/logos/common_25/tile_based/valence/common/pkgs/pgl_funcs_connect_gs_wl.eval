use pgl_defs;

package pgl_funcs_connect_gs_wl
{
/*******************************************************************
********************************************************************/
function connect_WLSR_gs_wl // cs ,col_inc,bus_gseb connect one by one horizon
(
    unsigned int index,
    unsigned int tx_blsr,
    unsigned int region_index,
    unsigned int l_tx_wlsrs_l[27:0],
    unsigned int l_tx_wlsrs_r[27:0],
    unsigned int l_tx_wlsr[],
    unsigned int l_tx_wlsr1[],
    unsigned int l_tx_wlsr2[],
    unsigned int l_tx_wlsra[],
    unsigned int l_tx_wlsrd[],
    unsigned int l_tx_iock[],
    string dev_blsr,
    unsigned int flag_lr,
    unsigned int flag_de_lr //direction
)
{
    int ty = region_index * NUM_TILES_REGION + MID_TILE_REGION;
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = region_index * NUM_GRID_BLSR_Y + NUM_GRID_Y;

    string blsr ,device_wlsr , device_skew ;
    blsr = dev_blsr ;

    int tx = (flag_lr==1) ? l_tx_wlsrs_l[index] : l_tx_wlsrs_r[index];
    
    int tx_skew ;

    if(index!=0)
    {
        tx_skew = (flag_lr==1) ? l_tx_wlsrs_l[index - 1] : l_tx_wlsrs_r[index - 1];
    }
    else
    {
        tx_skew = tx - 1 ; //rl,index never == 0
    }

    int tmp_tx ;

    foreach tmp_tx(l_tx_wlsr)
    {
      if(tmp_tx==tx)
      {
       device_wlsr = "WLSR_TILE";
      }
      if(tmp_tx==tx_skew)
      {
       device_skew = "WLSR_TILE";
      }
    }

    foreach tmp_tx(l_tx_wlsr1)
    {
      if(tmp_tx==tx)
      {
       device_wlsr = "WLSR1_TILE";
      }
      if(tmp_tx==tx_skew)
      {
       device_skew = "WLSR1_TILE";
      }
    }
    foreach tmp_tx(l_tx_wlsr2)
    {
      if(tmp_tx==tx)
      {
       device_wlsr = "WLSR2_TILE";
      }
      if(tmp_tx==tx_skew)
      {
       device_skew = "WLSR2_TILE";
      }
    }
    foreach tmp_tx(l_tx_wlsra)
    {
      if(tmp_tx==tx)
      {
       device_wlsr = "WLSRA_TILE";
      }
      if(tmp_tx==tx_skew)
      {
       device_skew = "WLSRA_TILE";
      }
    }
    foreach tmp_tx(l_tx_wlsrd)
    {
      if(tmp_tx==tx)
      {
       device_wlsr = "WLSRD_TILE";
      }
      if(tmp_tx==tx_skew)
      {
       device_skew = "WLSRD_TILE";
      }
    }
    foreach tmp_tx(l_tx_iock)
    {
      if(tmp_tx==tx)
      {
       device_wlsr = "IOCKGATEDLL_TILE";
      }
      if(tmp_tx==tx_skew)
      {
       device_skew = "IOCKGATEDLL_TILE";
      }
    }

    int sx = (device_wlsr == "IOCKGATEDLL_TILE") ? tx * NUM_GRID_X + 2 : tx * NUM_GRID_X + 2 ;
    int sx_skew = (device_skew == "IOCKGATEDLL_TILE") ? tx_skew * NUM_GRID_X + 2 : tx_skew * NUM_GRID_X + 2 ;
    int sy = (device_wlsr == "IOCKGATEDLL_TILE") ? ty * NUM_GRID_Y - NUM_GRID_Y : ty * NUM_GRID_Y ;
    int sy_skew = (device_skew == "IOCKGATEDLL_TILE") ? ty * NUM_GRID_Y - NUM_GRID_Y : ty * NUM_GRID_Y;

    if(flag_de_lr==0)
    {

      if(tx==(tx_blsr-1))
      {
        connect
          (  
              <pin BUS_GSEB_IN of <device *device_wlsr @ [sx,sy]>>     => <wire BUS_GSEB_OUTL of <device *blsr @ [sx_blsr,sy_blsr]>>
          );
      }
      else if(tx==(tx_blsr+1))
      {
        connect
          (  
              <pin BUS_GSEB_IN of <device *device_wlsr @ [sx,sy]>>     => <wire BUS_GSEB_OUTR of <device *blsr @ [sx_blsr,sy_blsr]>>
          );
      }
      else
      {
        if(tx < tx_blsr - 1)
        {
            connect
            (
                  <pin BUS_GSEB_IN of <device *device_wlsr @ [sx,sy]>>      => <wire BUS_GSEB_OUT of <device *device_skew @ [sx_skew,sy_skew]>>  
            );            
        }        
      }
    }
    else
    {
        connect
        (
           <pin CS_IN of <device *device_wlsr @[sx,sy]>>           => <wire CS_OUT of <device *device_skew @ [sx_skew,sy_skew]>>,
           
           <pin COL_INC_IN of <device *device_wlsr @[sx,sy]>>      =>  <wire COL_INC_OUT of <device *device_skew @ [sx_skew,sy_skew]>>
        );        
        if(tx> tx_blsr + 1)
        {
            connect
            (
                  <pin BUS_GSEB_IN of <device *device_wlsr @ [sx,sy]>>      => <wire BUS_GSEB_OUT of <device *device_skew @ [sx_skew,sy_skew]>>  
            );        
        }
    }

   
}//end of function connect_WLSR_global_sig

/***********************************************************************************************************************
************************************************************************************************************************/
function connect_iock_iob_gs_h // cs ,col_inc,bus_gseb connect one by one horizon
(
    unsigned int rx,
    unsigned int tx_iob,
    unsigned int ty_iob,
    unsigned int tx_iock,
    unsigned int tx,
    string dev_iob,
    unsigned int flag_lr 
 
)
{
    int Snum_region = rx * NUM_TILES_REGION * NUM_GRID_Y ;

    int sx = tx * NUM_GRID_X ;
    int sy = MID_TILE_REGION * NUM_GRID_Y + Snum_region ;

    string iock  =  "IOCKGATEDLL_TILE" ;
    string iob = dev_iob ;

    int sy_iob = ty_iob * NUM_GRID_Y;
    int sx_iob = tx_iob * NUM_GRID_X;

    int sy_iock = sy - NUM_GRID_Y ;
    int sx_iock = tx_iock * NUM_GRID_X + 2;
    int sx_iock_lr1 = sx_iock - NUM_GRID_X ; 
    int sx_iock_rl1 = sx_iock + NUM_GRID_X;

    int tx_lr1 = tx-1;
    int tx_rl1 = tx+1;
    int sx_lr1 = tx_lr1 * NUM_GRID_X ;
    int sx_rl1 = tx_rl1 * NUM_GRID_X ;

    if(rx==0)
    {
      connect
        (

          <pin WLSHFT_POR_ERASE_N[0] of <device *iob @[sx_iob,sy_iob]>>   => <wire BUS_GSEB_OUT[2] of <device *iock @[sx_iock,sy_iock]>>
        );

        if(flag_lr==0)
        {
            connect
            (
              <pin CS_IN[0] of <device *iob @[sx_iob,sy_iob]>> => <wire CS_OUT of <device *iock @[sx_iock,sy_iock]>>,
              <pin COL_INC_IN[0] of <device *iob @[sx_iob,sy_iob]>> => <wire COL_INC_OUT of <device *iock @[sx_iock,sy_iock]>>
            );
        }
        if(flag_lr==1)
        {
            connect
            (
              <pin CS_IN of <device *iock @[sx_iock,sy_iock]>> => <wire CS_OUT[0] of <device *iob @ [sx_iob,sy_iob]>>,
               
              <pin COL_INC_IN of <device *iock @[sx_iock,sy_iock]>> => <wire COL_INC_OUT[0] of <device *iob @ [sx_iob,sy_iob]>>  
            );
        }
    }
    else
    {
        connect
        (

          <pin WLSHFT_POR_ERASE_N[1] of <device *iob @[sx_iob,sy_iob]>>   => <wire BUS_GSEB_OUT[2] of <device *iock @[sx_iock,sy_iock]>>
        );
        if(flag_lr==0)
        {
            connect
            (
              <pin CS_IN[1] of <device *iob @[sx_iob,sy_iob]>> => <wire CS_OUT of <device *iock @[sx_iock,sy_iock]>>,
              <pin COL_INC_IN[1] of <device *iob @[sx_iob,sy_iob]>> => <wire COL_INC_OUT of <device *iock @[sx_iock,sy_iock]>>
            );
        }

        if(flag_lr==1)
        {
            connect
            (
              <pin CS_IN of <device *iock @[sx_iock,sy_iock]>> => <wire CS_OUT[1] of <device *iob @ [sx_iob,sy_iob]>> , 

              <pin COL_INC_IN of <device *iock @[sx_iock,sy_iock]>> => <wire COL_INC_OUT[1] of <device *iob @ [sx_iob,sy_iob]>> 
            ); 
        }
    }


    if(flag_lr==1)
    {
      connect
        (
          <pin BUS_GSEB_IN of <device *iock @[sx_iock,sy_iock]>>  =>  <wire BUS_GSEB_OUT of <device WLSR_TILE @ [sx_iock_rl1,sy]>>
        );
    }

    if(flag_lr==0)
    {
        connect
        (
              <pin BUS_GSEB_IN of <device WLSR_TILE @[sx,sy]>>  =>  <wire BUS_GSEB_OUT of <device WLSR_TILE @ [sx_rl1,sy]>>, 
              <pin CS_IN of <device WLSR_TILE @[sx,sy]>>  =>  <wire TIE_LO_1V1 of <device WLSR_TILE @ [sx,sy]>>, 
              <pin COL_INC_IN of <device WLSR_TILE @[sx,sy]>>  => <wire TIE_LO_1V1 of <device WLSR_TILE @ [sx,sy]>> 
        );
    }

}//end of function connect_iock_iob_gs_h
/*****************************************************************************************************************
************************************************************************************************************************/
function connect_glogen_fb_gs_input
(
    unsigned int rx,
    unsigned int tx,
    unsigned int tx_blsr,
    unsigned int num_region,
    string dev_wlsr,
    string dev_blsr1,
    string dev_blsr2
)
{
    int Snum_region = rx * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy = MID_TILE_REGION * NUM_GRID_Y + Snum_region;
    int sy_blsr = rx * NUM_GRID_BLSR_Y +  NUM_GRID_Y;
    int sy_blsr_u = (rx+1) * NUM_GRID_BLSR_Y + NUM_GRID_Y;
    int sx = tx * NUM_GRID_X;
    int sx_blsr = tx_blsr * NUM_GRID_X;
    string wlsr = dev_wlsr ;
    string blsr1 = dev_blsr1 ;
    string blsr2 = (rx==0) ? dev_blsr2 : dev_blsr1 ;

    if(rx==num_region - 1)
    {
    connect
      (
        <pin CLK_ADC_IN2 of <device *blsr1 @[sx_blsr,sy_blsr]>> => <wire GLOGEN_OUT of <device *wlsr @[sx,sy]>> 
      );
    }
    else
    {
    connect
      (
        <pin CLK_ADC_IN2 of <device *blsr2 @[sx_blsr,sy_blsr]>> => <wire CLK_ADC_R of <device *blsr1 @[sx_blsr,sy_blsr_u]>> 
      );
    }
                      
}//end of function connect_glogen_fb_gs_input
/*******************************************************************
********************************************************************/
function connect_blsr_gs_output // blsr signals by one to multi horizon 
(
    unsigned int rx,
    unsigned int tx,
    unsigned int tx_blsr,
    unsigned int tx_ccs,
    unsigned int ty_ccs,
    unsigned int ty_iob,
    string device_wlsr,
    string sig_bus_wlsrb_in,
    string sig_wlshft_ref,
    string dev_blsr,
    unsigned int flag_iob
)
{
    int sx = (device_wlsr=="IOCKGATEDLL_TILE"||device_wlsr=="IOCKGATEDLL_100H_TILE") ? tx * NUM_GRID_X + 2 : tx * NUM_GRID_X;
    int sy = ((device_wlsr=="IOCKGATEDLL_TILE")||(device_wlsr=="IOCKGATEDLL_100H_TILE")) ? (rx*NUM_TILES_REGION+MID_TILE_REGION-1)*NUM_GRID_Y
                                                                                                                     : (rx*NUM_TILES_REGION+MID_TILE_REGION)*NUM_GRID_Y;
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = rx * NUM_GRID_BLSR_Y + NUM_GRID_Y;
    int sx_ccs = tx_ccs * NUM_GRID_X;
    int sy_ccs = ty_ccs * NUM_GRID_Y;
    string wlsr = device_wlsr;
    string bus_wlsrb_in = sig_bus_wlsrb_in;
    string wlshft_ref = sig_wlshft_ref;
    string blsr = dev_blsr ;
    int sy_iob = ty_iob * NUM_GRID_Y;

    if(flag_iob==1)
    {
      connect
        (
          <pin  *bus_wlsrb_in of <device *wlsr @[sx,sy_iob]>>  => (tx>tx_blsr) ? <wire BUS_WLSR_OUTR of <device *blsr @[sx_blsr,sy_blsr]>>
                                                                                 : <wire BUS_WLSR_OUTL of <device *blsr @[sx_blsr,sy_blsr]>>,

          <pin  *wlshft_ref of <device *wlsr @[sx,sy_iob]>>   => (tx>tx_blsr) ? <wire WLSR_REF[1] of <device *blsr @[sx_blsr,sy_blsr]>>
                                                                                : <wire WLSR_REF[0] of <device *blsr @[sx_blsr,sy_blsr]>>
        );
            
    }
    else
    {
      connect
        (
          <pin BUS_WLSR_IN of <device *wlsr @[sx,sy]>>        => (tx>tx_blsr) ? <wire BUS_WLSR_OUTR of <device *blsr @[sx_blsr,sy_blsr]>>
                                                                              : <wire BUS_WLSR_OUTL of <device *blsr @[sx_blsr,sy_blsr]>>,

          <pin WLSHFT_REF of <device *wlsr @[sx,sy]>>          => (tx>tx_blsr) ? <wire WLSR_REF[1] of <device *blsr @[sx_blsr,sy_blsr]>>
                                                                               : <wire WLSR_REF[0] of <device *blsr @[sx_blsr,sy_blsr]>>

      );
    }

    if(wlsr=="WLSRD_TILE")
    {
      connect
        (
          <pin MCE of <device *wlsr @[sx,sy]>>   => (tx>tx_blsr) ? <wire MCE_OR of <device *blsr @[sx_blsr,sy_blsr]>>
                                                                 : <wire MCE_OL of <device *blsr @[sx_blsr,sy_blsr]>>,
          <pin MCLKB of <device *wlsr @[sx,sy]>> => (tx>tx_blsr) ? <wire MCLKB_OR of <device *blsr @[sx_blsr,sy_blsr]>>
                                                                 : <wire MCLKB_OL of <device *blsr @[sx_blsr,sy_blsr]>>,
          <pin MWR of <device *wlsr @[sx,sy]>>   => (tx>tx_blsr) ? <wire MWR_OR of <device *blsr @[sx_blsr,sy_blsr]>>
                                                                 : <wire MWR_OL of <device *blsr @[sx_blsr,sy_blsr]>>
        );
    }
    
}//end of function connect_blsr_gs_output
/*******************************************************************
********************************************************************/
function connect_col_wlsrdd_addr
(
    unsigned int tx_blsr,
    unsigned int tx_wlsrd,
    unsigned int num_wlsrd,
    unsigned int region_index, 
    string dev_blsr 
)
{
    int num_wlsr = num_wlsrd - 1 ;

    int sx_blsr = tx_blsr * NUM_GRID_X;

    int tx,sx,ty,sy;

    ty = MID_TILE_REGION + region_index * NUM_TILES_REGION ;

    sy = ty * NUM_GRID_Y;

    int sy_blsr = region_index * NUM_GRID_BLSR_Y + NUM_GRID_Y;

    sx = tx_wlsrd * NUM_GRID_X;        

    string blsr = dev_blsr ;

    string wlsr = "WLSRD_TILE" ;

    string col_addr = "COL_ADDR_D" ;

    string col_n = (tx_wlsrd < tx_blsr) ? "COL_L_N_O" : "COL_R_N_O" ;   
    string col = (tx_wlsrd < tx_blsr) ? "COL_L_O" : "COL_R_O" ;   

    int fflag6 = num_wlsr - 64 ;
    int flag6 = (fflag6 >= 0) ? 1 : 0 ;

    int fflag5 = num_wlsr - flag6 * 64 - 32 ;
    int flag5 = (fflag5 >= 0) ? 1 : 0 ;

    int fflag4 = num_wlsr - flag6 * 64 - flag5 * 32 - 16 ;
    int flag4 = (fflag4 >= 0) ? 1 : 0 ;

    int fflag3 = num_wlsr - flag6 * 64 - flag5 * 32 - flag4 * 16 - 8;
    int flag3 = (fflag3 >= 0) ? 1 : 0 ;

    int fflag2 = num_wlsr - flag6 * 64 - flag5 * 32 - flag4 * 16 - flag3 * 8 - 4;
    int flag2 = (fflag2 >= 0) ? 1 : 0 ;

    int fflag1 = num_wlsr - flag6 * 64 - flag5 * 32 - flag4 * 16 - flag3 * 8 - flag2 * 4 - 2;
    int flag1 = (fflag1 >= 0) ? 1 : 0 ;

    int fflag0 = num_wlsr - flag6 * 64 - flag5 * 32 - flag4 * 16 - flag3 * 8 - flag2 * 4 - flag1 * 2 - 1;
    int flag0 = (fflag0 >= 0) ? 1 : 0 ;


    connect
      (
        <pin *col_addr[6] of <device *wlsr @[sx,sy]>> => (flag6 == 1) ? <wire *col[6] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[6] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
        <pin *col_addr[5] of <device *wlsr @[sx,sy]>> => (flag5 == 1) ? <wire *col[5] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[5] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
        <pin *col_addr[4] of <device *wlsr @[sx,sy]>> => (flag4 == 1) ? <wire *col[4] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[4] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
        <pin *col_addr[3] of <device *wlsr @[sx,sy]>> => (flag3 == 1) ? <wire *col[3] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[3] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
        <pin *col_addr[2] of <device *wlsr @[sx,sy]>> => (flag2 == 1) ? <wire *col[2] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[2] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
        <pin *col_addr[1] of <device *wlsr @[sx,sy]>> => (flag1 == 1) ? <wire *col[1] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[1] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
        <pin *col_addr[0] of <device *wlsr @[sx,sy]>> => (flag0 == 1) ? <wire *col[0] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[0] of <device *blsr @ [sx_blsr,sy_blsr]>> 
      );

}//end of function connect_col_wlsrdd_addr
/*******************************************************************
********************************************************************/
function connect_col_wlsr_addr
(
    unsigned int tx_blsr,
    unsigned int tx_wlsr,
    unsigned int num_wlsr,
    unsigned int region_index,
    unsigned int ty_iob_l,
    unsigned int ty_iob_r,
    unsigned int l_wlsr_tx[],
    unsigned int l_wlsrd_tx[],
    unsigned int l_wlsra_tx[],
    unsigned int l_wlsr2_tx[],
    unsigned int l_wlsr1_tx[],
    unsigned int l_iock_tx[],
    unsigned int l_iob_tx[],
    string sig_col_addr,
    string dev_blsr,
    string dev_wlsra,
    string dev_iob_l,
    string dev_iob_r
)
{

    string col_addr = sig_col_addr ;
    string blsr = dev_blsr ;
    string iob_l = dev_iob_l ;
    string iob_r = dev_iob_r ;

    int sx_blsr = tx_blsr * NUM_GRID_X;

    int tmp_x;

    string wlsr ; 

    int ty = MID_TILE_REGION + region_index * NUM_TILES_REGION ;

    int sy_blsr = region_index * NUM_GRID_BLSR_Y + NUM_GRID_Y;

    string col_n = (tx_wlsr < tx_blsr) ? "COL_L_N_O" : "COL_R_N_O" ;   
    string col = (tx_wlsr < tx_blsr) ? "COL_L_O" : "COL_R_O" ;   

    foreach tmp_x(l_wlsr_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSR_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_wlsra_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = dev_wlsra ;
            break ;
        }
    }
    foreach tmp_x(l_wlsrd_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSRD_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_wlsr1_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSR1_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_wlsr2_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSR2_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_iock_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "IOCKGATEDLL_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_iob_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            if(tmp_x < 1)
            {
                wlsr = iob_l ;
                break ;
            }
            else
            {
                wlsr = iob_r ;
                break ;
            }
        }
    }

    int sx = (wlsr == "IOCKGATEDLL_TILE") ? tx_wlsr * NUM_GRID_X + 2 : tx_wlsr * NUM_GRID_X ;
    int sy = (wlsr == "IOCKGATEDLL_TILE") ? ty * NUM_GRID_Y - NUM_GRID_Y :
             (wlsr == iob_l) ? ty_iob_l * NUM_GRID_Y :
             (wlsr == iob_r) ? ty_iob_r * NUM_GRID_Y :
                                    ty * NUM_GRID_Y ;

    int fflag6 = num_wlsr - 64 ;
    int flag6 = (fflag6 >= 0) ? 1 : 0 ;

    int fflag5 = num_wlsr - flag6 * 64 - 32 ;
    int flag5 = (fflag5 >= 0) ? 1 : 0 ;

    int fflag4 = num_wlsr - flag6 * 64 - flag5 * 32 - 16 ;
    int flag4 = (fflag4 >= 0) ? 1 : 0 ;

    int fflag3 = num_wlsr - flag6 * 64 - flag5 * 32 - flag4 * 16 - 8;
    int flag3 = (fflag3 >= 0) ? 1 : 0 ;

    int fflag2 = num_wlsr - flag6 * 64 - flag5 * 32 - flag4 * 16 - flag3 * 8 - 4;
    int flag2 = (fflag2 >= 0) ? 1 : 0 ;

    int fflag1 = num_wlsr - flag6 * 64 - flag5 * 32 - flag4 * 16 - flag3 * 8 - flag2 * 4 - 2;
    int flag1 = (fflag1 >= 0) ? 1 : 0 ;

    int fflag0 = num_wlsr - flag6 * 64 - flag5 * 32 - flag4 * 16 - flag3 * 8 - flag2 * 4 - flag1 * 2 - 1;
    int flag0 = (fflag0 >= 0) ? 1 : 0 ;


    connect
      (
        <pin *col_addr[6] of <device *wlsr @[sx,sy]>> => (flag6 == 1) ? <wire *col[6] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[6] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
