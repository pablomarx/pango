function connect_MASK_WL2CLM
(
    unsigned int rx,          
    unsigned int tx,          
    unsigned int ty,          
    string  dev_wlsr
)
{

    int sx = tx * NUM_GRID_X + 2 ;
    int sy = ty * NUM_GRID_Y + rx * NUM_TILES_REGION *NUM_GRID_Y ;
    int sx_wlsr = tx * NUM_GRID_X ;
    int sy_wlsr = (MID_TILE_REGION) * NUM_GRID_Y + rx * NUM_TILES_REGION *NUM_GRID_Y ;

    string wlsr = dev_wlsr ;

    connect
    (
          <pin MASK_N of <device CLMS_TILE @ [sx,sy]>> =>  <wire MASK_N  of <device *wlsr @ [sx_wlsr,sy_wlsr]>>, 
          <pin READ of <device CLMS_TILE @ [sx,sy]>> =>  <wire READ2T  of <device *wlsr @ [sx_wlsr,sy_wlsr]>>, 
          <pin RST_N of <device CLMS_TILE @ [sx,sy]>> =>  <wire BUS_GSEB_OUT[2]  of <device *wlsr @ [sx_wlsr,sy_wlsr]>> 
    );


}//end of function connect_MASK_WL2CLM
/****************************************************************************************************
*****************************************************************************************************/
function connect_MASK_CLM2CLM_ll
(
    unsigned int rx,          
    unsigned int ty,          
    unsigned int l_clms_ll[6:0],          
    unsigned int index          
)
{
    int tx = l_clms_ll[index] ; 

    int sx   = tx   * NUM_GRID_X + 2;
    int sy = ty * NUM_GRID_Y + rx * NUM_TILES_REGION *NUM_GRID_Y ;

    if(!(index==6))
    {
        int tx_b = l_clms_ll[index + 1] ; 
        int sx_b = tx_b * NUM_GRID_X + 2 ;

        connect
        (
           <pin MASK_DATA_ACIN of <device CLMS_TILE @ [sx,sy]>> =>  <wire  MASK_DATA_ACOUT  of <device CLMS_TILE @ [sx_b,sy]>> , 
           <pin MASK_DATA_BCIN of <device CLMS_TILE @ [sx,sy]>> =>  <wire  MASK_DATA_BCOUT  of <device CLMS_TILE @ [sx_b,sy]>> , 
           <pin MASK_DATA_CCIN of <device CLMS_TILE @ [sx,sy]>> =>  <wire  MASK_DATA_CCOUT  of <device CLMS_TILE @ [sx_b,sy]>> , 
           <pin MASK_DATA_DCIN of <device CLMS_TILE @ [sx,sy]>> =>  <wire  MASK_DATA_DCOUT  of <device CLMS_TILE @ [sx_b,sy]>>  
        );
    }

    if(index==6)
    {
        connect
        (
          <pin MASK_DATA_ACIN of <device CLMS_TILE @ [sx,sy]>> => <wire  TIELO  of <device CLMS_TILE @ [sx,sy]>> ,   
          <pin MASK_DATA_BCIN of <device CLMS_TILE @ [sx,sy]>> => <wire  TIELO  of <device CLMS_TILE @ [sx,sy]>> ,   
          <pin MASK_DATA_CCIN of <device CLMS_TILE @ [sx,sy]>> => <wire  TIELO  of <device CLMS_TILE @ [sx,sy]>> ,   
          <pin MASK_DATA_DCIN of <device CLMS_TILE @ [sx,sy]>> => <wire  TIELO  of <device CLMS_TILE @ [sx,sy]>>    
        );     
    }

}//end of function connect_MASK_CLM2CLM_ll
/****************************************************************************************************
*****************************************************************************************************/
function connect_MASK_CLM2CLM_rr
(
    unsigned int rx,          
    unsigned int ty,          
    unsigned int l_clms_ll[7:0],          
    unsigned int l_clms_rr_r[6:0],          
    unsigned int index,          
    unsigned int flag // for which array          
)
{
    int tx = (flag==0) ? l_clms_ll[index] :
                         l_clms_rr_r[index] ;

    int sx   = tx   * NUM_GRID_X + 2;
    int sy = ty * NUM_GRID_Y + rx * NUM_TILES_REGION *NUM_GRID_Y ;

    if(!(index==0))
    {
        int tx_f = (flag==0) ? l_clms_ll[index - 1] :
                               l_clms_rr_r[index - 1] ; 
        int sx_f = tx_f * NUM_GRID_X + 2 ;

        connect
        (
           <pin MASK_DATA_ACIN of <device CLMS_TILE @ [sx,sy]>> =>  <wire  MASK_DATA_ACOUT  of <device CLMS_TILE @ [sx_f,sy]>> , 
