/****************************************************************************************************
*****************************************************************************************************/
function connect_ccs_wl_bl_input
(
    unsigned int tx,          
    unsigned int ty,          
    unsigned int tx_wlsr,          
    unsigned int tx_blsr,          
    unsigned int ty_blsr2,
    unsigned int ty_blsr1,
    string dev_wlsr,
    string dev_wlsrl,
    string dev_blsr1,
    string dev_blsr2
)
{
    string wlsr = dev_wlsr ;
    string wlsrl = dev_wlsrl ;
    string blsr1 = dev_blsr1 ;
    string blsr2 = dev_blsr2 ;
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr2 = ty_blsr2 * NUM_GRID_BLSR_Y+ NUM_GRID_Y;
    int sy_blsr1 = ty_blsr1 * NUM_GRID_BLSR_Y+ NUM_GRID_Y;
    int sx = tx * NUM_GRID_X;
    int sy = ty * NUM_GRID_Y;
    int sx_wlsr = tx_wlsr * NUM_GRID_X + 2;
    int sx_wlsrl = tx_wlsr * NUM_GRID_X + 2 - NUM_GRID_X;
    int sy_wlsr = MID_TILE_REGION * NUM_GRID_Y - NUM_GRID_Y;
    int sy_wlsrl = MID_TILE_REGION * NUM_GRID_Y ;
    int sy_wlsr_u = MID_TILE_REGION * NUM_GRID_Y - NUM_GRID_Y + NUM_TILES_REGION * NUM_GRID_Y;

    string data = "DATA" ;
    string datan = "DATAN" ;


    connect
      (
      <pin DATA[61:0]  of <device CCS_TILE @ [sx,sy]>>  => <wire *data[71:10]      of <device *blsr2 @ [sx_blsr,sy_blsr2]>>,
      <pin DATAN[61:0] of <device CCS_TILE @ [sx,sy]>>  => <wire *datan[71:10]     of <device *blsr2 @ [sx_blsr,sy_blsr2]>>,
      <pin DATA[133:62]  of <device CCS_TILE @ [sx,sy]>>  => <wire *data[143:72]     of <device *blsr2 @ [sx_blsr,sy_blsr2]>>,
      <pin DATAN[133:62] of <device CCS_TILE @ [sx,sy]>>  => <wire *datan[143:72]    of <device *blsr2 @ [sx_blsr,sy_blsr2]>>,
      <pin DATA[205:134]  of <device CCS_TILE @ [sx,sy]>>  => <wire *data[215:144]    of <device *blsr2 @ [sx_blsr,sy_blsr2]>>,
      <pin DATAN[205:134] of <device CCS_TILE @ [sx,sy]>>  => <wire *datan[215:144]   of <device *blsr2 @ [sx_blsr,sy_blsr2]>>,
      <pin DATA[267:206]  of <device CCS_TILE @ [sx,sy]>> => <wire *data[2165:2104]   of <device *blsr1 @ [sx_blsr,sy_blsr1]>>,
      <pin DATAN[267:206] of <device CCS_TILE @ [sx,sy]>> => <wire *datan[2165:2104]  of <device *blsr1 @ [sx_blsr,sy_blsr1]>>,

        <pin ADDR_U of <device CCS_TILE @[sx,sy]>>            => <wire ADDR[23:22] of <device  *wlsr @ [sx_wlsr,sy_wlsr_u]>>,
        <pin ADDR[5:0] of <device CCS_TILE @[sx,sy]>>            => <wire ADDR[27:22] of <device  *wlsrl @ [sx_wlsrl,sy_wlsrl]>>,
        <pin ADDR_D of <device CCS_TILE @[sx,sy]>>             => <wire ADDR[23:22] of <device  *wlsr @ [sx_wlsr,sy_wlsr]>>
      );


}//end of function connect_WLSR_bl_input
/****************************************************************************************************
*****************************************************************************************************/
function connect_MASK_BL2WL
(
    unsigned int rx,          
    unsigned int tx,          
    unsigned int tx_blsr,
    string  dev_blsr,
    string  dev_wlsr,
    unsigned int flag_lr //for region 02 or 13
)
{

    int sx = tx * NUM_GRID_X ;
    int sy_blsr = rx * NUM_GRID_BLSR_Y + NUM_GRID_Y;
    int sy = (MID_TILE_REGION) * NUM_GRID_Y + rx * NUM_TILES_REGION *NUM_GRID_Y ;

    int sx_blsr =  tx_blsr * NUM_GRID_X ; 

    string blsr = dev_blsr ;
    string wlsr = dev_wlsr ;

    connect
    (
          <pin MASK of <device *wlsr @ [sx,sy]>> => (tx<tx_blsr) ? <wire MASK_OL  of <device *blsr @ [sx_blsr,sy_blsr]>> : 
                                                                   <wire MASK_OR  of <device *blsr @ [sx_blsr,sy_blsr]>>
    );


}//end of function connect_MASK_BL2WL
