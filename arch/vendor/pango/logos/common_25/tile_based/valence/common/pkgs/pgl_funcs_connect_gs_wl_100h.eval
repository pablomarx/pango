/***********************************************************************************************************************
************************************************************************************************************************/
function connect_iock_iob_gs_h_100h // cs ,col_inc,bus_gseb connect one by one horizon
(
    unsigned int rx,
    unsigned int tx_iob,
    unsigned int ty_iob,
    unsigned int tx_iock,
    unsigned int tx,
    string dev_iob,
    string sig_wlshft_por_erase_n,
    string sig_cs_in,
    string sig_col_inc_in,
    string sig_cs_out,
    string sig_col_inc_out,
    unsigned int flag_lr 
 
)
{
    int Snum_region = rx * NUM_TILES_REGION * NUM_GRID_Y ;

    int sx = tx * NUM_GRID_X ;
    int sy = MID_TILE_REGION * NUM_GRID_Y + Snum_region ;

    string iock  =  "IOCKGATEDLL_100H_TILE" ;
    string iob = dev_iob ;
    string wlshft_por_erase_n = sig_wlshft_por_erase_n ;
    string cs_in = sig_cs_in ;
    string col_inc_in = sig_col_inc_in ;
    string cs_out = sig_cs_out ;
    string col_inc_out = sig_col_inc_out ;

    int sy_iob = ty_iob * NUM_GRID_Y;
    int sx_iob = tx_iob * NUM_GRID_X;

    int sy_iock = sy - NUM_GRID_Y ;
    int sx_iock = tx_iock * NUM_GRID_X + 2;
    int sx_iock_lr1 = sx_iock - NUM_GRID_X ; 
    int sx_iock_rl1 = sx_iock + NUM_GRID_X;

    int tx_lr1 = tx-1;
    int tx_rl1 = tx+1;
    int sx_lr1 = tx_lr1 * NUM_GRID_X ;
    int sx_rl1 = tx_rl1 * NUM_GRID_X ;

    connect(<pin *wlshft_por_erase_n of <device *iob @[sx_iob,sy_iob]>>   => <wire BUS_GSEB_OUT[2] of <device *iock @[sx_iock,sy_iock]>>);

    if(flag_lr==0)
    {
        connect
        (
          <pin *cs_in of <device *iob @[sx_iob,sy_iob]>> => <wire CS_OUT of <device *iock @[sx_iock,sy_iock]>>,
          <pin *col_inc_in of <device *iob @[sx_iob,sy_iob]>> => <wire COL_INC_OUT of <device *iock @[sx_iock,sy_iock]>>
        );
    }

    if(flag_lr==1)
    {
        connect
        (
          <pin CS_IN of <device *iock @[sx_iock,sy_iock]>> => <wire *cs_out of <device *iob @ [sx_iob,sy_iob]>>,
           
          <pin COL_INC_IN of <device *iock @[sx_iock,sy_iock]>> => <wire *col_inc_out of <device *iob @ [sx_iob,sy_iob]>>  
        );
    }

    if(flag_lr==1)
    {
      connect(<pin BUS_GSEB_IN of <device *iock @[sx_iock,sy_iock]>>  =>  <wire BUS_GSEB_OUT of <device WLSR_TILE @ [sx_iock_rl1,sy]>>);
    }

    if(flag_lr==0)
    {
        connect
        (
              <pin BUS_GSEB_IN of <device WLSR_TILE @[sx,sy]>>  =>  <wire BUS_GSEB_OUT of <device WLSR_TILE @ [sx_rl1,sy]>>, 
              <pin CS_IN of <device WLSR_TILE @[sx,sy]>>  =>  <wire TIE_LO_1V1 of <device WLSR_TILE @ [sx,sy]>>, 
              <pin COL_INC_IN of <device WLSR_TILE @[sx,sy]>>  => <wire TIE_LO_1V1 of <device WLSR_TILE @ [sx,sy]>> 
        );
    }

}//end of function connect_iock_iob_gs_h
