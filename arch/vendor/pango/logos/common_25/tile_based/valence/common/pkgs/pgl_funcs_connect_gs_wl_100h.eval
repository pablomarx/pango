/****************************************************************************************************
*****************************************************************************************************/
function connect_DRM_bl_input_100h
(
    unsigned int tx,
    unsigned int rx, 
    unsigned int tx_blsr,
    string dev_blsr,
    string dev_srb,
    unsigned int flag_srb,
    unsigned int flag_begin,
    unsigned int flag_y0,
    unsigned int flag_y1,
    unsigned int flag_y2,
    unsigned int flag_y3,
    unsigned int flag_y4,
    unsigned int flag_y5    
)
{
    string blsr = dev_blsr ;
    string srb = dev_srb ;
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = rx * NUM_GRID_BLSR_Y + flag_begin * NUM_GRID_Y;
    int sx = (flag_srb == 1) ? tx * NUM_GRID_X + 2 : tx * NUM_GRID_X;

    int Snum_region = rx * NUM_TILES_REGION *NUM_GRID_Y + flag_begin * NUM_GRID_Y;   

    string data = "DATA" ;
    string datan = "DATAN" ;

    if(flag_y0==1)
    {
        connect
        (
            <pin DATA  of <device *srb @ [sx,Snum_region]>>  => <wire *data[359:0]     of <device *blsr @ [sx_blsr,sy_blsr]>>,
            <pin DATAN of <device *srb @ [sx,Snum_region]>>  => <wire *datan[359:0]    of <device *blsr @ [sx_blsr,sy_blsr]>>
        );
    }
    if(flag_y1==1)
    {
        connect
        (
            <pin DATA  of <device *srb @ [sx,5*NUM_GRID_Y+Snum_region]>>  => <wire *data[719:360]    of <device *blsr @ [sx_blsr,sy_blsr]>>,
            <pin DATAN of <device *srb @ [sx,5*NUM_GRID_Y+Snum_region]>>  => <wire *datan[719:360]   of <device *blsr @ [sx_blsr,sy_blsr]>>
        );
    }
    if(flag_y2==1)
    {
        connect
        (
            <pin DATA  of <device *srb @ [sx,10*NUM_GRID_Y+Snum_region]>> => <wire *data[1079:720]   of <device *blsr @ [sx_blsr,sy_blsr]>>,
            <pin DATAN of <device *srb @ [sx,10*NUM_GRID_Y+Snum_region]>> => <wire *datan[1079:720]  of <device *blsr @ [sx_blsr,sy_blsr]>>
        );
    }
    if(flag_y3==1)
    {
        connect
        (
            <pin DATA  of <device *srb @ [sx,16*NUM_GRID_Y+Snum_region]>> => <wire *data[1455:1096]  of <device *blsr @ [sx_blsr,sy_blsr]>>,
            <pin DATAN of <device *srb @ [sx,16*NUM_GRID_Y+Snum_region]>> => <wire *datan[1455:1096] of <device *blsr @ [sx_blsr,sy_blsr]>>
        );
    }
    if(flag_y4==1)
    {
        connect
        (
            <pin DATA  of <device *srb @ [sx,21*NUM_GRID_Y+Snum_region]>> => <wire *data[1815:1456]  of <device *blsr @ [sx_blsr,sy_blsr]>>,
            <pin DATAN of <device *srb @ [sx,21*NUM_GRID_Y+Snum_region]>> => <wire *datan[1815:1456] of <device *blsr @ [sx_blsr,sy_blsr]>>
        );
    }
    if(flag_y5==1)
    {
        connect
        (
            <pin DATA  of <device *srb @ [sx,26*NUM_GRID_Y+Snum_region]>> => <wire *data[2175:1816]  of <device *blsr @ [sx_blsr,sy_blsr]>>,
            <pin DATAN of <device *srb @ [sx,26*NUM_GRID_Y+Snum_region]>> => <wire *datan[2175:1816] of <device *blsr @ [sx_blsr,sy_blsr]>>
