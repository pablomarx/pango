    int Snum_region = rx * NUM_TILES_REGION * NUM_GRID_Y ;

    int sx_blsr_l = tx_blsr_l * NUM_GRID_X ;
    int sx_blsr_r = tx_blsr_r * NUM_GRID_X ;
    int sy_blsr = Snum_region + NUM_GRID_Y ;
    int sx_iock_r = sx ;
    int sx_iock_l = tx_iock_l * NUM_GRID_X + 2;

    int sy_iock = ( MID_TILE_REGION -1 ) * NUM_GRID_Y + Snum_region ;

    string analog ,blsr1,blsr2;

    analog = dev_analog ;
    blsr1 = dev_blsr1 ;
    blsr2 = dev_blsr2 ;

    if(flag_d==1)
    {
        connect 
        (
             <pin ADDR_D of <device *analog @ [sx,sy]>>   => <wire ADDR[23:22] of <device IOCKGATEDLL_100H_TILE @ [sx_iock_l,sy_iock]>>,
             <pin ADDR of <device *analog @ [sx,sy]>>   => <wire ADDR[23:22] of <device IOCKGATEDLL_100H_TILE @ [sx_iock_r,sy_iock]>>,
             <pin DATA[61:0] of <device *analog @ [sx,sy]>>   => <wire DATA[71:10] of <device *blsr1 @ [sx_blsr_l,sy_blsr]>>,
             <pin DATA[133:62] of <device *analog @ [sx,sy]>>   => <wire DATA[359:288] of <device *blsr2 @ [sx_blsr_r,sy_blsr]>>,
             <pin DATAN[61:0] of <device *analog @ [sx,sy]>>   => <wire DATAN[71:10] of <device *blsr1 @ [sx_blsr_l,sy_blsr]>>,
             <pin DATAN[133:62] of <device *analog @ [sx,sy]>>   => <wire DATAN[359:288] of <device *blsr2 @ [sx_blsr_r,sy_blsr]>>,

             <pin GWEN of <device *analog @ [sx,sy]>>    => <wire BUS_GSEB_OUT[1] of <device IOCKGATEDLL_100H_TILE @ [sx_iock_r,sy_iock]>>
        );
    }
    else
    {
        connect 
        (
             <pin ADDR_U of <device *analog @ [sx,sy]>>   => <wire ADDR[23:22] of <device IOCKGATEDLL_100H_TILE @ [sx_iock_l,sy_iock]>>,
             <pin DATA[195:134] of <device *analog @ [sx,sy]>>   => <wire DATA[2165:2104] of <device *blsr1 @ [sx_blsr_l,sy_blsr]>>,
             <pin DATAN[195:134] of <device *analog @ [sx,sy]>>  => <wire DATAN[2165:2104] of <device *blsr1 @ [sx_blsr_l,sy_blsr]>>
        );
    }

}// end of function connect_analog_wl_bl_input
/*******************************************************************
********************************************************************/
function connect_col_wlsr_addr_100h
(
    unsigned int tx_blsr,
    unsigned int tx_wlsr,
    unsigned int num_wlsr,
    unsigned int region_index,
    unsigned int ty_iob_l,
    unsigned int ty_iob_r,
    unsigned int l_wlsr_tx[],
    unsigned int l_wlsrd_tx[],
    unsigned int l_wlsra_tx[],
    unsigned int l_wlsr2_tx[],
    unsigned int l_wlsr1_tx[],
    unsigned int l_wlsr1b_tx[],
    unsigned int l_iock_tx[],
    unsigned int l_iob_tx[],
    string sig_col_addr,
    string dev_blsr,
    string dev_wlsra,
    string dev_iob_l,
    string dev_iob_r
)
{

    string col_addr = sig_col_addr ;
    string blsr = dev_blsr ;
    string iob_l = dev_iob_l ;
    string iob_r = dev_iob_r ;

    int sx_blsr = tx_blsr * NUM_GRID_X;

    int tmp_x;

    string wlsr ; 

    int ty = MID_TILE_REGION + region_index * NUM_TILES_REGION ;

    int sy_blsr = region_index * NUM_GRID_BLSR_Y + NUM_GRID_Y;

    string col_n = (tx_wlsr < tx_blsr) ? "COL_L_N_O" : "COL_R_N_O" ;   
    string col = (tx_wlsr < tx_blsr) ? "COL_L_O" : "COL_R_O" ;   

    foreach tmp_x(l_wlsr_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSR_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_wlsra_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = dev_wlsra ;
            break ;
        }
    }
    foreach tmp_x(l_wlsrd_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSRD_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_wlsr1_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSR1_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_wlsr1b_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSR1_100H_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_wlsr2_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSR2_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_iock_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "IOCKGATEDLL_100H_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_iob_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            if(tmp_x < 1)
            {
                wlsr = iob_l ;
                break ;
            }
            else
            {
                wlsr = iob_r ;
                break ;
            }
        }
    }

    int sx = (wlsr == "IOCKGATEDLL_100H_TILE") ? tx_wlsr * NUM_GRID_X + 2 : tx_wlsr * NUM_GRID_X ;
    int sy = (wlsr == "IOCKGATEDLL_100H_TILE") ? ty * NUM_GRID_Y - NUM_GRID_Y :
             (wlsr == iob_l) ? ty_iob_l * NUM_GRID_Y :
             (wlsr == iob_r) ? ty_iob_r * NUM_GRID_Y :
                                    ty * NUM_GRID_Y ;

    int fflag6 = num_wlsr - 64 ;
    int flag6 = (fflag6 >= 0) ? 1 : 0 ;

    int fflag5 = num_wlsr - flag6 * 64 - 32 ;
    int flag5 = (fflag5 >= 0) ? 1 : 0 ;

    int fflag4 = num_wlsr - flag6 * 64 - flag5 * 32 - 16 ;
    int flag4 = (fflag4 >= 0) ? 1 : 0 ;

    int fflag3 = num_wlsr - flag6 * 64 - flag5 * 32 - flag4 * 16 - 8;
    int flag3 = (fflag3 >= 0) ? 1 : 0 ;

    int fflag2 = num_wlsr - flag6 * 64 - flag5 * 32 - flag4 * 16 - flag3 * 8 - 4;
    int flag2 = (fflag2 >= 0) ? 1 : 0 ;

    int fflag1 = num_wlsr - flag6 * 64 - flag5 * 32 - flag4 * 16 - flag3 * 8 - flag2 * 4 - 2;
    int flag1 = (fflag1 >= 0) ? 1 : 0 ;

    int fflag0 = num_wlsr - flag6 * 64 - flag5 * 32 - flag4 * 16 - flag3 * 8 - flag2 * 4 - flag1 * 2 - 1;
    int flag0 = (fflag0 >= 0) ? 1 : 0 ;


    connect
      (
        <pin *col_addr[6] of <device *wlsr @[sx,sy]>> => (flag6 == 1) ? <wire *col[6] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[6] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
        <pin *col_addr[5] of <device *wlsr @[sx,sy]>> => (flag5 == 1) ? <wire *col[5] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[5] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
        <pin *col_addr[4] of <device *wlsr @[sx,sy]>> => (flag4 == 1) ? <wire *col[4] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[4] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
        <pin *col_addr[3] of <device *wlsr @[sx,sy]>> => (flag3 == 1) ? <wire *col[3] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[3] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
        <pin *col_addr[2] of <device *wlsr @[sx,sy]>> => (flag2 == 1) ? <wire *col[2] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[2] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
        <pin *col_addr[1] of <device *wlsr @[sx,sy]>> => (flag1 == 1) ? <wire *col[1] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[1] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
        <pin *col_addr[0] of <device *wlsr @[sx,sy]>> => (flag0 == 1) ? <wire *col[0] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                       <wire *col_n[0] of <device *blsr @ [sx_blsr,sy_blsr]>> 
      );

}//end of function connect_col_wlsr_addr

}//end of package pgl_funcs_connect_global_sig

