            { 
             
               connect(<pin *pin_logic_device of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire *pin_SRB of <device SRB_TILE@[sx_SRB,sy_SRB]>>);
            }
            else
            {
              connect(<pin *pin_SRB of<device SRB_TILE@[sx_SRB,sy_SRB]>> => (incVar.port_name_device == "TIEHI") ? <wire TIEHI of <device SRB_TILE@[sx_SRB,sy_SRB]>>
                                                                         : <wire *pin_logic_device of <device ANALOG_PGL100H_TILE @[sx,sy]>>); 
            }
        }

