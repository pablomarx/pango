  unsigned int tx,
  unsigned int ty,
  unsigned int ty_srb_h
)
{
  int sx = tx*NUM_GRID_X + 2;

  int tmp_ty,sy;
  sy = ty * NUM_GRID_Y;
  for(tmp_ty=ty;tmp_ty > MID_TILE_REGION;tmp_ty = tmp_ty - NUM_TILES_REGION)
  {
    sy = sy + NUM_GRID_WLSR_Y - NUM_GRID_Y;
  }

    string strVar;

    sprintf(strVar,"ntIPOLY10U_%d_%d",sx,sy);
    &wire ntRef_ntIPOLY10U[26:0] = wire *strVar[26:0];

    sprintf(strVar,"ntICONST10U_%d_%d",sx,sy);
    &wire ntRef_ntICONST10U[24:0] = wire *strVar[24:0];

    sprintf(strVar,"ntIPOLY20U_%d_%d",sx,sy);
    &wire ntRef_ntIPOLY20U[19:0] = wire *strVar[19:0];

    sprintf(strVar,"ntNGB_LDOLD_%d_%d",sx,sy);
    &wire ntRef_ntNGB_LDOLD = wire *strVar;

    sprintf(strVar,"ntNGB_LDORD_%d_%d",sx,sy);
    &wire ntRef_ntNGB_LDORD = wire *strVar;

    sprintf(strVar,"ntNGB_LDOLU_%d_%d",sx,sy);
    &wire ntRef_ntNGB_LDOLU = wire *strVar;

    sprintf(strVar,"ntPOR_N_%d_%d",sx,sy);
