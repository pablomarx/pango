     STRUCT inc_spec_PIOMUX incVar;
     string pin_SRB;
     string pin_logic_device;
     string attribute_IO;
     int loc_SRB;
     int sy_SRB;
   
     unsigned int sx_SRB;
     sx_SRB = sx-2;
     int cnt;

     foreach incVar(inc_PIOMUX_SRB_spec)
     {
         pin_SRB = incVar.port_name_SRB;
         pin_logic_device = incVar.port_name_device;
         attribute_IO = incVar.attribute_IO;
         loc_SRB = incVar.loc_SRB;
        
         sy_SRB = sy;
         
          if(attribute_IO =="I")
          { 
            if(pin_SRB == "TIEH") 
            {
                connect(<pin *pin_logic_device of <device PIOMUX_TILE @[sx,sy]>> => <wire *pin_SRB of <device PIOMUX_TILE@[sx,sy]>>);
            }
            else
            {
                connect(<pin *pin_logic_device of <device PIOMUX_TILE @[sx,sy]>> => <wire *pin_SRB of <device SRB_TILE@[sx_SRB,sy_SRB]>>);
            }
          }
          else
          {
            connect
            (
              <pin *pin_SRB of<device SRB_TILE@[sx_SRB,sy_SRB]>> => (incVar.port_name_device == "TIEHI") ? <wire TIEHI of <device SRB_TILE @ [sx_SRB,sy_SRB]>>
                                                                                                         : <wire *pin_logic_device of <device PIOMUX_TILE @[sx,sy]>>
            ); 
          }
     }

