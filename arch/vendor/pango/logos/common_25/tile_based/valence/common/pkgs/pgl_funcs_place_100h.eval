        <pin IBM1_IN[11] of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_1 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM1_IN[12] of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_1 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
