        <pin IBM1_IN[3]  of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_1 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM1_IN[4]  of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_1 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM1_IN[5]  of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_1 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM1_IN[6]  of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_1 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
