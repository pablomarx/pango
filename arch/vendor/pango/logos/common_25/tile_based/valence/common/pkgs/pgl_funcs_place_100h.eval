        <pin IBM1_IN[14] of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_1 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM1_IN[15] of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_1 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM1_IN[16] of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_1 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM1_IN[17] of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_1 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM1_IN[18] of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_1 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM1_IN[19] of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_1 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM2_IN[0]  of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_2 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM2_IN[1]  of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_2 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM2_IN[2]  of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_2 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
        <pin IBM2_IN[3]  of <device ANALOG_PGL100H_TILE @[sx,sy]>> => <wire TIEHI_2 of <device ANALOG_PGL100H_TILE @[sx,sy]>>,
