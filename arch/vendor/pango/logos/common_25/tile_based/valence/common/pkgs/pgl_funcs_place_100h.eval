                        TIEHI_2        =>    ntRef_ntTIEHI_2,       
                        TIEHI_3        =>    ntRef_ntTIEHI_3,       
                        ATP_O        =>    ntRef_ntATP_O,       
                        ATP_DUMMY_O        =>    ntRef_ntATP_DUMMY_O,       
