prim
device MULT
{
    parameter
    (
        config int IWIDTH = 1,
        config int OWIDTH = 1
    );
    port 
    ( 
        // Input Ports of MULT
        input X[IWIDTH-1:0],
        input Y[IWIDTH-1:0],
        input XSIGNED,
        input YSIGNED,
        
        // Output Ports for MULT
        output P[OWIDTH-1:0]
    );

};


