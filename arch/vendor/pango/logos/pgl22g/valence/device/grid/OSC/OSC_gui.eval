symbol logsym of OSC // pragma PAP_ARC_COLOR="210:45:45"
{
    // The bounding box
    generate ( 60 # 60 );
    //
    // Poloygon declaration
    //
    shape
    [ 0, 0 ]  ->  [ , 60 ] <
    
                             RST_N           @[ ,20],
                             OSC_EN_SRB      @[ ,40]
                           
                           > 

              ->  [ 60, ]                                                         

              ->  [ , 0]  <
                             
                             CLK_USER        @[ ,30]                            
                
                          > 

              ->  [ 0, ]  ;

}; // symbol logsym of OSC

