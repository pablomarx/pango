unsigned int draw_x = 0;
unsigned int draw_y = 0;
unsigned int is_print_fp_map = 0;

function print_inst_fp_map(string inst_name, unsigned int lsx, unsigned int lsy)
{
    if (is_print_fp_map)
    {
        printf("%s (%d, %d) => (%d, %d)", inst_name, lsx, lsy, fsx, fsy);
    }
}


function adjust_floorplan_draw(unsigned int lsx, lsy)
{
    if (lsx / NUM_GRID_X < 21)
    {
        draw_x = 0;
    }
    else if (lsx / NUM_GRID_X < 59)
    {
        draw_x = 1;
    }
    else
    {
        draw_x = 2;
    }


    if (lsy / NUM_GRID_Y < 16)
    {
        draw_y = 0;
    }
    else if (lsy / NUM_GRID_Y < 47)
    {
        draw_y = 1;
    }
    else if (lsy / NUM_GRID_Y < 78)
    {
        draw_y = 2;
    }
    else
    {
        draw_y = 3;
    }
    
    draw_x = 320 * draw_x;
    draw_y = 320 * draw_y;
} 

function adjust_floorplan_gap(unsigned int lsx, lsy)
{
    if (lsx / NUM_GRID_X < 21)
    {
        gsx = 0;
    }
    else if (lsx / NUM_GRID_X < 59)
    {
        gsx = 1;
    }
    else
    {
        gsx = 2;
    }

    if (lsy / NUM_GRID_Y < 16)
    {
        gsy = 0;
    }
    else if (lsy / NUM_GRID_Y < 47)
    {
        gsy = 1;
    }
    else if (lsy / NUM_GRID_Y < 78)
    {
        gsy = 2;
    }
    else 
    {
        gsy = 3;
    }

    gsx = lsx - NUM_GRID_X * gsx;
    gsy = lsy - NUM_GRID_Y * gsy;
}  
function get_coordinate_of_iol(unsigned int lsx, lsy)    
{ 
    unsigned int tem_x = 0;
    unsigned int tem_y = 0;       
    adjust_floorplan_gap(lsx, lsy);
 
    if (lsy < 8)
    {
        // iol in bottom
        fsx = (gsx / NUM_GRID_X)* 2;
        if (lsy == 6)
        {
            fsy = 0;
        }
        else
        {
            fsy = 1;
        }
        fsx = fsx + 2;
    }
  else if (lsy > 370)
    {
        // iol in top
        fsx = (gsx / NUM_GRID_X)* 2;
        if (lsy == 374)
        {
            fsy = 90;
        }
        else
        {
            fsy = 91;
        }
        fsx = fsx + 2;
    }
    else if (lsx < 8)
    {
       // iol in left
      tem_x = gsy % 8;
      tem_y = gsy / NUM_GRID_Y + (gsy - 1) % 2;
    
      if (tem_x < 3)
      {
         fsx = 0;
         fsy = tem_y;
      }
      else
      {
         fsx = 2;
         fsy = tem_y - 1;
      }            
    }
    else
    {
      // iol in rigth
      tem_x = gsy % 8;
      tem_y = gsy / NUM_GRID_Y + (gsy - 1) % 2;

      if (tem_x < 3)
      {
         fsx = 0;
         fsy = tem_y;
      }
      else
      {
         fsx = 2;
         fsy = tem_y - 1;
      }
      if(lsy % 2)
      {
         fsy = fsy + 1;
      }
      else
      {
         fsy = fsy - 1;
      }      
      fsx = fsx + 162;
       
    }
}    

function get_coordinate_of_iob(unsigned int lsx, lsy)
{
       unsigned int tem_x = 0;
       unsigned int tem_y = 0;
       adjust_floorplan_gap(lsx, lsy);
        if (lsy < 7)
        {
            // iob in bottom
            fsx = (gsx / NUM_GRID_X) * 2 + 1;
            if (gsx % 4 == 0)
            {
                fsy = 1;
            }
            else
            {
                fsy = 0;
            }
            fsx = fsx + 2;
            if(lsx == 319 && lsy == 0)          
            {
                fsx  = 160;
                fsy  = 0;
            }
            if(lsx == 322 && lsy == 0)          
            {
                fsx  = 161;
                fsy  = 0;
            }
            if(lsx == 323 && lsy == 0)          
            {
                fsx  = 161;
                fsy  = 1;
            }                        
        }
        else if (lsy >= 370)
        {
            // iob in top
            fsx = (gsx / NUM_GRID_X) * 2 + 1;
            if (gsx % 4 == 0)
            {
                fsy = 90;
            }
            else
            {
                fsy = 91;
            }
           fsx = fsx + 2;
        }
        else if (lsx < 1)
        {
            // iob in left
           tem_x = gsy % 8;
           tem_y = gsy / NUM_GRID_Y + gsy % 2;
          
          if (tem_x < 3)
          {
             fsx = 1;
             fsy = tem_y;
          }
          else
          {
             fsx = 3;
             fsy = tem_y - 1;
          }          
        }
        else
        {
           // iob in right
           tem_x = gsy % 8;
           tem_y = gsy / NUM_GRID_Y + gsy % 2;
 
           if (tem_x < 3)
           {
              fsx = 1;
              fsy = tem_y;
           }
           else
           {
              fsx = 3;
              fsy = tem_y - 1;
           }
           if(lsy % 2)
          {
             fsy = fsy - 1;
          }
          else
           {
            fsy = fsy + 1;
          }            
           fsx = fsx + 162;
       }
}
function get_coordinate_of_bkcl(unsigned int lsx, lsy)
{
   adjust_floorplan_gap(lsx, lsy);
   fsx = gsx / 2  - 1;
   fsy = gsy / NUM_GRID_Y;       
   if (lsx < 100)
   {
      fsx = fsx + 5;
      fsy = fsy + 1;
   }
   else if (lsx >= 100 && lsx < 250)
   {
      if (lsy > 300)
      {
         fsy = fsy - 1;
      }
      else
      {
         fsy = fsy + 1;
         fsx = fsx + 3;         
      }

   }
   else
   {
      fsx = fsx + 2;
      fsy = fsy + 1;      
   }
}
function get_coordinate_of_rescal(unsigned int lsx, lsy)
{
   adjust_floorplan_gap(lsx, lsy);
   fsx = gsx / 2 - 1;
   fsy = gsy / NUM_GRID_Y + 2; 
   if (lsx < 100)
   {
      fsx = fsx + 2 + 3;
   }
   else if (lsx >= 100 && lsx < 250)
   {
      if (lsy > 300)
      {
         fsy = fsy - 4;
      }
      else
      {
         fsy = fsy;
         fsx = fsx - 4;
      }

   }
   else
   {
      
      fsx = fsx + 2;
   }
}
function get_coordinate_of_crystal(unsigned int lsx, lsy)
{
   adjust_floorplan_gap(lsx, lsy);
   
   fsx = gsx / 2;
   fsy = gsy / NUM_GRID_Y + 3;
   if (lsx < 164)
   {
      fsx = fsx + 4;
   }
   else
   {
      fsx = fsx + 1;
   }
}

function get_coordinate_of_rckb(unsigned int lsx, lsy)
{
   adjust_floorplan_gap(lsx, lsy);
   
    fsx = gsx / 2 + 1;
    fsy = gsy / NUM_GRID_Y + gsy % 4 - 8;
    if (lsx < 150)
    {
       fsx = fsx + 1;
    }

}

function get_coordinate_of_iockdly(unsigned int lsx, lsy)
{
   adjust_floorplan_gap(lsx, lsy);

   fsx = gsx / 2 - 1;
   fsy = gsy / NUM_GRID_Y;
   fsx = fsx + 2;
   if (lsx < 10)
   {
      fsx = fsx + 1;
      fsy = fsy + gsy % 4 + 4;
   }
   else if (10 <= lsx && lsx < 300)
   {
      fsx = fsx + 2;
      if (lsy < 100)
      {
         fsy = fsy + lsy % 4 + lsy % 2 + 6;
      }
      else
      {
         fsy = fsy + lsy % 8 - 10;
      }
   }
   else if (lsx >= 300)
   {
      fsy = fsy + gsy % 4 + 4;
   }
}

function get_coordinate_of_iockdiv(unsigned int lsx, lsy)
{
   adjust_floorplan_gap(lsx, lsy);

   fsx = gsx / 2  - 1;
   fsy = gsy / NUM_GRID_Y + gsy % 2;
   fsx = fsx + 2;
   if (lsx < 10)
   {
      fsx = fsx + 1;
   }
   else if (lsx >= 10 && lsx < 300)
   {
      fsx = fsx + gsx % 2 + 1;
      fsy = fsy + gsx % 2;
   }
   else
   {
      fsx = fsx;
      fsy = fsy + 1;
   }
}
function get_coordinate_of_iockgate(unsigned int lsx, lsy)
{
   adjust_floorplan_gap(lsx, lsy);
   
   fsx = gsx / 2 - 1;
   fsy = gsy / NUM_GRID_Y + gsy % 2;
   if (lsx < 10)
   {
      fsx = fsx + 1;
      fsy = fsy  - 3;
   }
   else if (lsx >= 10 && lsx < 300)
   {
      fsx = fsx + gsx % 2 + 1;
      fsy = fsy + gsx % 2 + 3 ;
   }
   else
   {
       fsy = fsy - 3;
   }
   fsx = fsx + 2;

}

