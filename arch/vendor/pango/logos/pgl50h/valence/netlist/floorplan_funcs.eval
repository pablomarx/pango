function get_coordinate_of_clm(unsigned int lsx, lsy)
{

   adjust_floorplan_gap(lsx, lsy);

    fsx = gsx / 2 + gsy % 2 - 1;
    fsy = gsy / NUM_GRID_Y;
    fsx = fsx + 2;
}
function get_coordinate_of_ldo(unsigned int lsx, lsy)
{

   adjust_floorplan_gap(lsx, lsy);

    fsx = gsx / 2 + 2;
    fsy = gsy / NUM_GRID_Y + lsy % 3;
    if(lsx == 326)
    {
      fsx = fsx + 3;    
    }
    else
    {
      fsx = fsx + 1;
    }
}
