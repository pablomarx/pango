/****************************************************************************************************
*****************************************************************************************************/
function connect_USCM_bl_input
(
    unsigned int region_index,
    unsigned int ty_uscm,
    unsigned int ty_int,
    unsigned int tx_uscm,
    unsigned int tx_blsr
)
{
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = region_index * NUM_GRID_BLSR_Y;

    int sx = tx_uscm * NUM_GRID_X;
    int sy = ty_uscm * NUM_GRID_Y;

    if(ty_int==0)
    {
      connect
        (
          <pin DATA of <device USCM_TILE @ [sx+2,sy]>>   => <wire DATA[143:0] of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>,
          <pin DATAN of <device USCM_TILE @ [sx+2,sy]>>  => <wire DATAN[143:0] of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>
        );
    }
    if(ty_int==13)
    {
      connect
        (
          <pin DATA of <device USCM_TILE @ [sx+2,sy]>>   => <wire DATA[1079:936] of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>,
          <pin DATAN of <device USCM_TILE @ [sx+2,sy]>>  => <wire DATAN[1079:936] of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>
        );
    }
    if(ty_int==16)
    {
      connect
        (
          <pin DATA of <device USCM_TILE @ [sx+2,sy]>>   => <wire DATA[1239:1096] of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>,
          <pin DATAN of <device USCM_TILE @ [sx+2,sy]>>  => <wire DATAN[1239:1096] of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>
        );
    }
    if(ty_int==29)
    {
      connect
        (
          <pin DATA of <device USCM_TILE @ [sx+2,sy]>>   => <wire DATA[2175:2032] of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>,
          <pin DATAN of <device USCM_TILE @ [sx+2,sy]>>  => <wire DATAN[2175:2032] of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>
        );
    }

}//end of function connect_USCM_bl_input
/****************************************************************************************************
*****************************************************************************************************/
function connect_IOL_bl_input
(
    unsigned int tx,
    unsigned int region_index, 
    unsigned int tx_blsr,
    unsigned int l_ty_iol[],
    unsigned int l_ty_hmemciol[],
    unsigned int l_ty_dqsl[],
    unsigned int l_ty_hmemcdqsl[],
    unsigned int l_ty_iockgate[],
    unsigned int l_ty_hmemciockgate[],
    unsigned int l_ty_cramsl2[],
    unsigned int l_ty_rlybufs5[]
)
{
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = region_index * NUM_GRID_BLSR_Y;

    int ty,tmp_ty,ty_all;
    int sx,sy;

    string device_iol;
    sx = tx * NUM_GRID_X;  
    foreach ty({[0:14],[16:30]})
    {
        sy =(ty>MID_TILE_REGION) ? region_index*((NUM_TILES_REGION-1)*NUM_GRID_Y+NUM_GRID_WLSR_Y)+(ty-1)*NUM_GRID_Y + NUM_GRID_WLSR_Y : region_index * ((NUM_TILES_REGION-1)*NUM_GRID_Y+NUM_GRID_WLSR_Y) + ty*NUM_GRID_Y;

        ty_all = ty + region_index * NUM_TILES_REGION;  
        foreach tmp_ty( l_ty_iol )
        {
          if(tmp_ty == ty_all)
          {
            device_iol = "IOL_TILE";
            break;
          }
        }
        foreach tmp_ty( l_ty_cramsl2 )
        {
          if(tmp_ty == ty_all)
          {
            device_iol = "CRAMSL2_TILE";
            break;
          }
        }
        foreach tmp_ty( l_ty_rlybufs5 )
