use pgl_defs;

package pgl_funcs_connect_blsr
{
/*******************************************************************
********************************************************************/
function connect_blsr_gs_output
(
    unsigned int rx,
    unsigned int tx,
    unsigned int tx_blsr,
    unsigned int tx_ccs,
    unsigned int ty_ccs,
    string device_wlsr
)
{
    int sx = ((device_wlsr=="IOCKGATEDLL_TILE")||(device_wlsr=="HMEMCIOCKGATEDLL_TILE")) ? tx * NUM_GRID_X + 2 : tx * NUM_GRID_X;
    int sy = ((device_wlsr=="IOCKGATEDLL_TILE")||(device_wlsr=="HMEMCIOCKGATEDLL_TILE")) ? (rx*NUM_TILES_REGION+MID_TILE_REGION-1)*NUM_GRID_Y
                                                                                                                     : (rx*NUM_TILES_REGION+MID_TILE_REGION)*NUM_GRID_Y;
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = rx * NUM_GRID_BLSR_Y;
    int sx_ccs = tx_ccs * NUM_GRID_X + 2;
    int sy_ccs = ty_ccs * NUM_GRID_Y;
    string wlsr = device_wlsr;
    int sx_iob = tx * NUM_GRID_X;
    int sy_iob = rx * NUM_TILES_REGION * NUM_GRID_Y;

    if(device_wlsr == "IOB_TILE" || device_wlsr == "IOB_WO_ODIFF_TILE")
    {
      connect
        (
          <pin BUS_WLSRB_IN of <device *wlsr @[sx_iob,sy_iob]>>  => (tx>tx_blsr) ? <wire BUS_WLSR_OUTR of <device BLSR_TILE @[sx_blsr,sy_blsr]>>
                                                                                 : <wire BUS_WLSR_OUTL of <device BLSR_TILE @[sx_blsr,sy_blsr]>>,

          <pin WLSHFT_REF of <device *wlsr @[sx_iob,sy_iob]>>   => (tx>tx_blsr) ? <wire WLSR_REF[1] of <device BLSR_TILE @[sx_blsr,sy_blsr]>>
                                                                                : <wire WLSR_REF[0] of <device BLSR_TILE @[sx_blsr,sy_blsr]>>
        );
            
    }
    else
    {
      connect
        (
          <pin BUS_WLSR_IN of <device *wlsr @[sx,sy]>>        => (tx>tx_blsr) ? <wire BUS_WLSR_OUTR of <device BLSR_TILE @[sx_blsr,sy_blsr]>>
                                                                              : <wire BUS_WLSR_OUTL of <device BLSR_TILE @[sx_blsr,sy_blsr]>>,

          <pin WLSHFT_REF of <device *wlsr @[sx,sy]>>          => (tx>tx_blsr) ? <wire WLSR_REF[1] of <device BLSR_TILE @[sx_blsr,sy_blsr]>>
                                                                               : <wire WLSR_REF[0] of <device BLSR_TILE @[sx_blsr,sy_blsr]>>
      );
    }

    if(device_wlsr=="WLSRD_TILE")
    {
      connect
        (
          <pin MCE of <device *wlsr @[sx,sy]>>   => (tx>tx_blsr) ? <wire MCE_OR of <device BLSR_TILE @[sx_blsr,sy_blsr]>>
                                                                 : <wire MCE_OL of <device BLSR_TILE @[sx_blsr,sy_blsr]>>,
          <pin MCLKB of <device *wlsr @[sx,sy]>> => (tx>tx_blsr) ? <wire MCLKB_OR of <device BLSR_TILE @[sx_blsr,sy_blsr]>>
                                                                 : <wire MCLKB_OL of <device BLSR_TILE @[sx_blsr,sy_blsr]>>,
          <pin MWR of <device *wlsr @[sx,sy]>>   => (tx>tx_blsr) ? <wire MWR_OR of <device BLSR_TILE @[sx_blsr,sy_blsr]>>
                                                                 : <wire MWR_OL of <device BLSR_TILE @[sx_blsr,sy_blsr]>>
        );
    }
    
    if(device_wlsr!="IOB_TILE" && device_wlsr != "IOB_WO_ODIFF_TILE")
    {
      connect
        (
          <pin VCTRL of <device *wlsr @[sx,sy]>>              => <wire NGB of <device CCS_TILE @[sx_ccs,sy_ccs]>>
//          <pin WLSHFT_POR_ERASE_N of <device *wlsr @[sx,sy]>> => <wire POR_N_OUT of <device *wlsr @[sx,sy]>>
        );
    }
    
    
}//end of function connect_blsr_gs_output

/*******************************************************************
********************************************************************/
function connect_blsr_gs_output1 
(
    unsigned int rx,
    unsigned int tx_blsr,
    unsigned int tx_wlsr1_l,
    unsigned int tx_wlsr1_r,
    unsigned int tx_dll_l,
    unsigned int tx_dll_r,
    unsigned int flag
)
{
    int sy_blsr = rx * NUM_GRID_BLSR_Y;
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_wlsr = rx * ((NUM_TILES_REGION-1)*NUM_GRID_Y+NUM_GRID_WLSR_Y) + MID_TILE_REGION*NUM_GRID_Y;
    int sx_wlsr1_l = tx_wlsr1_l * NUM_GRID_X;
    int sx_wlsr1_r = tx_wlsr1_r * NUM_GRID_X;
    int sx_dll_l = tx_dll_l * NUM_GRID_X + 2;
    int sx_dll_r = tx_dll_r * NUM_GRID_X + 2;

    string device_dll_l =  "IOCKGATEDLL_TILE";
    string device_dll_r =  "IOCKGATEDLL_TILE";


    connect
      (
        <pin BUS_BSCAN_IN of <device WLSR1_TILE @ [sx_wlsr1_l,sy_wlsr]>>    => <wire BUS_BSCAN_OUTL of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>,
        <pin BUS_BSCAN_IN of <device WLSR1_TILE @ [sx_wlsr1_r,sy_wlsr]>>    => <wire BUS_BSCAN_OUTR of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>,

        <pin CLK_ADC_IN of <device WLSR1_TILE @ [sx_wlsr1_l,sy_wlsr]>>      => <wire CLK_ADC_L of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>,
        <pin CLK_ADC_IN of <device WLSR1_TILE @ [sx_wlsr1_r,sy_wlsr]>>      => <wire CLK_ADC_R of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>,

        <pin BUS_BSCAN_IN of <device *device_dll_l @ [sx_dll_l,sy_wlsr-NUM_GRID_Y]>>   => <wire BUS_BSCAN_OUT of <device WLSR1_TILE @ [sx_wlsr1_l,sy_wlsr]>>,
        <pin BUS_BSCAN_IN of <device *device_dll_r @ [sx_dll_r,sy_wlsr-NUM_GRID_Y]>>   => <wire BUS_BSCAN_OUT of <device WLSR1_TILE @ [sx_wlsr1_r,sy_wlsr]>>
      );


}//end of function connect_blsr_gs_output
/****************************************************************************************************
*****************************************************************************************************/
function connect_BLSR_input_pgl
(
   unsigned int region_index,
   unsigned int tx_wlsr1_l,
   unsigned int tx_wlsr1_r,
   unsigned int tx_blsr,
   unsigned int tx_ccs,
   unsigned int ty_ccs,
   unsigned int tx_wlsr2,
   unsigned int num_region,
   unsigned int num_tx,
   unsign)
{
  int sx = tx_blsr * NUM_GRID_X;
  int sy = region_index * NUM_GRID_BLSR_Y;

  int sy_wlsr = region_index * ((NUM_TILES_REGION - 1) * NUM_GRID_Y + NUM_GRID_DLL_Y) + MID_TILE_REGION * NUM_GRID_Y;
  int sx_bkcl_l = 0;
  int sx_bkcl_r = (num_tx-1)*NUM_GRID_X;

  int sx_wlsr1_l = tx_wlsr1_l * NUM_GRID_X;
  int sx_wlsr1_r = tx_wlsr1_r * NUM_GRID_X;
  int sy_iob = region_index * ((NUM_TILES_REGION - 1) * NUM_GRID_Y + NUM_GRID_DLL_Y);



  connect
    (
      <pin COL_INC_I of <device BLSR_TILE @ [sx,sy]>>         => <wire COL_INC_OUT of <device IOB_TILE @ [sx_bkcl_r,sy_iob]>>,

      <pin NONDRM_REG_INC_I of <device BLSR_TILE @ [sx,sy]>>  => <wire REGION_INC of <device IOB_TILE @ [sx_bkcl_r,sy_iob]>>,
      <pin CAL_DONE_L_IN of <device BLSR_TILE @ [sx,sy]>>     => <wire CAL_DONE_OUT of <device WLSR1_TILE @ [sx_wlsr1_l,sy_wlsr]>>,
      <pin CAL_DONE_R_IN of <device BLSR_TILE @ [sx,sy]>>     => <wire CAL_DONE_OUT of <device WLSR1_TILE @ [sx_wlsr1_r,sy_wlsr]>>
    );

  int sx_ccs = tx_ccs * NUM_GRID_X + 2;
  int sy_ccs = ty_ccs * NUM_GRID_Y;

  int ty;
  for(ty=ty_ccs;ty>MID_TILE_REGION;ty=ty-NUM_TILES_REGION)
  {
    sy_ccs = sy_ccs + NUM_GRID_WLSR_Y - NUM_GRID_Y;
  }

  if(region_index==(num_region-1))
  {
    connect
      (
        <pin DATA_IN of <device BLSR_TILE @ [sx,sy]>>       => <wire DATA_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin CLK_IN of <device BLSR_TILE @ [sx,sy]>>        => <wire BLSRCLK_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin ERASE_EN of <device BLSR_TILE @ [sx,sy]>>      => <wire ERASE_EN_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin POR_ERASE_N of <device BLSR_TILE @ [sx,sy]>>   => <wire POR_N_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin PRECHG of <device BLSR_TILE @ [sx,sy]>>        => <wire PRECHG_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin PROG_CAP of <device BLSR_TILE @ [sx,sy]>>      => <wire PROG_CAP_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin PROG_ON of <device BLSR_TILE @ [sx,sy]>>       => <wire PROG_ON_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin READ of <device BLSR_TILE @ [sx,sy]>>          => <wire READ_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin RDBK_CAP of <device BLSR_TILE @ [sx,sy]>>      => <wire RDBK_CAP_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin WL_ON_I of <device BLSR_TILE @ [sx,sy]>>       => <wire WL_ON_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin PD_IN of <device BLSR_TILE @ [sx,sy]>>         => <wire PD_OUT of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin PUB_IN of <device BLSR_TILE @ [sx,sy]>>        => <wire PUB_OUT of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin WLSR_SEL_I of <device BLSR_TILE @ [sx,sy]>>    => <wire WLSHFT_SEL_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin RSTN of <device BLSR_TILE @ [sx,sy]>>          => <wire RST_BLSR_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin BLSR_SEL of <device BLSR_TILE @ [sx,sy]>>      => <wire BLSHFT_SEL_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin REGION_RW_EN of <device BLSR_TILE @ [sx,sy]>>  => <wire REGION_RW_EN_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin COL_IN_I of <device BLSR_TILE @ [sx,sy]>>      => <wire COL_IN_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin TYPE_I of <device BLSR_TILE @ [sx,sy]>>        => <wire TYPE_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin FRAME_ADDR_I of <device BLSR_TILE @ [sx,sy]>>  => <wire FRAME_ADDR_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,

        <pin MCLKB_I of <device BLSR_TILE @ [sx,sy]>>       => <wire MCLKB_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin MCE_I of <device BLSR_TILE @ [sx,sy]>>         => <wire MCE_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin MWR_I of <device BLSR_TILE @ [sx,sy]>>         => <wire MWR_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,

        <pin BUS_BSCAN_IN of <device BLSR_TILE @ [sx,sy]>>  => <wire BUS_BSCAN_OUT of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin CLK_REL_IN of <device BLSR_TILE @ [sx,sy]>>    => <wire RELCK of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin GLOGEN_IN of <device BLSR_TILE @ [sx,sy]>>     => <wire GLOGEN of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin GRS_N_IN of <device BLSR_TILE @ [sx,sy]>>      => <wire GRS_N of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin GWEN_IN of <device BLSR_TILE @ [sx,sy]>>       => <wire GWEN of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin POR_N_IN of <device BLSR_TILE @ [sx,sy]>>      => <wire POR_N of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin PUP_REL_N_IN of <device BLSR_TILE @ [sx,sy]>>  => <wire PUP_SEL_N of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,

        <pin CLK_ADC_IN0 of <device BLSR_TILE @ [sx,sy]>>   => <wire CLK_ADC of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin CLK_ADC_IN1 of <device BLSR_TILE @ [sx,sy]>>   => <wire CLK_ADC of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin CLK_ADC_IN2 of <device BLSR_TILE @ [sx,sy]>>   => <wire CLK_ADC of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin CLK_USER_IN of <device BLSR_TILE @ [sx,sy]>>   => <wire CLK_USER of <device CCS_TILE @ [sx_ccs,sy_ccs]>>

      );
  }
  else
  {
     connect
      (
        <pin DATA_IN of <device BLSR_TILE @ [sx,sy]>>       => <wire DATA_TO_NEXT of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin CLK_IN of <device BLSR_TILE @ [sx,sy]>>        => <wire CLKOUT_M of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin ERASE_EN of <device BLSR_TILE @ [sx,sy]>>      => <wire ERASE_EN_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin POR_ERASE_N of <device BLSR_TILE @ [sx,sy]>>   => <wire POR_ERASE_N_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin PRECHG of <device BLSR_TILE @ [sx,sy]>>        => <wire PRECHG_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin PROG_CAP of <device BLSR_TILE @ [sx,sy]>>      => <wire PROG_CAP_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin PROG_ON of <device BLSR_TILE @ [sx,sy]>>       => <wire PROG_ON_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin READ of <device BLSR_TILE @ [sx,sy]>>          => <wire READ_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin RDBK_CAP of <device BLSR_TILE @ [sx,sy]>>      => <wire RDBK_CAP_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin WL_ON_I of <device BLSR_TILE @ [sx,sy]>>       => <wire WL_ON_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin PD_IN of <device BLSR_TILE @ [sx,sy]>>         => <wire PD_OUT of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin PUB_IN of <device BLSR_TILE @ [sx,sy]>>        => <wire PUB_OUT of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin WLSR_SEL_I of <device BLSR_TILE @ [sx,sy]>>    => <wire WLSR_SEL_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin RSTN of <device BLSR_TILE @ [sx,sy]>>          => <wire RSTN_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin BLSR_SEL of <device BLSR_TILE @ [sx,sy]>>      => <wire BLSR_SEL_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin REGION_RW_EN of <device BLSR_TILE @ [sx,sy]>>  => <wire REGION_RW_EN_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin COL_IN_I of <device BLSR_TILE @ [sx,sy]>>      => <wire COL_IN_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin TYPE_I of <device BLSR_TILE @ [sx,sy]>>        => <wire TYPE_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin FRAME_ADDR_I of <device BLSR_TILE @ [sx,sy]>>  => <wire FRAME_ADDR_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,

        <pin MCLKB_I of <device BLSR_TILE @ [sx,sy]>>       => <wire MCLKB_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin MCE_I of <device BLSR_TILE @ [sx,sy]>>         => <wire MCE_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin MWR_I of <device BLSR_TILE @ [sx,sy]>>         => <wire MWR_O of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,

        <pin BUS_BSCAN_IN of <device BLSR_TILE @ [sx,sy]>>    => <wire BUS_BSCAN_OUTM of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin CLK_REL_IN of <device BLSR_TILE @ [sx,sy]>>      => <wire CLK_REL_OUT_M of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin GLOGEN_IN of <device BLSR_TILE @ [sx,sy]>>       => <wire GLOGEN_OUT of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin GRS_N_IN of <device BLSR_TILE @ [sx,sy]>>        => <wire GRS_N_OUT_M of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin GWEN_IN of <device BLSR_TILE @ [sx,sy]>>         => <wire GWEN_OUT_M of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin POR_N_IN of <device BLSR_TILE @ [sx,sy]>>        => <wire POR_N_OUT_M of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin PUP_REL_N_IN of <device BLSR_TILE @ [sx,sy]>>    => <wire PUP_REL_N_OUT_M of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,

        <pin CLK_ADC_IN0 of <device BLSR_TILE @ [sx,sy]>>     => <wire CLK_ADC_OUT of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin CLK_ADC_IN1 of <device BLSR_TILE @ [sx,sy]>>     => <wire CLK_ADC_OUT of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin CLK_ADC_IN2 of <device BLSR_TILE @ [sx,sy]>>     => <wire CLK_ADC_OUT of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>,
        <pin CLK_USER_IN of <device BLSR_TILE @ [sx,sy]>>     => <wire CLK_USER_OUT of <device BLSR_TILE @ [sx,sy+NUM_GRID_BLSR_Y]>>
      );
  }

  unsigned int sx_wlsr = tx_wlsr2 * NUM_GRID_Y;

  if(region_index==0)
  {
    connect
      (
        <pin DSOUT_PRE[31] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[30] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[29] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[28] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[27] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[26] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[25] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[24] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[23] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[22] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[21] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[20] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[19] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[18] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[17] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[16] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[15] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[14] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[13] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[12] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[11] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[10] of <device BLSR_TILE @ [sx,sy]>>          => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[9] of <device BLSR_TILE @ [sx,sy]>>           => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
        <pin DSOUT_PRE[8] of <device BLSR_TILE @ [sx,sy]>>           => <wire TIEL1 of <device BLSR_TILE @ [sx,sy]>>,
