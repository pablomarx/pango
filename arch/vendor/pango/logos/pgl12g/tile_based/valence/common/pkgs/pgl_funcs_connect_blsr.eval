*****************************************************************************************************/
function connect_WLSR_bl_input
(
    unsigned int region_index, 
    unsigned int tx,
    unsigned int tx_blsr,
    string wlsr
)
{
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = region_index * NUM_GRID_BLSR_Y;

    int sy = region_index*NUM_TILES_REGION*NUM_GRID_Y+MID_TILE_REGION*NUM_GRID_Y;

    int sx;
    sx = tx * NUM_GRID_X;

   string device_wlsr = wlsr;

    connect
      (
        <pin DATA of <device *device_wlsr @[sx,sy]>>  => <wire DATA[1095:1080] of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>,
        <pin DATAN of <device *device_wlsr @[sx,sy]>> => <wire DATAN[1095:1080] of <device BLSR_TILE @ [sx_blsr,sy_blsr]>>
      );


}//end of function connect_WLSR_bl_input


