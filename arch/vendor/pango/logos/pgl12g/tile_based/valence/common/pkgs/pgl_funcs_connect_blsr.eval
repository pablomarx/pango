/****************************************************************************************************
*****************************************************************************************************/
function connect_IOB_bl_input
(
    unsigned int rx, 
    unsigned int tx,
    unsigned int tx_blsr
)
{
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = rx * NUM_GRID_BLSR_Y;

    int ty,tmp_ty;
    int sx,sy;

    string device_iob;

    sx = tx * NUM_GRID_X; 
    sy =  sy_blsr;

    connect
      (
        <pin DATA of <device IOB_TILE @[sx,sy]>>  => <wire DATA of <device BLSR_TILE @[sx_blsr,sy_blsr]>>,
        <pin DATAN of <device IOB_TILE @[sx,sy]>> => <wire DATAN of <device BLSR_TILE @[sx_blsr,sy_blsr]>>
      );


}//end of function connect_IOB_bl_input

