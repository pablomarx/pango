        input TS = 1'b1,
        input DO = 1'b1,
        input DIFFI_IN = 1'b1,
        input DIFFO_IN = 1'b1,
        input MIPI_SW_DYN = 1'b1,
        output DIN,
        output DIFFI_OUT,
        inout PAD
    );
};
//grid device end

//grid device structure netlist started
structure netlist of IOBS
{
    //wire for input ports
    wire ntTS;
    wire ntDO;
    wire ntDIFFI_IN;
    wire ntDIFFO_IN;
    wire ntMIPI_SW_DYN;
    
    //wire for output ports    
    wire ntDIN;
    wire ntPAD;

    //internal wire
    wire ntOBUFO_IN;
    wire ntMIPIMUX;

    // Connection to ports 
    ntTS          <= TS;     
    ntDO          <= DO;     
    ntDIFFI_IN    <= DIFFI_IN;
    ntDIFFO_IN    <= DIFFO_IN;
    ntMIPI_SW_DYN <= MIPI_SW_DYN;

    DIFFI_OUT  <=  ntPAD;
    DIN        <=  ntDIN;     
    PAD        <=  ntPAD;    

    device IBUFS IBUFS
        parameter map
        (
            CP_IOB_MODE    => CP_IOB_MODE,
            CP_IO_STANDARD => CP_IO_STANDARD
        )
        port map
        (
            OUT         => ntDIN,
            IN          => ntPAD,
            DIFFI_IN    => ntDIFFI_IN,
            MIPI_SW_DYN => ntMIPI_SW_DYN
        );

    device OBUFS OBUFS
        parameter map
        (
            CP_IOB_MODE    => CP_IOB_MODE,
            CP_OPENDRAIN   => CP_OPENDRAIN,
            CP_IO_STANDARD => CP_IO_STANDARD
        )
        port map
        (
            OUT => ntOBUFO_IN,  
            DO  => ntDO,
            TS  => ntTS,
            MIPI_SW_DYN => ntMIPI_SW_DYN
        ); 

    device MIPIMUX MIPIMUX
        parameter map
        (
            CP_IO_STANDARD => CP_IO_STANDARD
        )
        port map
        (
            X0 => ntMIPI_SW_DYN,
            X1 => 1'b0,
            Y  => ntMIPIMUX
        );
        
    device DIFFOSMUX DIFFOSMUX
        parameter map
        (
            CP_IOB_MODE    => CP_IOB_MODE
        )
        port map
        (
            OUT      => ntPAD,
            DIFFO_IN => ntDIFFO_IN,
            OBUFO_IN => ntOBUFO_IN,
            MIPI_SW_DYN => ntMIPIMUX
        );    
         
}; // end of struct netlist of IOBS
//grid device structure netlist end
//grid device configure body started

