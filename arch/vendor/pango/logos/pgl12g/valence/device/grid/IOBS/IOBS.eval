tarted
grid
device IOBS
{
    // grid_property_def_start
    property
    (
        //DONT_USE means this iob unbond, and EMPTY means the layout dont have this iob ;NAME will assign PAD name later
        bool   DONT_USE = false,
        bool   EMPTY = false,
        string PAD_NAME = "",
        string PIN_NAME = "",
        string LOCATION = "",
        string HARDWARE_NAME = "",
        string IO_TYPE  = "",
        bool   SHARED_IO = false,
        bool   CLK_PAD   = false,
        string PLL_PAD   = ""
    );
    // grid_property_def_end

    parameter
    (
        config string CP_IOB_MODE = "UNUSED",
        config string CP_PORT_STATUS = "PULLUP",
        config string CP_IO_STANDARD = "DEFAULT",
        config string CP_DIFFIN_TERM = "OFF",
        config string CP_DDRIN_TERM = "OFF",
        config string CP_W_VOLT_R_IN = "NOHYS",
        config string CP_DRV_STRENGTH = "8",
        config string CP_SLEW_MODE = "F",
        config string CP_HOT_SOCKET = "ON",
        config string CP_OPENDRAIN = "OFF",
        config string CP_VCCIO = "2.5"
    );
    port
    (
// configuration_body_def_on


    config input SC_PU = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_PU" */,

    config input SC_PD_N = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_PD_N" */,

    config input SC_CLAMP = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_CLAMP" */,

    config input SC_INS_N = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_INS_N" */,

    config input SC_IND = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_IND" */,

    config input SC_INSD = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_INSD" */,

    config input SC_TS_FORCE[1:0] = 2'h0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_TS_FORCE[1],SC_TS_FORCE[0]" */,

    config input SC_INS_BS2 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_INS_BS2" */,

    config input SC_INS_BS1 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_INS_BS1" */,

    config input SC_RESIND = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_RESIND" */,

    config input SC_TERM90 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_TERM90" */,

    config input SC_SLEW_N = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SLEW_N" */,

    config input SC_3325_N = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_3325_N" */,

    config input SC_PDG1 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_PDG1" */,

    config input SC_PDG2_N = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_PDG2_N" */,

    config input SC_PDG3 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_PDG3" */,

