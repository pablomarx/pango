timing iobs_tnl of IOBS
{
    operator V_BUF V_BUF
        port map
        (
            I         => PAD,
            Z         => DIFFI_OUT
        );

////////////////////////////////////////////////////////////
////////////////////////IBUFS///////////////////////////////
////////////////////////////////////////////////////////////
    wire ntDI;
    if (CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI")
    {
         ntDI <= DIFFI_IN;
    }
    else if (CP_IOB_MODE == "IBUFHR" || CP_IOB_MODE == "IOBUFHR")
    {
        operator V_IBUF ibuf
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I => PAD,
                O => ntDI
            );
    }
    else if (CP_IOB_MODE == "IBUFHR_DS" || CP_IOB_MODE == "IOBUFHR_DS")
    {
        operator V_IBUFDS ibufds
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I  => PAD,
                IB => DIFFI_IN,
                O  => ntDI
            );
    }

    DIN <= //(CP_IOB_MODE == "IBUFHR" || CP_IOB_MODE == "IOBUFHR" || CP_IOB_MODE == "IBUFHR_DS" || CP_IOB_MODE == "IOBUFHR_DS") ? ntDI : 1'b1;
           ((CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI") && (CP_IOB_MODE == "IBUFHR" || CP_IOB_MODE == "IOBUFHR")) ? ntDI :
            (CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI") ? 1'b1 :
            (CP_IOB_MODE == "IBUFHR" || CP_IOB_MODE == "IOBUFHR" || CP_IOB_MODE == "IBUFHR_DS" || CP_IOB_MODE == "IOBUFHR_DS") ? ntDI : 1'b1;

////////////////////////////////////////////////////////////
////////////////////////OBUFS AND OBUFSMUX//////////////////
////////////////////////////////////////////////////////////
    if (CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI")
    {
        if (CP_IOB_MODE == "OBUFHR" || CP_IOB_MODE == "OBUFHR_T" || CP_IOB_MODE == "IOBUFHR")
        {
            operator V_OUTBUFT_MIPI_SLAVE V_OUTBUFT_MIPI_SLAVE
                parameter map
                (
                    IOSTANDARD => "SLVS"
                )
                port map
                (
                    IB => DO,
                    I  => DIFFO_IN,
                    TB => TS,
                    M  => MIPI_SW_DYN,
                    OB => PAD
                );
        }
    }
    else if (CP_IOB_MODE == "OBUFHR")
    {
        operator V_OBUF obuf
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I => DO,
                O => PAD
            );
    }
    else if (CP_IOB_MODE == "OBUFHR_T" || CP_IOB_MODE == "IOBUFHR" || CP_IOB_MODE == "IOBUFHR_DS")
    {
        operator V_OBUFT obuft
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I => DO,
                T => TS,
                O => PAD
            );
    }
    else if (CP_IOB_MODE == "UNUSED")
    {
        PAD <= DIFFO_IN;
    }
};
