                    I  => PAD,
                    IB => DIFFI_IN,
                    M  => MIPI_SW_DYN,
                    O  => ntDI
                );

            operator V_INBUF_MIPI ibfmipi_lp
                parameter map
                (
                    IOSTANDARD => "SLVS",
                    MIPI_MODE  => "LP_MIPI"  // RX O_LP
                )
                port map
                (
                    I  => PAD,
                    IB => DIFFI_IN,
                    TS => TS,
                    M  => MIPI_SW_DYN,
                    O  => S11_DIN_MIPI_LP
                );
        }
        else if (CP_IOB_MODE == "OBUFHS_IOLP")
        {
            operator V_INBUF_MIPI ibfmipi_lp
                parameter map
                (
                    IOSTANDARD => "SLVS",
                    MIPI_MODE  => "SLAVE_LP"  // TX O_LP
                )
                port map
                (
                    I  => PAD,
                    IB => DIFFI_IN,
                    TS => TS,
                    M  => MIPI_SW_DYN,
                    O  => S11_DIN_MIPI_LP
                );
        }
    }
    else if (CP_IOB_MODE == "IBUFHR" || CP_IOB_MODE == "IOBUFHR")
    {
        operator V_IBUF ibuf
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I => PAD,
                O => ntDI
            );
    }
    else if (CP_IOB_MODE == "IBUFHR_DS" || CP_IOB_MODE == "IOBUFHR_DS")
    {
        operator V_IBUFDS ibufds
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I  => PAD,
                IB => DIFFI_IN,
                O  => ntDI
            );
    }

    DIN <= ((CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI") && (CP_IOB_MODE == "IBUFHR_DS" || CP_IOB_MODE == "IBUFHS_IOLP")) ? ntDI :
            (CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI") ? 1'b1 :
            (CP_IOB_MODE == "IBUFHR" || CP_IOB_MODE == "IOBUFHR" || CP_IOB_MODE == "IBUFHR_DS" || CP_IOB_MODE == "IOBUFHR_DS") ? ntDI : 1'b1;

