        input DO = 1'b1,
        input DIFFI_IN = 1'b1,
        input MIPI_SW_DYN = 1'b1,
        output DIFFO_OUT,
        output DIFFI_OUT,
        output DIN,
        output S11_DIN_MIPI_LP,
        inout PAD
    );
