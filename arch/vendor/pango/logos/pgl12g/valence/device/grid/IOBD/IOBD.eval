{
    if (CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI")
    {
        if (CP_IOB_MODE == "IBUFHS_IOLP" || CP_IOB_MODE == "IBUFHR_DS")
        {
            wire net_DIFFI_OUT;
            operator V_INBUF_MIPI ibfmipi_lp_ob
                parameter map
                (
                    IOSTANDARD => "SLVS",
                    MIPI_MODE  => "LP_MIPI"  // RX OB_LP
                )
                port map
                (
                    IB => PAD,
                    I  => DIFFI_IN,
                    TS => TS,
                    M  => MIPI_SW_DYN,
                    O  => net_DIFFI_OUT
                );

            operator V_MUX2 ibfmipi_lp_ob_mux
                port map
                (
                    I0  => DIFFI_IN,
                    I1  => net_DIFFI_OUT,
                    SEL => TS,
                    Z   => DIFFI_OUT
                );
        }
        else if (CP_IOB_MODE == "OBUFHS_IOLP")
        {
            operator V_INBUF_MIPI ibfmipi_lp_ob
                parameter map
                (
                    IOSTANDARD => "SLVS",
                    MIPI_MODE  => "SLAVE_LP"  // TX OB_LP
                )
                port map
                (
                    IB => PAD,
                    I  => DIFFI_IN,
                    TS => TS,
                    M  => MIPI_SW_DYN,
                    O  => DIFFI_OUT
                );
        }
    }
    else
    {
        operator V_BUF V_BUF
            port map
            (
                I         => PAD,
                Z         => DIFFI_OUT
            );
    }

