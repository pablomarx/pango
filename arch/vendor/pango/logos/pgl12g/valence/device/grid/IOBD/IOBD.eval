//grid device end

//grid device structure netlist started
structure netlist of IOBD
{
    //wire for input ports
    wire ntTS;
    wire ntDO;
    wire ntDIFFI_IN;
    wire ntMIPI_SW_DYN;
    
    //wire for output ports
    wire ntDIFFO_OUT;
    wire ntDIN;
    wire ntPAD;
    wire ntS11_DIN_MIPI_LP;
    
    //Internal wires
    
    
    // Connection to ports
    ntTS          <= TS;      
    ntDO          <= DO;      
    ntDIFFI_IN    <= DIFFI_IN;
    ntMIPI_SW_DYN <= MIPI_SW_DYN;
   
    DIFFI_OUT <= ntPAD;
    DIFFO_OUT <= ntDIFFO_OUT;
    DIN       <= ntDIN;
    PAD       <= ntPAD;
    S11_DIN_MIPI_LP <= ntS11_DIN_MIPI_LP;
    
    device IBUFD IBUFD
        parameter map
        (
            CP_IOB_MODE    => CP_IOB_MODE,
            CP_IO_STANDARD => CP_IO_STANDARD
        )
        port map
        (
            OUT         => ntDIN,
            IN          => ntPAD,
            MIPI_SW_DYN => ntMIPI_SW_DYN,
            DIFFI_IN    => ntDIFFI_IN,
            DIN_MIPI_LP => ntS11_DIN_MIPI_LP
        );
        
    device OBUFD OBUFD
        parameter map
        (
            CP_IOB_MODE    => CP_IOB_MODE,
            CP_OPENDRAIN   => CP_OPENDRAIN,
            CP_IO_STANDARD => CP_IO_STANDARD
        )
        port map
        (
            OUT         => ntPAD,  
            DIFFO_OUT   => ntDIFFO_OUT,
            MIPI_SW_DYN => ntMIPI_SW_DYN,
            DO          => ntDO,
            TS          => ntTS
        );
};// end of structure netlist of IOBD
//grid device structure netlist end
