symbol logsym of DRM_INST // pragma PAP_ARC_COLOR="191:191:191"
{
    // The bounding box
    generate ( 10 # 10 );

    //
    // Poloygon declaration
    //
    shape
    [ 0, 0 ]  ->  [  0, 10 ]   < C @[,4] >
              ->  [ 10, 10 ]   
              ->  [ 10,  0 ]   
              ->  [  0,  0 ]   
              ;

}; // end of symbol logsym of DRM_INST


