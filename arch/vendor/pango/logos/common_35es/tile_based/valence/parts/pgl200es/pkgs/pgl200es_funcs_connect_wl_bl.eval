        <pin IREF10UA of <device *blsr_u @ [sx,4*NUM_GRID_BLSR_Y]>>     => <wire ICONST10U[21] of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin IREF10UA of <device *blsr @ [sx,3*NUM_GRID_BLSR_Y]>>     => <wire ICONST10U[22] of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin IREF10UA of <device *blsr @ [sx,2*NUM_GRID_BLSR_Y]>>     => <wire ICONST10U[23] of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin IREF10UA of <device *blsr @ [sx,1*NUM_GRID_BLSR_Y]>>     => <wire ICONST10U[24] of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
        <pin IREF10UA of <device *blsr @ [sx,0*NUM_GRID_BLSR_Y]>>     => <wire ICONST10U[25] of <device CCS_TILE @ [sx_ccs,sy_ccs]>>

      );
}//end of function connect_BLSR_input
/****************************************************************************************************
*****************************************************************************************************/
function connect_hsst_bl_input
(
    unsigned int tx,          
    unsigned int ty,          
    unsigned int tx_blsr,          
    unsigned int rx,
    string sig_data,
    string sig_datan,
    string dev_blsr,
    string dev_blsr_u
)
{
    string blsr = dev_blsr ;
    string blsr_u = dev_blsr_u ;
    string data = sig_data ;
    string datan = sig_datan ;
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = rx * NUM_GRID_BLSR_Y;

    int sx = tx * NUM_GRID_X;
    int sy = ty * NUM_GRID_Y;

    connect
    (
        <pin DATA[3255:1080]  of <device HSST_TILE @ [sx,sy]>>  => <wire *data[2175:0]     of <device *blsr_u @ [sx_blsr,sy_blsr + NUM_TILES_REGION*NUM_GRID_Y]>>,
        <pin DATAN[3255:1080]  of <device HSST_TILE @ [sx,sy]>>  => <wire *datan[2175:0]     of <device *blsr_u @ [sx_blsr,sy_blsr + NUM_TILES_REGION*NUM_GRID_Y]>>,
        <pin DATA[1079:0]  of <device HSST_TILE @ [sx,sy]>>  => <wire *data[2175:1096]     of <device *blsr @ [sx_blsr,sy_blsr]>>,
        <pin DATAN[1079:0]  of <device HSST_TILE @ [sx,sy]>>  => <wire *datan[2175:1096]     of <device *blsr @ [sx_blsr,sy_blsr]>>
    );

}//end of connect_hsst_bl_input
/****************************************************************************************************
*****************************************************************************************************/
function connect_hsst_wl_input
(
    unsigned int tx,          
    unsigned int ty,          
    unsigned int tx_blsr,          
    unsigned int ty_blsr,
    string dev_blsr
)
{
    string blsr = dev_blsr ;
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = ty_blsr * NUM_GRID_Y;
    int sy_wlsr = MID_TILE_REGION * NUM_GRID_Y + 3 * NUM_TILES_REGION * NUM_GRID_Y;
    int sx = tx * NUM_GRID_X;
    int sy = ty * NUM_GRID_Y;
    int sy_iob = 3 * NUM_TILES_REGION * NUM_GRID_Y;

    int Snum_region = NUM_TILES_REGION * NUM_GRID_Y ;

    connect
    (
          <pin COL_SEL[0]  of <device HSST_TILE @ [sx,sy]>>   => <wire COL_SEL_CCS  of <device IOB_TILE_HALF @ [0 * NUM_GRID_X,sy_iob]>>,
          <pin COL_SEL[1]  of <device HSST_TILE @ [sx,sy]>>   => <wire COL_SEL_CCS  of <device IOCKGATEDLL2_TILE @ [ 1 * NUM_GRID_X + 2,sy_wlsr - NUM_GRID_Y]>>,
          <pin COL_SEL[2]  of <device HSST_TILE @ [sx,sy]>>   => <wire COL_SEL_CCS  of <device WLSR_TILE @ [2 * NUM_GRID_X,sy_wlsr]>>,
          <pin COL_SEL[3]  of <device HSST_TILE @ [sx,sy]>>   => <wire COL_SEL_CCS  of <device WLSR_TILE @ [3 * NUM_GRID_X,sy_wlsr]>>,
          <pin COL_SEL[4]  of <device HSST_TILE @ [sx,sy]>>   => <wire COL_SEL_CCS  of <device WLSR_TILE @ [4 * NUM_GRID_X,sy_wlsr]>>,
          <pin COL_SEL[5]  of <device HSST_TILE @ [sx,sy]>>   => <wire COL_SEL_CCS  of <device WLSR_TILE @ [5 * NUM_GRID_X,sy_wlsr]>>,
          <pin COL_SEL[6]  of <device HSST_TILE @ [sx,sy]>>   => <wire COL_SEL_CCS  of <device WLSR_TILE @ [6 * NUM_GRID_X,sy_wlsr]>>,
          <pin COL_SEL[7]  of <device HSST_TILE @ [sx,sy]>>   => <wire COL_SEL_CCS  of <device WLSR_TILE @ [7 * NUM_GRID_X,sy_wlsr]>>,
          <pin COL_SEL[8]  of <device HSST_TILE @ [sx,sy]>>   => <wire COL_SEL_CCS1  of <device WLSRD_TILE @ [8 * NUM_GRID_X,sy_wlsr]>>,
          <pin COL_SEL[9]  of <device HSST_TILE @ [sx,sy]>>   => <wire COL_SEL_CCS2  of <device WLSRD_TILE @ [8 * NUM_GRID_X,sy_wlsr]>>,
          <pin COL_SEL[10]  of <device HSST_TILE @ [sx,sy]>>   => <wire COL_SEL_CCS  of <device WLSR_TILE @ [9 * NUM_GRID_X,sy_wlsr]>>,
          <pin COL_SEL[11]  of <device HSST_TILE @ [sx,sy]>>   => <wire COL_SEL_CCS  of <device WLSR_TILE @ [10 * NUM_GRID_X,sy_wlsr]>>,
          <pin READ_OR  of <device HSST_TILE @ [sx,sy]>>  => <wire BUS_WLSR_OUT[11]  of <device *blsr @ [sx_blsr,sy_blsr]>>

    );
    connect
    (
          <pin ADDR   of <device HSST_TILE @ [sx,sy]>>  => <wire ADDR[23:22] of <device WLSR_TILE @ [11 * NUM_GRID_X,sy_wlsr]>>,        
          <pin ADDR_C   of <device HSST_TILE @ [sx,sy]>>  => <wire ADDR[27:24] of <device WLSR_TILE @ [11 * NUM_GRID_X,sy_wlsr]>>,        

          <pin ADDR_U   of <device HSST_TILE @ [sx,sy]>>  => <wire ADDR[23:22] of <device WLSRA_TILE @ [11 * NUM_GRID_X,sy_wlsr + Snum_region]>>,        

          <pin GLOGEN  of <device HSST_TILE @ [sx,sy]>>  => <wire GLOGEN_OUT of <device WLSR_TILE @ [11 * NUM_GRID_X,sy_wlsr]>>,        
          <pin GRSN  of <device HSST_TILE @ [sx,sy]>>  => <wire BUS_GSEB_OUT[0] of <device WLSR_TILE @ [11 * NUM_GRID_X,sy_wlsr]>>        
    );
}//end of connect_hsst_wl_input

}//end of package pgl200es_funcs_connect_wl_bl

