      );
}// end of function connect_USCM_gs_addr_input

/*******************************************************************
********************************************************************/
function connect_blsr_gs_output // blsr signals by one to multi horizon 
(
    unsigned int rx,
    unsigned int tx,
    unsigned int tx_blsr,
    unsigned int tx_ccs,
    unsigned int ty_ccs,
    string device_wlsr,
    string sig_bus_wlsr_in,
    unsigned int tx_source,
    string sig_source_bus_wlsr_l,
    string sig_source_bus_wlsr_r,
    string sig_source_mce_r  ,
    string sig_source_mce_l  ,
    string sig_source_mclkb_r,
    string sig_source_mclkb_l,
    string sig_source_mwr_r  ,
    string sig_source_mwr_l  ,
    string dev_source,
    string dev_blsr,
    int flag_iob
)
{
    int Snum_region = rx * NUM_TILES_REGION * NUM_GRID_Y ;
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = rx * NUM_GRID_BLSR_Y;
    int sx_ccs = tx_ccs * NUM_GRID_X + 2;
    int sy_ccs = ty_ccs * NUM_GRID_Y;

    int sx = (device_wlsr=="IOCKGATEDLL_TILE"||device_wlsr=="IOCKGATEDLL2_TILE"||device_wlsr=="RCKB_TILE") ? tx * NUM_GRID_X + 2 : tx * NUM_GRID_X;
    int sy = (device_wlsr=="IOCKGATEDLL_TILE"||device_wlsr=="IOCKGATEDLL2_TILE"||device_wlsr=="RCKB_TILE") ? (rx*NUM_TILES_REGION+MID_TILE_REGION-1)*NUM_GRID_Y :
             (flag_iob==1) ? rx * NUM_TILES_REGION * NUM_GRID_Y :
             (rx*NUM_TILES_REGION+MID_TILE_REGION)*NUM_GRID_Y;

    int sx_source = tx_source * NUM_GRID_X ;
    
    int sy_source = (tx_source==tx_blsr) ? sy_blsr : (rx*NUM_TILES_REGION+MID_TILE_REGION)*NUM_GRID_Y ;

    string source = dev_source ;
    string blsr = dev_blsr ;
    string source_bus_wlsr_l = sig_source_bus_wlsr_l ;
    string source_bus_wlsr_r = sig_source_bus_wlsr_r ;
    string source_mce_r   = sig_source_mce_r   ;
    string source_mce_l   = sig_source_mce_l   ;
    string source_mclkb_r = sig_source_mclkb_r ;
    string source_mclkb_l = sig_source_mclkb_l ;
    string source_mwr_r   = sig_source_mwr_r   ;
    string source_mwr_l   = sig_source_mwr_l   ;
    string wlsr = device_wlsr;
    string bus_wlsr_in = sig_bus_wlsr_in;

    connect
    (
        <pin WLSHFT_REF of <device *wlsr @[sx,sy]>>          => (tx>tx_blsr) ? <wire WLSR_REF[1] of <device *blsr @[sx_blsr,sy_blsr]>>
                                                                             : <wire WLSR_REF[0] of <device *blsr @[sx_blsr,sy_blsr]>>
    );

    connect
    (
        <pin *bus_wlsr_in of <device *wlsr @[sx,sy]>>        => (tx>tx_blsr) ? <wire *source_bus_wlsr_r of <device *source @[sx_source,sy_source]>>
                                                                             : <wire *source_bus_wlsr_l of <device *source @[sx_source,sy_source]>>
    );
    

    if(wlsr=="WLSRD_TILE"||wlsr=="WLSR1B_TILE")
    {
      connect
      (
          <pin MCE of <device *wlsr @[sx,sy]>>   => (tx>tx_blsr) ? <wire *source_mce_r   of <device *source @[sx_source,sy_source]>>
                                                                 : <wire *source_mce_l   of <device *source @[sx_source,sy_source]>>,
          <pin MCLKB of <device *wlsr @[sx,sy]>> => (tx>tx_blsr) ? <wire *source_mclkb_r of <device *source @[sx_source,sy_source]>>
                                                                 : <wire *source_mclkb_l of <device *source @[sx_source,sy_source]>>,
          <pin MWR of <device *wlsr @[sx,sy]>>   => (tx>tx_blsr) ? <wire *source_mwr_r   of <device *source @[sx_source,sy_source]>>
                                                                 : <wire *source_mwr_l   of <device *source @[sx_source,sy_source]>>
      );
    }

    if(!(wlsr=="IOB_TILE"||wlsr=="IOB_TILE_HALF"))
    {
        connect(<pin VCTRL of <device *wlsr @[sx,sy]>>              => <wire NGB of <device CCS_TILE @[sx_ccs,sy_ccs]>>);
    }
    
}//end of function connect_blsr_gs_output
/*******************************************************************
********************************************************************/
function connect_blsr_gs_output1 
(
    unsigned int index,
    unsigned int tx_blsr,
    unsigned int rx,
    unsigned int l_tx_wlsrs_0[7:0],
    unsigned int l_tx_wlsrs_1[6:0],
    unsigned int l_tx_wlsrs_2[6:0],
    unsigned int l_tx_wlsrs_3[5:0],
    unsigned int l_tx_wlsr1s[],
    unsigned int l_tx_wlsr1b[],
    unsigned int l_tx_iock[],
    unsigned int tx_wlsr1_l,
    unsigned int tx_wlsr1_r,
    string dev_blsr ,
    unsigned int flag_y, // for y
    unsigned int less_skew  
)
{
    string blsr = dev_blsr ;

    int sx , sx_skew,sy,sy_skew;

    int ty = rx * NUM_TILES_REGION + MID_TILE_REGION;
    int sy_blsr = rx * NUM_GRID_BLSR_Y;
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int Snum_region = rx * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy_wlsr = MID_TILE_REGION * NUM_GRID_Y + Snum_region ;
    int sx_wlsr1_l = tx_wlsr1_l * NUM_GRID_X;
    int sx_wlsr1_r = tx_wlsr1_r * NUM_GRID_X;

    int tx =  (flag_y==0) ? l_tx_wlsrs_0[index] :
              (flag_y==1) ? l_tx_wlsrs_1[index] :
              (flag_y==2) ? l_tx_wlsrs_2[index] :
                            l_tx_wlsrs_3[index] ;

    string dev_iock = (rx==3&&tx<tx_blsr) ? "IOCKGATEDLL2_TILE" : "IOCKGATEDLL_TILE" ; 
   
    int tx_skew ,tmp_tx;

    string device_wlsr,device_skew ;

    if(index!=0)
    {
        tx_skew =  (flag_y==0) ? l_tx_wlsrs_0[index - 1] :
                   (flag_y==1) ? l_tx_wlsrs_1[index - 1] :
                   (flag_y==2) ? l_tx_wlsrs_2[index - 1] :
                                 l_tx_wlsrs_3[index - 1] ;
        foreach tmp_tx(l_tx_wlsr1s)
        {
          if(tmp_tx==tx)
          {
           device_wlsr = "WLSR1_TILE";
          }
          if(tmp_tx==tx_skew)
          {
           device_skew = "WLSR1_TILE";
          }
        }
        foreach tmp_tx(l_tx_wlsr1b)
        {
          if(tmp_tx==tx)
          {
           device_wlsr = "WLSR1B_TILE";
          }
          if(tmp_tx==tx_skew)
          {
           device_skew = "WLSR1B_TILE";
          }
        }
        foreach tmp_tx(l_tx_iock)
        {
          if(tmp_tx==tx)
          {
           device_wlsr = dev_iock;
          }
          if(tmp_tx==tx_skew)
          {
           device_skew = dev_iock;
          }
        }

        sx = device_wlsr == "IOCKGATEDLL_TILE"  ? tx * NUM_GRID_X + 2 : 
             device_wlsr == "IOCKGATEDLL2_TILE" ? tx * NUM_GRID_X + 2 : 
             device_wlsr == "RCKB_TILE"         ? tx * NUM_GRID_X + 2 : 
                                                  tx * NUM_GRID_X ;
        sx_skew = device_skew == "IOCKGATEDLL_TILE"  ? tx_skew * NUM_GRID_X + 2 : 
                  device_skew == "IOCKGATEDLL2_TILE" ? tx_skew * NUM_GRID_X + 2 : 
                  device_skew == "RCKB_TILE"         ? tx_skew * NUM_GRID_X + 2 : 
                                                       tx_skew * NUM_GRID_X ;
        sy = device_wlsr == "IOCKGATEDLL_TILE"  ? ty * NUM_GRID_Y - NUM_GRID_Y :
             device_wlsr == "IOCKGATEDLL2_TILE" ? ty * NUM_GRID_Y - NUM_GRID_Y : 
             device_wlsr == "RCKB_TILE"         ? ty * NUM_GRID_Y - NUM_GRID_Y : 
                                                  ty * NUM_GRID_Y ;
        sy_skew = device_skew == "IOCKGATEDLL_TILE"  ? ty * NUM_GRID_Y - NUM_GRID_Y :
                  device_skew == "IOCKGATEDLL2_TILE" ? ty * NUM_GRID_Y - NUM_GRID_Y : 
                  device_skew == "RCKB_TILE"         ? ty * NUM_GRID_Y - NUM_GRID_Y : 
                                                       ty * NUM_GRID_Y ;

        if(less_skew==0)
        {
            if(tx==tx_wlsr1_l)
            {
                connect
                (
                    <pin BUS_BSCAN_IN of <device *device_wlsr @ [sx_wlsr1_l,sy_wlsr]>>    => <wire BUS_BSCAN_OUTL of <device *blsr @ [sx_blsr,sy_blsr]>>,
                    <pin CLK_ADC_IN of <device *device_wlsr @ [sx_wlsr1_l,sy_wlsr]>>      => <wire CLK_ADC_L of <device *blsr @ [sx_blsr,sy_blsr]>>
                );
            }
            else if(tx==tx_wlsr1_r)
            {
                connect
                (
                    <pin BUS_BSCAN_IN of <device *device_wlsr @ [sx_wlsr1_r,sy_wlsr]>>    => <wire BUS_BSCAN_OUTR of <device *blsr @ [sx_blsr,sy_blsr]>>,
                    <pin CLK_ADC_IN of <device *device_wlsr @ [sx_wlsr1_r,sy_wlsr]>>      => <wire CLK_ADC_R of <device *blsr @ [sx_blsr,sy_blsr]>>
                );
            }
        }
        if((less_skew==0&&tx < tx_wlsr1_l)||(less_skew==1&&tx > tx_wlsr1_r))
        {

             connect
             (
                <pin BUS_BSCAN_IN of <device *device_wlsr @ [sx,sy]>>   => <wire BUS_BSCAN_OUT of <device *device_skew @ [sx_skew,sy_skew]>>,
                <pin CLK_ADC_IN of <device *device_wlsr @ [sx,sy]>>   => <wire CLK_ADC_OUT of <device *device_skew @ [sx_skew,sy_skew]>>
             );
        }
    }

}//end of function connect_blsr_gs_output1
/****************************************************************************************************
*****************************************************************************************************/
function connect_BLSR_input_loop
(
    unsigned int rx,
    unsigned int tx_wlsr1_l,
    unsigned int tx_wlsr1_r,
    unsigned int tx_blsr,
    unsigned int tx_iol0,
    unsigned int tx_iol1,
    unsigned int tx_iol2,
    unsigned int tx_ccs,
    unsigned int ty_ccs,
    unsigned int tx_wlsr2,
    unsigned int num_region,
    string dev_blsr,
    string dev_blsr_u,
    string dev_blsr_d,
    unsigned int tx_wlsr1b,
    unsigned int flag_y
)
{
    string blsr = dev_blsr ;
    string blsr_d = dev_blsr_d ;
    string blsr_u = dev_blsr_u ;

    int sx = tx_blsr * NUM_GRID_X;

    int Snum_reigon = rx * NUM_TILES_REGION * NUM_GRID_Y ;

    int sy = Snum_reigon ;

    int sx_iol0 = tx_iol0 * NUM_GRID_X + 2 ;
    int sx_iob0 = (tx_iol0 - 1) * NUM_GRID_X ;

    int sx_iol1 = tx_iol1 * NUM_GRID_X + 2 ;
    int sx_iob1 = (tx_iol1 + 1) * NUM_GRID_X ;

    int sx_iol2 = tx_iol2 * NUM_GRID_X + 2 ;
    int sx_iob2 = (tx_iol2 + 1) * NUM_GRID_X ;

    int sy_wlsr = MID_TILE_REGION * NUM_GRID_Y + Snum_reigon;
    int sy_iock = sy_wlsr - NUM_GRID_Y ;

    int sx_wlsr1_l = tx_wlsr1_l * NUM_GRID_X;
    int sx_wlsr1_r = tx_wlsr1_r * NUM_GRID_X;
    int sx_wlsr1b = tx_wlsr1b * NUM_GRID_X;
    int sy_iob = Snum_reigon;

    connect
      (
        <pin COL_INC_I of <device *blsr @ [sx,sy]>>         =>  <wire COL_INC_O of <device WLSR1B_TILE @  [sx_wlsr1b,sy_wlsr]>>, 

        <pin NONDRM_REG_INC_I of <device *blsr @ [sx,sy]>>  =>  <wire REGION_INC_O of <device WLSR1B_TILE @  [sx_wlsr1b,sy_wlsr]>>,

        <pin CAL_DONE_L_IN of <device *blsr @ [sx,sy]>>     => <wire CAL_DONE_OUT of <device WLSR1B_TILE @ [sx_wlsr1_l,sy_wlsr]>>,
        <pin CAL_DONE_R_IN of <device *blsr @ [sx,sy]>>     => (rx>=4) ? <wire CAL_DONE_OUT of <device WLSR1B_TILE @ [sx_wlsr1_r,sy_wlsr]>> : 
                                                                         <wire CAL_DONE_OUT of <device WLSR1B_TILE @ [sx_wlsr1_r,sy_wlsr]>>
      );

    int sx_ccs = tx_ccs * NUM_GRID_X + 2;
    int sy_ccs = ty_ccs * NUM_GRID_Y;

    if(rx==(num_region-1))
    {
      connect
        (
          <pin DATA_IN of <device *blsr @ [sx,sy]>>       => <wire DATA_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin CLK_IN of <device *blsr @ [sx,sy]>>        => <wire BLSRCLK_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin ERASE_EN of <device *blsr @ [sx,sy]>>      => <wire ERASE_EN_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin POR_ERASE_N of <device *blsr @ [sx,sy]>>   => <wire POR_N_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin PRECHG of <device *blsr @ [sx,sy]>>        => <wire PRECHG_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin PROG_CAP of <device *blsr @ [sx,sy]>>      => <wire PROG_CAP_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin PROG_ON of <device *blsr @ [sx,sy]>>       => <wire PROG_ON_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin READ of <device *blsr @ [sx,sy]>>          => <wire READ_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin RDBK_CAP of <device *blsr @ [sx,sy]>>      => <wire RDBK_CAP_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin WL_ON_I of <device *blsr @ [sx,sy]>>       => <wire WL_ON_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin PD_IN of <device *blsr @ [sx,sy]>>         => <wire PD_OUT of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin PUB_IN of <device *blsr @ [sx,sy]>>        => <wire PUB_OUT of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin WLSR_SEL_I of <device *blsr @ [sx,sy]>>    => <wire WLSHFT_SEL_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin RSTN of <device *blsr @ [sx,sy]>>          => <wire RST_BLSR_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin BLSR_SEL of <device *blsr @ [sx,sy]>>      => <wire BLSHFT_SEL_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin REGION_RW_EN of <device *blsr @ [sx,sy]>>  => <wire REGION_RW_EN_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin COL_IN_I of <device *blsr @ [sx,sy]>>      => <wire COL_IN_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin TYPE_I of <device *blsr @ [sx,sy]>>        => <wire TYPE_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin FRAME_ADDR_I of <device *blsr @ [sx,sy]>>  => <wire FRAME_ADDR_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,

          <pin MCLKB_I of <device *blsr @ [sx,sy]>>       => <wire MCLKB_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin MCE_I of <device *blsr @ [sx,sy]>>         => <wire MCE_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin MWR_I of <device *blsr @ [sx,sy]>>         => <wire MWR_O of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,

          <pin BUS_BSCAN_IN of <device *blsr @ [sx,sy]>>  => <wire BUS_BSCAN_OUT of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin CLK_REL_IN of <device *blsr @ [sx,sy]>>    => <wire RELCK of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin GLOGEN_IN of <device *blsr @ [sx,sy]>>     => <wire GLOGEN of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin GRS_N_IN of <device *blsr @ [sx,sy]>>      => <wire GRS_N of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin GWEN_IN of <device *blsr @ [sx,sy]>>       => <wire GWEN of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin POR_N_IN of <device *blsr @ [sx,sy]>>      => <wire POR_N of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin PUP_REL_N_IN of <device *blsr @ [sx,sy]>>  => <wire PUP_SEL_N of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,

          <pin CLK_ADC_IN0 of <device *blsr @ [sx,sy]>>   => <wire CLK_ADC of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin CLK_ADC_IN1 of <device *blsr @ [sx,sy]>>   => <wire CLK_ADC of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin CLK_ADC_IN2 of <device *blsr @ [sx,sy]>>   => <wire CLK_ADC of <device CCS_TILE @ [sx_ccs,sy_ccs]>>,
          <pin CLK_USER_IN of <device *blsr @ [sx,sy]>>   => <wire CLK_USER of <device CCS_TILE @ [sx_ccs,sy_ccs]>>

        );
    }
    else
    {
       connect
        (
          <pin DATA_IN of <device *blsr @ [sx,sy]>>       => <wire DATA_TO_NEXT of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin CLK_IN of <device *blsr @ [sx,sy]>>        => <wire CLKOUT_M of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin ERASE_EN of <device *blsr @ [sx,sy]>>      => <wire ERASE_EN_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin POR_ERASE_N of <device *blsr @ [sx,sy]>>   => <wire POR_ERASE_N_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin PRECHG of <device *blsr @ [sx,sy]>>        => <wire PRECHG_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin PROG_CAP of <device *blsr @ [sx,sy]>>      => <wire PROG_CAP_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin PROG_ON of <device *blsr @ [sx,sy]>>       => <wire PROG_ON_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin READ of <device *blsr @ [sx,sy]>>          => <wire READ_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin RDBK_CAP of <device *blsr @ [sx,sy]>>      => <wire RDBK_CAP_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin WL_ON_I of <device *blsr @ [sx,sy]>>       => <wire WL_ON_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin PD_IN of <device *blsr @ [sx,sy]>>         => <wire PD_OUT of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin PUB_IN of <device *blsr @ [sx,sy]>>        => <wire PUB_OUT of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin WLSR_SEL_I of <device *blsr @ [sx,sy]>>    => <wire WLSR_SEL_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin RSTN of <device *blsr @ [sx,sy]>>          => <wire RSTN_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin BLSR_SEL of <device *blsr @ [sx,sy]>>      => <wire BLSR_SEL_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin REGION_RW_EN of <device *blsr @ [sx,sy]>>  => <wire REGION_RW_EN_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin COL_IN_I of <device *blsr @ [sx,sy]>>      => <wire COL_IN_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin TYPE_I of <device *blsr @ [sx,sy]>>        => <wire TYPE_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin FRAME_ADDR_I of <device *blsr @ [sx,sy]>>  => <wire FRAME_ADDR_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,

          <pin MCLKB_I of <device *blsr @ [sx,sy]>>       => <wire MCLKB_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin MCE_I of <device *blsr @ [sx,sy]>>         => <wire MCE_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,
          <pin MWR_I of <device *blsr @ [sx,sy]>>         => <wire MWR_O of <device *blsr_u @ [sx,sy+NUM_GRID_BLSR_Y]>>,

