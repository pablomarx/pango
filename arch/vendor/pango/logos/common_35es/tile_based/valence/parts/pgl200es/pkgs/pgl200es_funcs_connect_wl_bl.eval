    int sx_clm = sx + 2 ;

    string pin_wlout = "ADDR";

    int Snum_region = region_index * NUM_TILES_REGION * NUM_GRID_Y ;

    int sy = (flag_lop == 0) ? MID_TILE_REGION * NUM_GRID_Y + Snum_region :
             (flag_lop == 1) ? MID_TILE_REGION * NUM_GRID_Y :
                               MID_TILE_REGION * NUM_GRID_Y + NUM_TILES_REGION * NUM_GRID_Y ;

    int sy_clm = ty_clm * NUM_GRID_Y + Snum_region;

    connect 
      (
        <pin GRS_N of <device *clm @ [sx_clm,sy_clm]>>  => <wire BUS_GSEB_OUT[0] of <device *wlsr @ [sx,sy]>>,
        <pin ADDR of <device *clm @ [sx_clm,sy_clm]>>   => <wire ADDR[27:22] of <device *wlsr @ [sx,sy]>>,
        <pin GLOGEN of <device *clm @ [sx_clm,sy_clm]>> => <wire GLOGEN_OUT of <device *wlsr  @ [sx,sy]>>
