use pgl_defs,pgl200es_defs;

package pgl200es_connect_wl_bl
{
/*******************************************************************
********************************************************************/
function connect_col_wlsr_addr
(
    unsigned int tx_blsr,
    unsigned int tx_wlsr,
    unsigned int num_wlsr,
    unsigned int num_rckb,
    unsigned int flag_rckb,
    unsigned int region_index,
    unsigned int l_wlsr_tx[],
    unsigned int l_wlsrd_tx[],
    unsigned int l_wlsr2_tx[],
    unsigned int l_wlsr1_tx[],
    unsigned int l_wlsr1b_tx[],
    unsigned int l_iock_tx[],
    unsigned int l_iob_tx[],
    string sig_col_addr,
    string dev_blsr,
    string dev_iob
)
{

    string col_addr = sig_col_addr ;
    string blsr = dev_blsr ;
    string iob = dev_iob ;

    int sx_blsr = tx_blsr * NUM_GRID_X;

    int tmp_x;

    string wlsr ; 

    int ty = MID_TILE_REGION + region_index * NUM_TILES_REGION ;

    int sy_blsr = region_index * NUM_GRID_BLSR_Y;

    string col_n = (tx_wlsr < tx_blsr) ? "COL_L_N_O" : "COL_R_N_O" ;   
    string col = (tx_wlsr < tx_blsr) ? "COL_L_O" : "COL_R_O" ;   

    if(flag_rckb>0&&region_index==4)
    {
        wlsr = "WLSRA_TILE" ;
    }
    else
    {
        foreach tmp_x(l_wlsr_tx)
        {
            if(tmp_x==tx_wlsr)
            {
                wlsr = "WLSR_TILE" ;
                break ;
            }
        }
    }
    foreach tmp_x(l_wlsrd_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSRD_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_wlsr1_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSR1_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_wlsr1b_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSR1B_TILE" ;
            break ;
        }
    }
    foreach tmp_x(l_wlsr2_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = "WLSR2_TILE" ;
            break ;
        }
    }
    if(flag_rckb>0&&(region_index==2||region_index==3))
    {
        wlsr = "WLSRA_TILE" ;
    }
    else if(tx_wlsr==1&&region_index==3)
    {
        wlsr = "IOCKGATEDLL2_TILE" ;
    }
    else
    {
        foreach tmp_x(l_iock_tx)
        {
            if(tmp_x==tx_wlsr)
            {
                wlsr = "IOCKGATEDLL_TILE" ;
                break ;
            }
        }
    }
    foreach tmp_x(l_iob_tx)
    {
        if(tmp_x==tx_wlsr)
        {
            wlsr = *iob ;
            break ;
        }
    }

    int sx = (wlsr == "IOCKGATEDLL_TILE"|| wlsr == "IOCKGATEDLL2_TILE"||wlsr == "RCKB_TILE") ? tx_wlsr * NUM_GRID_X + 2 : tx_wlsr * NUM_GRID_X ;
    int sy = (wlsr == "IOCKGATEDLL_TILE"|| wlsr == "IOCKGATEDLL2_TILE"||wlsr == "RCKB_TILE") ? ty * NUM_GRID_Y - NUM_GRID_Y :
             (wlsr == "IOB_TILE") ? ty * NUM_GRID_Y - MID_TILE_REGION * NUM_GRID_Y:
                                    ty * NUM_GRID_Y ;

    int fflag7 = num_wlsr - 128 ;
    int flag7 = (fflag7 >= 0) ? 1 : 0 ;

    int fflag6 = num_wlsr - flag7 * 128 - 64 ;
    int flag6 = (fflag6 >= 0) ? 1 : 0 ;

    int fflag5 = num_wlsr - flag7 * 128 - flag6 * 64 - 32 ;
    int flag5 = (fflag5 >= 0) ? 1 : 0 ;

    int fflag4 = num_wlsr - flag7 * 128 - flag6 * 64 - flag5 * 32 - 16 ;
    int flag4 = (fflag4 >= 0) ? 1 : 0 ;

    int fflag3 = num_wlsr - flag7 * 128 - flag6 * 64 - flag5 * 32 - flag4 * 16 - 8;
    int flag3 = (fflag3 >= 0) ? 1 : 0 ;

    int fflag2 = num_wlsr - flag7 * 128 - flag6 * 64 - flag5 * 32 - flag4 * 16 - flag3 * 8 - 4;
    int flag2 = (fflag2 >= 0) ? 1 : 0 ;

    int fflag1 = num_wlsr - flag7 * 128 - flag6 * 64 - flag5 * 32 - flag4 * 16 - flag3 * 8 - flag2 * 4 - 2;
    int flag1 = (fflag1 >= 0) ? 1 : 0 ;

    int fflag0 = num_wlsr - flag7 * 128 - flag6 * 64 - flag5 * 32 - flag4 * 16 - flag3 * 8 - flag2 * 4 - flag1 * 2 - 1;
    int flag0 = (fflag0 >= 0) ? 1 : 0 ;

    if(num_rckb==0)
    {
        connect
        (
            <pin *col_addr[7] of <device *wlsr @[sx,sy]>> => (flag7 == 1) ? <wire *col[7] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[7] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[6] of <device *wlsr @[sx,sy]>> => (flag6 == 1) ? <wire *col[6] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[6] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[5] of <device *wlsr @[sx,sy]>> => (flag5 == 1) ? <wire *col[5] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[5] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[4] of <device *wlsr @[sx,sy]>> => (flag4 == 1) ? <wire *col[4] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[4] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[3] of <device *wlsr @[sx,sy]>> => (flag3 == 1) ? <wire *col[3] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[3] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[2] of <device *wlsr @[sx,sy]>> => (flag2 == 1) ? <wire *col[2] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[2] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[1] of <device *wlsr @[sx,sy]>> => (flag1 == 1) ? <wire *col[1] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[1] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[0] of <device *wlsr @[sx,sy]>> => (flag0 == 1) ? <wire *col[0] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[0] of <device *blsr @ [sx_blsr,sy_blsr]>> 
        );
    }
    else
    {
        connect
        (
            <pin *col_addr[15] of <device *wlsr @[sx,sy]>> => (flag7 == 1) ? <wire *col[7] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[7] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[14] of <device *wlsr @[sx,sy]>> => (flag6 == 1) ? <wire *col[6] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[6] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[13] of <device *wlsr @[sx,sy]>> => (flag5 == 1) ? <wire *col[5] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[5] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[12] of <device *wlsr @[sx,sy]>> => (flag4 == 1) ? <wire *col[4] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[4] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[11] of <device *wlsr @[sx,sy]>> => (flag3 == 1) ? <wire *col[3] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[3] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[10] of <device *wlsr @[sx,sy]>> => (flag2 == 1) ? <wire *col[2] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[2] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[9] of <device *wlsr @[sx,sy]>> => (flag1 == 1) ? <wire *col[1] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[1] of <device *blsr @ [sx_blsr,sy_blsr]>> ,
            <pin *col_addr[8] of <device *wlsr @[sx,sy]>> => (flag0 == 1) ? <wire *col[0] of <device  *blsr @ [sx_blsr,sy_blsr]>> :
                                                                           <wire *col_n[0] of <device *blsr @ [sx_blsr,sy_blsr]>> 
        );
    }

}//end of function connect_col_wlsr_addr
/****************************************************************************************************
*****************************************************************************************************/
function connect_CCS_wl_bl
(
    unsigned int tx,
    unsigned int ty,
    unsigned int ty_pll0,
    unsigned int ty_pll1,
    unsigned int tx_bufs,
    unsigned int ty_bufs,
    unsigned int tx_blsr,
    unsigned int ty_blsr,
    unsigned int tx_idcode,
    unsigned int ty_idcode,
    unsigned int tx_wlsr1,
    unsigned int tx_iob
)
{
    int sx = tx*NUM_GRID_X + 2;
    int sx_iob = tx_iob*NUM_GRID_X;

    int tmp_ty,sy;
    sy = ty * NUM_GRID_Y;
    for(tmp_ty=ty;tmp_ty > MID_TILE_REGION;tmp_ty = tmp_ty - NUM_TILES_REGION)
    {
      sy = sy + NUM_GRID_WLSR_Y - NUM_GRID_Y;
    }
  
  connect 
    (
      <pin ADDR_D of <device CCS_TILE @[sx,sy]>>       => <wire ADDR_D of <device WLSRD_TILE @ [sx,sy-NUM_GRID_WLSR_Y]>>,
      <pin ADDR2 of <device CCS_TILE @[sx,sy]>>        => <wire ADDR[22] of <device WLSRD_TILE @ [sx,sy-NUM_GRID_WLSR_Y]>>,
      <pin ADDR1 of <device CCS_TILE @[sx,sy]>>        => <wire ADDR[27] of <device WLSR_TILE @ [sx+9*NUM_GRID_X,sy-NUM_GRID_WLSR_Y]>>,
      <pin ADDR0 of <device CCS_TILE @[sx,sy]>>        => <wire ADDR[26] of <device WLSR_TILE @ [sx+9*NUM_GRID_X,sy-NUM_GRID_WLSR_Y]>>,
      <pin ADDR_BLINT of <device CCS_TILE @[sx,sy]>>   => <wire ADDR[25:0] of <device WLSR_TILE @ [sx+9*NUM_GRID_X,sy-NUM_GRID_WLSR_Y]>>
    );
 
  int sx_blsr = tx_blsr * NUM_GRID_X;
  int sy_blsr = ty_blsr * NUM_GRID_BLSR_Y;

  int sy_esd = NUM_REGION * ((NUM_TILES_REGION-1)*NUM_GRID_Y + NUM_GRID_WLSR_Y);

  connect
    (
      <pin COL_INC_I of <device CCS_TILE @[sx,sy]>>        => <wire COL_INC_O of <device BLSR_PGL200ES_TILE @ [sx_blsr,sy_blsr]>>,
      <pin NONDRM_REG_INC_I of <device CCS_TILE @[sx,sy]>> => <wire NONDRM_REG_INC_O of <device BLSR_PGL200ES_TILE @ [sx_blsr,sy_blsr]>>,
      <pin NONDRM_END_I of <device CCS_TILE @[sx,sy]>>     => <wire NONDRM_END_O of <device BLSR_PGL200ES_TILE @ [sx_blsr,sy_blsr]>>,
      <pin DATABACK_I of <device CCS_TILE @[sx,sy]>>       => <wire DSROUTTO_CONFIG of <device BLSR_PGL200ES_TILE @ [sx_blsr,sy_blsr]>>,
      <pin DATA of <device CCS_TILE @[sx,sy]>>             => <wire DATA[2175:1096] of <device BLSR_PGL200ES_TILE @ [sx_blsr,sy_blsr]>>,
      <pin DATAN of <device CCS_TILE @[sx,sy]>>            => <wire DATAN[2175:1096] of <device BLSR_PGL200ES_TILE @ [sx_blsr,sy_blsr]>>,
      <pin READ_OL of <device CCS_TILE @[sx,sy]>>          => <wire BUS_WLSR_OUTL[11] of <device BLSR_PGL200ES_TILE @ [sx_blsr,sy_blsr]>>,
      <pin DCI_MATCH of <device CCS_TILE @[sx,sy]>>        => <wire DCI_INIT of <device BLSR_PGL200ES_TILE@[sx_blsr,sy_blsr]>>,
      <pin DDR_VREF_I of <device CCS_TILE @ [sx,sy]>>       => <wire VREF       of <device IOB_TILE @ [sx_iob,7*NUM_TILES_REGION*NUM_GRID_Y]>>,
      <pin CMFB_VREF_I of <device CCS_TILE @ [sx,sy]>>      => <wire IVREF_T[9] of <device IOB_TILE @ [sx_iob,7*NUM_TILES_REGION*NUM_GRID_Y]>>

    );

    int sx_pll = PLL_TILE_X * NUM_GRID_X + 2;
    int sy_pll0 =  ty_pll0 * NUM_GRID_Y;
    int sy_pll1 =  ty_pll1 * NUM_GRID_Y;
    int sy_pll2 =  sy_pll0 + 1 * NUM_TILES_REGION * NUM_GRID_Y;
    int sy_pll3 =  sy_pll1 + 1 * NUM_TILES_REGION * NUM_GRID_Y;
    int sy_pll4 =  sy_pll0 + 2 * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy_pll5 =  sy_pll1 + 2 * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy_pll6 =  sy_pll0 + 3 * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy_pll7 =  sy_pll1 + 3 * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy_pll8 =  sy_pll0 + 4 * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy_pll9 =  sy_pll1 + 4 * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy_pll10 = sy_pll0 + 5 * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy_pll11 = sy_pll1 + 5 * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy_pll12 = sy_pll0 + 6 * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy_pll13 = sy_pll1 + 6 * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy_pll14 = sy_pll0 + 7 * NUM_TILES_REGION * NUM_GRID_Y ;
    int sy_pll15 = sy_pll1 + 7 * NUM_TILES_REGION * NUM_GRID_Y ;
    int sx_wlsr1 = tx_wlsr1 * NUM_GRID_X;

    connect
      (
        <pin PLL_LOCK of <device CCS_TILE @ [sx,sy]>>         => <wire LOCK of <device PLL_TILE @ [sx_pll,sy_pll15]>>,
        <pin PLL_LDO_I[15] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll15]>>,
        <pin PLL_LDO_I[14] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll14]>>,
        <pin PLL_LDO_I[13] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll13]>>,
        <pin PLL_LDO_I[12] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll12]>>,
        <pin PLL_LDO_I[11] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll11]>>,
        <pin PLL_LDO_I[10] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll10]>>,
        <pin PLL_LDO_I[9] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll9]>>,
        <pin PLL_LDO_I[8] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll8]>>,
        <pin PLL_LDO_I[7] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll7]>>,
        <pin PLL_LDO_I[6] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll6]>>,
        <pin PLL_LDO_I[5] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll5]>>,
        <pin PLL_LDO_I[4] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll4]>>,
        <pin PLL_LDO_I[3] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll3]>>,
        <pin PLL_LDO_I[2] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll2]>>,
        <pin PLL_LDO_I[1] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll1]>>,
        <pin PLL_LDO_I[0] of <device CCS_TILE @ [sx,sy]>>     => <wire VCP of <device PLL_TILE @ [sx_pll,sy_pll0]>>

      );

    connect
    (
        <pin TDO_BSCAN_IO of <device CCS_TILE @[sx,sy]>>  => <wire SO_T_L of <device BUF_PGL200ES_TILE @ [tx_bufs*NUM_GRID_X,ty_bufs*NUM_GRID_Y]>>,//update @20180718

        <pin COL_SEL_CCS2 of <device CCS_TILE @[sx,sy]>>  => <wire COL_SEL_CCS of <device WLSR_TILE@[sx+1*NUM_GRID_X,sy-NUM_GRID_WLSR_Y]>>,
        <pin COL_SEL_CCS3 of <device CCS_TILE @[sx,sy]>>  => <wire COL_SEL_CCS of <device WLSR_TILE@[sx+2*NUM_GRID_X,sy-NUM_GRID_WLSR_Y]>>,
        <pin COL_SEL_CCS4 of <device CCS_TILE @[sx,sy]>>  => <wire COL_SEL_CCS of <device WLSR_TILE@[sx+3*NUM_GRID_X,sy-NUM_GRID_WLSR_Y]>>,
        <pin COL_SEL_CCS5 of <device CCS_TILE @[sx,sy]>>  => <wire COL_SEL_CCS of <device WLSR_TILE@[sx+4*NUM_GRID_X,sy-NUM_GRID_WLSR_Y]>>,
        <pin COL_SEL_CCS6 of <device CCS_TILE @[sx,sy]>>  => <wire COL_SEL_CCS of <device WLSR_TILE@[sx+5*NUM_GRID_X,sy-NUM_GRID_WLSR_Y]>>,
        <pin COL_SEL_CCS7 of <device CCS_TILE @[sx,sy]>>  => <wire COL_SEL_CCS of <device WLSR_TILE@[sx+6*NUM_GRID_X,sy-NUM_GRID_WLSR_Y]>>,
        <pin COL_SEL_CCS8 of <device CCS_TILE @[sx,sy]>>  => <wire COL_SEL_CCS1 of <device WLSRD_TILE@[sx+7*NUM_GRID_X,sy-NUM_GRID_WLSR_Y]>>,
        <pin COL_SEL_CCS9 of <device CCS_TILE @[sx,sy]>>  => <wire COL_SEL_CCS2 of <device WLSRD_TILE@[sx+7*NUM_GRID_X,sy-NUM_GRID_WLSR_Y]>>,
        <pin COL_SEL_CCS10 of <device CCS_TILE @[sx,sy]>>  => <wire COL_SEL_CCS of <device WLSR_TILE@[sx+8*NUM_GRID_X,sy-NUM_GRID_WLSR_Y]>>,

        <pin BSMODE1_DIO of <device CCS_TILE @[sx,sy]>>   => <wire BUS_BSCAN_OUT[2] of <device WLSR1B_TILE@[sx_wlsr1,sy-NUM_GRID_WLSR_Y]>>,
        <pin BSMODE2_DIO of <device CCS_TILE @[sx,sy]>>   => <wire BUS_BSCAN_OUT[3] of <device WLSR1B_TILE@[sx_wlsr1,sy-NUM_GRID_WLSR_Y]>>,
        <pin BSMODE3_DIO of <device CCS_TILE @[sx,sy]>>   => <wire BUS_BSCAN_OUT[15] of <device WLSR1B_TILE@[sx_wlsr1,sy-NUM_GRID_WLSR_Y]>>,
        <pin UPDATEDR_DIO of <device CCS_TILE @[sx,sy]>>  => <wire BUS_BSCAN_OUT[6] of <device WLSR1B_TILE@[sx_wlsr1,sy-NUM_GRID_WLSR_Y]>>,
        <pin SHIFTDR_DIO of <device CCS_TILE @[sx,sy]>>   => <wire BUS_BSCAN_OUT[5] of <device WLSR1B_TILE@[sx_wlsr1,sy-NUM_GRID_WLSR_Y]>>,
        <pin GOUTEN_DIO of <device CCS_TILE @[sx,sy]>>    => <wire BUS_BSCAN_OUT[13] of <device WLSR1B_TILE@[sx_wlsr1,sy-NUM_GRID_WLSR_Y]>>
    );

    connect
      (
        <pin GLOGEN_FB of <device CCS_TILE @ [sx,sy]>>  => <wire GLOGEN_FB_OUT of <device BUF_PGL200ES_TILE @ [tx_bufs*NUM_GRID_X,ty_bufs*NUM_GRID_Y]>>//update @20180718
      );

    int sx_idcode = tx_idcode * NUM_GRID_X + 2;
    int sy_idcode = ty_idcode * NUM_GRID_Y;

    connect
    (
        <pin IDCODE of <device CCS_TILE @[sx,sy]>>  => <wire IDCODE of <device IDCODE_PGL200ES_TILE @ [sx_idcode,sy_idcode]>>
    );

    &wire ntRef_ntCFG_DONE     = <wire ntCFG_DONE of architecture>;
    &wire ntRef_ntCFG_I_FCLK   = <wire ntCFG_I_FCLK of architecture>;
    &wire ntRef_ntI_FCS_N   = <wire ntI_FCS_N of architecture>;
    &wire ntRef_ntI_D0          = <wire ntI_D0 of architecture>;
    &wire ntRef_ntI_D1          = <wire ntI_D1 of architecture>;
    &wire ntRef_ntI_D2          = <wire ntI_D2 of architecture>;
    &wire ntRef_ntI_D3          = <wire ntI_D3 of architecture>;
    &wire ntRef_ntREXT         = <wire ntREXT of architecture>;
    &wire ntRef_ntRST_N        = <wire ntRST_N of architecture>;
    &wire ntRef_ntTCK          = <wire ntTCK of architecture>;
    &wire ntRef_ntTDI          = <wire ntTDI of architecture>;
    &wire ntRef_ntTDO          = <wire ntTDO of architecture>;
    &wire ntRef_ntTMS          = <wire ntTMS of architecture>;
    &wire ntRef_ntVDDEFUSE     = <wire ntVDDEFUSE of architecture>;
    &wire ntRef_ntVDDIOCFG      = <wire ntVDDIOCFG of architecture>;
    &wire ntRef_ntVSSIOCFG     = <wire ntVSSIOCFG of architecture>;

    connect
    (
        <pin CFG_DONE   of <device CCS_TILE @[sx,sy]>>  =>  ntRef_ntCFG_DONE  ,  
        <pin CFG_I_FCLK of <device CCS_TILE @[sx,sy]>>  =>  ntRef_ntCFG_I_FCLK, 
        <pin I_FCS_N of <device CCS_TILE @[sx,sy]>>  =>  ntRef_ntI_FCS_N, 
        <pin I_D[0]        of <device CCS_TILE @[sx,sy]>>  =>  ntRef_ntI_D0       , 
        <pin I_D[1]        of <device CCS_TILE @[sx,sy]>>  =>  ntRef_ntI_D1       , 
