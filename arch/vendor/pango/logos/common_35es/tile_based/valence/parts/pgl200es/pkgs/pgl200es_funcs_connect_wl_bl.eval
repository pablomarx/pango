    int sy_uscm = ty_uscm * NUM_GRID_Y ;

   connect 
      (
        <pin GLOGEN of <device USCM_TILE @ [sx_uscm,sy_uscm]>>     => <wire GLOGEN_OUT of <device WLSR2_TILE @ [sx,sy]>>,
