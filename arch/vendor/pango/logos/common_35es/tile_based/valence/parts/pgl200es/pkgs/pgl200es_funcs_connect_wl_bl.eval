/********************************************************************************************
*********************************************************************************************/
function connect_cramsl2_addr_input
(
    unsigned int tx, 
    unsigned int tx_wlsr, 
    unsigned int region_index, 
    unsigned int ty_iol, 
    string dev_iol, 
    string dev_wlsr,
    unsigned int flag,  // for sy-4
    unsigned int flag_x_lr,
    unsigned int flag_lr,
    unsigned int flag_ip
)
{
    string iol = dev_iol ;
    string wlsr = dev_wlsr ;

    int sx = (flag_x_lr==1) ? tx * NUM_GRID_X  : tx * NUM_GRID_X + 2;   

    int sx_wlsr = (flag==1) ? tx_wlsr * NUM_GRID_Y + 2 :  tx_wlsr * NUM_GRID_Y ;

    int Snum_region = region_index * NUM_TILES_REGION * NUM_GRID_Y ;

    int sy = (flag==1) ? ( MID_TILE_REGION - 1 ) * NUM_GRID_Y + Snum_region : MID_TILE_REGION * NUM_GRID_Y + Snum_region;

    int sy_cramsl2 = ty_iol * NUM_GRID_Y + Snum_region;

    int sy_iol = (flag_ip==1) ? ty_iol * NUM_GRID_Y : ty_iol * NUM_GRID_Y + Snum_region;

    if(flag_lr==0)    
    {
        connect(<pin ADDR of <device *iol @ [sx,sy_iol]>>  => <wire ADDR[23:22] of <device *wlsr @ [sx_wlsr,sy]>>); 
    }
    if(flag_lr==1)    
    {
        connect(<pin ADDR of <device *iol @ [sx,sy_iol]>>  => <wire ADDR[25:24] of <device *wlsr @ [sx_wlsr,sy]>>); 
    }
    if(flag_lr==2)    
    {
        connect(<pin ADDR of <device *iol @ [sx,sy_iol]>>  => <wire ADDR[27:26] of <device *wlsr @ [sx_wlsr,sy]>>); 
    }
}// end of function connect_cramsl2_addr_input
