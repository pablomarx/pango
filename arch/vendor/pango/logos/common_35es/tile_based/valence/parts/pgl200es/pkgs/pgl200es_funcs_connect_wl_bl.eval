******************************************************************************************************************************/
function connect_SRB_gs_addr_input
(
    unsigned int tx,
    unsigned int ty_srb,
    unsigned int region_index,
    string dev_wlsr,
    unsigned int flag_iock        // for sx_srb
)
{
    string wlsr = dev_wlsr ;
    
    int temp_ty;
    int sx = (flag_iock==1) ? tx * NUM_GRID_X + 2 : tx * NUM_GRID_X ;  
    int sx_srb = (flag_iock==1) ? sx - 2 : sx ;

    string pin_wlout = "ADDR";

    int Snum_region = region_index * NUM_TILES_REGION * NUM_GRID_Y ;

    int sy =  MID_TILE_REGION * NUM_GRID_Y + Snum_region ;

    int sy_srb = ty_srb * NUM_GRID_Y + Snum_region;

    connect 
      (
        <pin PU_N of <device SRB_TILE @ [sx_srb,sy_srb]>>   => <wire BUS_GSEB_OUT[3]  of <device *wlsr @ [sx,sy]>>,
        <pin POR_N of <device SRB_TILE @ [sx_srb,sy_srb]>>  => <wire BUS_GSEB_OUT[2] of <device *wlsr @ [sx,sy]>>,
        <pin GLOGEN of <device SRB_TILE @ [sx_srb,sy_srb]>> => <wire GLOGEN_OUT of <device *wlsr  @ [sx,sy]>>,
        <pin ADDR of <device SRB_TILE @ [sx_srb,sy_srb]>>   => <wire *pin_wlout[21:0] of <device *wlsr @ [sx,sy]>>
      );

}//end of function connect_SRB_gs_addr_input
