/****************************************************************************************************
*****************************************************************************************************/
function connect_USCM_bl_input
(
    unsigned int rx,
    unsigned int tx,
    string sig_data,
    string sig_datan,
    string dev_blsr,
    string dev_blsr_u,
    unsigned int tx_blsr
)
{
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = rx * NUM_GRID_BLSR_Y;
    int sy_blsr_u = (rx+1) * NUM_GRID_BLSR_Y;
    int Snum_region = NUM_TILES_REGION * rx * NUM_GRID_Y ;
    int Snum_region_u = NUM_TILES_REGION * (rx+1) * NUM_GRID_Y ;
    int sx = tx * NUM_GRID_X + 2;
    string blsr = dev_blsr ;
    string blsr_u = dev_blsr_u ;
    string data = sig_data;
    string datan = sig_datan;
    connect
    (
      <pin DATA  of <device USCM_TILE @ [sx,0*NUM_GRID_Y+Snum_region_u]>>  => <wire *data[143:0]      of <device *blsr_u @ [sx_blsr,sy_blsr_u]>>,
      <pin DATAN of <device USCM_TILE @ [sx,0*NUM_GRID_Y+Snum_region_u]>>  => <wire *datan[143:0]     of <device *blsr_u @ [sx_blsr,sy_blsr_u]>>,

      <pin DATA  of <device USCM_TILE @ [sx,26*NUM_GRID_Y+Snum_region]>> => <wire *data[2175:2032]  of <device *blsr @ [sx_blsr,sy_blsr]>>,
      <pin DATAN of <device USCM_TILE @ [sx,26*NUM_GRID_Y+Snum_region]>> => <wire *datan[2175:2032] of <device *blsr @ [sx_blsr,sy_blsr]>>
    );


}//end of function connect_USCM_bl_input
