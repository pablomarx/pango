/****************************************************************************************************
*****************************************************************************************************/
function interconnect_wlsr1b_tile
(
    unsigned int rx,
    unsigned int tx_wlsr1_l0,
    unsigned int tx_wlsr1_l2,
    unsigned int tx_wlsr1_r0,
    unsigned int tx_wlsr1_r2,
    unsigned int tx_iob1,
    unsigned int tx_iob2,
    unsigned int flag_lr    
)
{

    int tx,sx,sx_iob2,sx_iob1,sx_iol1,sy_iob ;

    tx = (flag_lr==1) ? tx_wlsr1_l2 : (flag_lr==0&&rx<=3) ? tx_wlsr1_r0 : tx_wlsr1_r2 ;

    int sx_wlsr1_0 = (flag_lr==1) ? tx_wlsr1_l0 * NUM_GRID_X : tx_wlsr1_r0 * NUM_GRID_X;

    sx = tx * NUM_GRID_X ;

    sx_iob1 = tx_iob1 * NUM_GRID_X ;
    sx_iob2 = tx_iob2 * NUM_GRID_X ;
    sx_iol1 = (tx_iob1-1) * NUM_GRID_X ;

    int Snum_reigon = rx * NUM_TILES_REGION * NUM_GRID_Y ;

    sy_iob = Snum_reigon ;

    int sy = Snum_reigon + MID_TILE_REGION * NUM_GRID_Y;

    int sy_source = Snum_reigon + MID_TILE_REGION * NUM_GRID_Y;

    connect
    (
        <pin COL_INC of <device WLSR1B_TILE @ [sx,sy]>>   =>       (flag_lr==0&&rx>=4) ? <wire COL_INC_OUT of <device IOB_TILE @  [sx_iob2,sy_iob]>> :
                                                                   (flag_lr==0&&rx>=2) ? <wire COL_INC_OUT of <device WLSRA_TILE @ [sx_iol1,sy_source]>> :
                                                                   (flag_lr==0)        ? <wire COL_INC_OUT of <device IOB_TILE @  [sx_iob1,sy_iob]>> :
                                                                                         <wire TIE_LO_1V1  of <device WLSR1B_TILE @ [sx,sy]>> ,

        <pin REGION_INC of <device WLSR1B_TILE @ [sx,sy]>>   => (flag_lr==0&&rx>=4) ? <wire REGION_INC of <device IOB_TILE @  [sx_iob2,sy_iob]>> :
                                                                (flag_lr==0&&rx>=2) ? <wire REGION_INC of <device WLSRA_TILE @ [sx_iol1,sy_source]>> :
                                                                (flag_lr==0)        ? <wire REGION_INC of <device IOB_TILE @  [sx_iob1,sy_iob]>> :
                                                                                      <wire TIE_LO_1V1 of <device WLSR1B_TILE @ [sx,sy]>>,

        <pin REGION_END of <device WLSR1B_TILE @ [sx,sy]>>   =>  <wire TIE_LO_1V1 of <device WLSR1B_TILE @ [sx,sy]>>        
    );

    if(!(rx<=3&&flag_lr==0))
    {
        connect
        (
            <pin COL_INC of <device WLSR1B_TILE @ [sx_wlsr1_0,sy]>>      =>  <wire COL_INC_O of <device WLSR1B_TILE @ [sx,sy]>> ,
            <pin REGION_INC of <device WLSR1B_TILE @ [sx_wlsr1_0,sy]>>   =>  <wire REGION_INC_O of <device WLSR1B_TILE @ [sx,sy]>>,
            <pin REGION_END of <device WLSR1B_TILE @ [sx_wlsr1_0,sy]>>   =>  <wire REGION_END_O of <device WLSR1B_TILE @ [sx,sy]>>        
        );
    }

}//end of function interconnect_wlsr1b_tile
