/*******************************************************************
********************************************************************/
function connect_WLSR_gs_wl // cs ,col_inc,bus_gseb connect one by one horizon
(
    unsigned int index,
    unsigned int tx_blsr,
    unsigned int region_index,
    unsigned int l_tx_wlsrs_0[159:0],
    unsigned int l_tx_wlsrs_1[148:0],
    unsigned int l_tx_wlsrs_2[78:0],
    unsigned int l_tx_wlsrs_3[79:0],
    unsigned int l_tx_wlsr[],
    unsigned int l_tx_wlsr1[],
    unsigned int l_tx_wlsr1b[],
    unsigned int l_tx_wlsr2[],
    unsigned int l_tx_wlsrd[],
    unsigned int l_tx_iock[],
    unsigned int l_tx_iob[],
    unsigned int tx_rpt_ll,
    unsigned int tx_rpt_lr,
    unsigned int tx_rpt_rl,
    unsigned int tx_rpt_rr,
    string dev_blsr,
    unsigned int flag_y, // for y
    unsigned int less_skew //direction ; 1 , tx_skew < tx
)

{
    int sx,sy,sx_skew,sy_skew ;

    int ty = region_index * NUM_TILES_REGION + MID_TILE_REGION;
    int sx_blsr = tx_blsr * NUM_GRID_X;
    int sy_blsr = region_index * NUM_GRID_BLSR_Y;

    string blsr ,device_wlsr , device_skew , tielo;
    blsr = dev_blsr ;


    int tx =  (flag_y==0) ? l_tx_wlsrs_0[index] :
              (flag_y==1) ? l_tx_wlsrs_1[index] :
              (flag_y==2) ? l_tx_wlsrs_2[index] :
                            l_tx_wlsrs_3[index] ;

    string dev_iock = ((region_index==2||region_index==3)&&tx>tx_blsr) ? "WLSRA_TILE" : (region_index==3&&tx<tx_blsr) ? "IOCKGATEDLL2_TILE" : "IOCKGATEDLL_TILE" ; 

    string dev_iob = (region_index==3&&tx<tx_blsr) ? "IOB_TILE_HALF" : "IOB_TILE" ;
    
    int tx_skew ,tmp_tx;

    if(index!=0)
    {
        tx_skew =  (flag_y==0) ? l_tx_wlsrs_0[index - 1] :
                   (flag_y==1) ? l_tx_wlsrs_1[index - 1] :
                   (flag_y==2) ? l_tx_wlsrs_2[index - 1] :
                                 l_tx_wlsrs_3[index - 1] ;
    }
    else
    {
        tx_skew = tx;  
    }

    foreach tmp_tx(l_tx_wlsr)
    {
      if(flag_y==1&&tx==11)
      {
        device_wlsr = "WLSRA_TILE";
      }
      else
      {
          if(tmp_tx==tx)
          {
           device_wlsr = "WLSR_TILE";
          }
      }
      if(flag_y==1&&tx_skew==11)
      {
        device_skew = "WLSRA_TILE";
      }
      else
      {
          if(tmp_tx==tx_skew)
          {
           device_skew = "WLSR_TILE";
          }
      }
    }
    foreach tmp_tx(l_tx_wlsr1)
    {
      if(tmp_tx==tx)
      {
       device_wlsr = "WLSR1_TILE";
      }
      if(tmp_tx==tx_skew)
      {
       device_skew = "WLSR1_TILE";
      }
    }
    foreach tmp_tx(l_tx_wlsr1b)
    {
      if(tmp_tx==tx)
      {
       device_wlsr = "WLSR1B_TILE";
      }
      if(tmp_tx==tx_skew)
      {
       device_skew = "WLSR1B_TILE";
      }
    }
    foreach tmp_tx(l_tx_wlsr2)
    {
      if(tmp_tx==tx)
      {
       device_wlsr = "WLSR2_TILE";
      }
      if(tmp_tx==tx_skew)
      {
       device_skew = "WLSR2_TILE";
      }
    }
    foreach tmp_tx(l_tx_wlsrd)
    {
      if(tmp_tx==tx)
      {
       device_wlsr = "WLSRD_TILE";
      }
      if(tmp_tx==tx_skew)
      {
       device_skew = "WLSRD_TILE";
      }
    }
    
    foreach tmp_tx(l_tx_iock)
    {
      if(tmp_tx==tx)
      {
       device_wlsr = dev_iock;
      }
      if(tmp_tx==tx_skew)
      {
       device_skew = dev_iock;
      }
    }
    foreach tmp_tx(l_tx_iob)
    {
      if(tmp_tx==tx)
      {
       device_wlsr = dev_iob;
      }
      if(tmp_tx==tx_skew)
      {
       device_skew = dev_iob;
      }
    }
  
 
    sx = device_wlsr == "IOCKGATEDLL_TILE"  ? tx * NUM_GRID_X + 2 : 
         device_wlsr == "IOCKGATEDLL2_TILE" ? tx * NUM_GRID_X + 2 : 
         device_wlsr == "RCKB_TILE"         ? tx * NUM_GRID_X + 2 : 
                                              tx * NUM_GRID_X ;
    sx_skew = device_skew == "IOCKGATEDLL_TILE"  ? tx_skew * NUM_GRID_X + 2 : 
              device_skew == "IOCKGATEDLL2_TILE" ? tx_skew * NUM_GRID_X + 2 : 
              device_skew == "RCKB_TILE"         ? tx_skew * NUM_GRID_X + 2 : 
                                                   tx_skew * NUM_GRID_X ;
    sy = device_wlsr == "IOCKGATEDLL_TILE"  ? ty * NUM_GRID_Y - NUM_GRID_Y :
         device_wlsr == "IOCKGATEDLL2_TILE" ? ty * NUM_GRID_Y - NUM_GRID_Y : 
         device_wlsr == "RCKB_TILE"         ? ty * NUM_GRID_Y - NUM_GRID_Y : 
        (device_wlsr == "IOB_TILE"||device_wlsr == "IOB_TILE_HALF") ? region_index * NUM_TILES_REGION * NUM_GRID_Y : 
                                              ty * NUM_GRID_Y ;
    sy_skew = device_skew == "IOCKGATEDLL_TILE"  ? ty * NUM_GRID_Y - NUM_GRID_Y :
              device_skew == "IOCKGATEDLL2_TILE" ? ty * NUM_GRID_Y - NUM_GRID_Y : 
              device_skew == "RCKB_TILE"         ? ty * NUM_GRID_Y - NUM_GRID_Y : 
              (device_skew == "IOB_TILE"||device_skew == "IOB_TILE_HALF") ? region_index * NUM_TILES_REGION * NUM_GRID_Y :
                                                   ty * NUM_GRID_Y ;

    tielo = (device_wlsr=="WLSRA_TILE") ? "TIE_LO_1V1" :
            (device_wlsr=="IOB_TILE"||device_wlsr=="IOB_TILE_HALF") ? "S11_TILO" : "TIE_LO_1V1" ;

    if(less_skew==0) // tx_skew > tx
    {

        if(tx==(tx_blsr-1))
        {
          connect(<pin BUS_GSEB_IN of <device *device_wlsr @ [sx,sy]>>     => <wire BUS_GSEB_OUTL of <device *blsr @ [sx_blsr,sy_blsr]>>);
        }
        if(tx==(tx_blsr+1))
        {
          connect(<pin BUS_GSEB_IN of <device *device_wlsr @ [sx,sy]>>     => <wire BUS_GSEB_OUTR of <device *blsr @ [sx_blsr,sy_blsr]>>);
        }
        if(tx==tx_rpt_ll)
        {
            connect(<pin CS_RPT_2_LEFT of <device *blsr @ [sx_blsr,sy_blsr]>>     => <wire CS_OUT of <device *device_wlsr @ [sx,sy]>>);        
        }
        if(tx==tx_rpt_lr)
        {
            connect(<pin CS_RPT_1_LEFT of <device *blsr @ [sx_blsr,sy_blsr]>>     => <wire CS_OUT of <device *device_wlsr @ [sx,sy]>>);        
        }
        if(region_index>3)
        {
            if(tx==tx_rpt_rl)
            {
                connect(<pin CS_RPT_1_RIGHT of <device *blsr @ [sx_blsr,sy_blsr]>>     => <wire CS_OUT of <device *device_wlsr @ [sx,sy]>>);        
            }
            if(tx==tx_rpt_rr)
            {
                connect(<pin CS_RPT_2_RIGHT of <device *blsr @ [sx_blsr,sy_blsr]>>     => <wire CS_OUT of <device *device_wlsr @ [sx,sy]>>);        
            }        
        }
    }
    else
    {
        if(index!=0)
        {
            connect(<pin COL_INC_IN of <device *device_wlsr @[sx,sy]>>      =>  <wire COL_INC_OUT of <device *device_skew @ [sx_skew,sy_skew]>>);
        }
        else
        {
            connect(<pin COL_INC_IN of <device *device_wlsr @[sx,sy]>>      =>  <wire *tielo of <device *device_wlsr @ [sx,sy]>>);
        }
                
    }
    if(!(device_wlsr=="IOB_TILE"||device_wlsr=="IOB_TILE_HALF")&&((tx<tx_blsr-1&&less_skew==0)||(tx>tx_blsr+1&&less_skew==1)))
    {
        connect(<pin BUS_GSEB_IN of <device *device_wlsr @ [sx,sy]>>      => <wire BUS_GSEB_OUT of <device *device_skew @ [sx_skew,sy_skew]>>);
    }
    if((tx<tx_blsr&&index!=0&&less_skew==1)||(tx>tx_blsr&&index!=0&&less_skew==0))
    {
        connect(<pin CS_IN of <device *device_wlsr @[sx,sy]>>   => <wire CS_OUT of <device *device_skew @ [sx_skew,sy_skew]>>);
    }
    if((tx<tx_blsr&&index==0&&less_skew==1)||(tx>tx_blsr&&index==0&&less_skew==0))
    {
        connect(<pin CS_IN of <device *device_wlsr @[sx,sy]>>   => <wire *tielo of <device *device_wlsr @ [sx,sy]>>);
    }
   
}//end of function connect_WLSR_gs_wl
/***********************************************************************************************************************
************************************************************************************************************************/
function connect_iockgatedll_gs_input
(
    unsigned int tx,
    unsigned int rx,
    unsigned int tx_iol0,
    unsigned int tx_iol1,
    unsigned int tx_iol2,
    unsigned int tx_esd, 
    unsigned int ty_esd, 
    unsigned int tx_hsst, 
    unsigned int ty_hsst,
    unsigned int num_rx, 
    string dev_esd, 
    string dev_iock, 
    string dev_iock_u, 
    string dev_iock_d, 
    string dev_iob,
    string dev_hsst
)
{
    string iob = dev_iob ;
    string esd = dev_esd;
    string iock = dev_iock;
    string iock_u = dev_iock_u;
    string iock_d = dev_iock_d;
    string hsst = dev_hsst;

    int Snum_region = rx * NUM_TILES_REGION * NUM_GRID_Y;

    int sx = (tx>tx_iol0&&tx<tx_iol1) ? tx * NUM_GRID_X : tx * NUM_GRID_X + 2;
    int sx_iol1 = tx_iol1 * NUM_GRID_X + 2;
    int ty = (tx>tx_iol0&&tx<tx_iol1) ? rx * NUM_TILES_REGION + MID_TILE_REGION : rx * NUM_TILES_REGION + MID_TILE_REGION - 1;
    int sy = ty * NUM_GRID_Y;

    int sy_d = sy - NUM_TILES_REGION * NUM_GRID_Y ;

    int sy_u = sy + NUM_TILES_REGION * NUM_GRID_Y ;

    int sy_iob = Snum_region;

    int sx_iob = (tx==tx_iol0) ? sx-NUM_GRID_X-2 : sx+2;

    int sx_esd = tx_esd * NUM_GRID_X;

    int sy_esd = ty_esd * NUM_GRID_Y;

    int sx_hsst = tx_hsst * NUM_GRID_X;

    int sy_hsst = ty_hsst * NUM_GRID_Y;


    if(!(tx>tx_iol0&&tx<tx_iol1))
    {
        connect
        (
            <pin S11_PAD_STATE_IN of <device *iock @[sx,sy]>> => ((tx==tx_iol0)&&(rx==(num_rx-1))) ? <wire S11_PAD_STATE_O of <device IOB_TILE @[sx_iob,sy_iob]>> :
                                                                              ((tx==tx_iol2)&&(rx==(num_rx-1))) ? <wire S11_PAD_STATE_OUT_T of <device *esd @[sx_esd,sy_esd]>> :
                                                                              ((tx==tx_iol1)&&(rx==1)) ? <wire S11_PAD_STATE_OUT_R of <device *esd @[sx_esd,sy_esd]>> :
                                                                              ((tx==tx_iol0)&&(rx==3)) ? <wire S11_PAD_STATE_OUT_L of <device *esd @[sx_esd,sy_esd]>> :
                                                                                                        <wire S11_PAD_STATE_OUT of <device *iock_u @[sx,sy_u]>>,

            <pin CLOCK_DR_IN of <device *iock @[sx,sy]>> => ((tx==tx_iol0)&&(rx==(num_rx-1))) ? <wire CLOCK_DR_OUT_T of <device *esd @[sx_esd,sy_esd]>> :
                                                            (tx==tx_iol0&&rx==3) ? <wire CLOCKDR_OUT of <device *hsst @[sx_hsst,sy_hsst]>> :
                                                            ((tx==tx_iol1)&&(rx==0)) ? <wire CLOCK_DR_OUT_B of <device *esd @[sx_esd,sy_esd]>> :
                                                            ((tx==tx_iol2)&&(rx==4)) ? <wire CLOCK_DR_OUT_R of <device *esd @[sx_esd,sy_esd]>> :
                                                            (tx==tx_iol0)  ? <wire CLOCK_DR_OUT of <device *iock_u @[sx,sy_u]>> :
                                                                             <wire CLOCK_DR_OUT of <device *iock_d @[sx,sy_d]>> ,

            <pin GLOGEN_FB_IN of <device *iock @[sx,sy]>> => ((tx==tx_iol2)&&(rx==4)) ? <wire BUS_GSEB_OUT[4] of <device *iock @[sx,sy]>> :
                                                             (tx==tx_iol2) ? <wire GLOGEN_FB_OUT of <device *iock_d @ [sx,sy_d]>> :
                                                                                      <wire TIEH of <device *iock @[sx,sy]>>
        );
    }
                      
}//end of function connect_iockgatedll_gs_input
/***********************************************************************************************************************
************************************************************************************************************************/
function connect_iob_gs_input
(
    unsigned int tx,
    unsigned int rx,
    string dev_iob,
    string dev_iock
)
{
    unsigned int sx = tx * NUM_GRID_X;
    unsigned int sx_dll = (tx==0) ? sx+6 : sx-2;

    unsigned int Snum_region = rx * NUM_TILES_REGION * NUM_GRID_Y;
    unsigned int sy = Snum_region;
    unsigned int sy_dll = (MID_TILE_REGION - 1) * NUM_GRID_Y + Snum_region;

    string dll =  dev_iock;
    string iob = dev_iob ;

    connect
    (
        <pin S11_BSRRST of <device *iob @[sx,sy]>>           => <wire BUS_BSCAN_OUT[4] of <device *dll @[sx_dll,sy_dll]>>,
        <pin BSMODE1 of <device *iob @[sx,sy]>>              => <wire BUS_BSCAN_OUT[2] of <device *dll @[sx_dll,sy_dll]>>,
        <pin BSMODE2 of <device *iob @[sx,sy]>>              => <wire BUS_BSCAN_OUT[3] of <device *dll @[sx_dll,sy_dll]>>,
        <pin BSMODE3 of <device *iob @[sx,sy]>>              => <wire BUS_BSCAN_OUT[15] of <device *dll @[sx_dll,sy_dll]>>,
        <pin GOUTEN of <device *iob @[sx,sy]>>               => <wire BUS_BSCAN_OUT[13] of <device *dll @[sx_dll,sy_dll]>>,
        <pin UPDATE_DR of <device *iob @[sx,sy]>>            => <wire BUS_BSCAN_OUT[6] of <device *dll @[sx_dll,sy_dll]>>,
        <pin SHIFT_DR of <device *iob @[sx,sy]>>             => <wire BUS_BSCAN_OUT[5] of <device *dll @[sx_dll,sy_dll]>>,
        <pin GWEN of <device *iob @[sx,sy]>>                 => <wire BUS_GSEB_OUT[1] of <device *dll @[sx_dll,sy_dll]>>,
        <pin POR_N of <device *iob @[sx,sy]>>                => <wire BUS_GSEB_OUT[2] of <device *dll @[sx_dll,sy_dll]>>,
        <pin WLSHFT_POR_ERASE_N of <device *iob @[sx,sy]>>   => <wire BUS_GSEB_OUT[2] of <device *dll @[sx_dll,sy_dll]>>,
        <pin REQ_CCS of <device *iob @[sx,sy]>>              => <wire BUS_GSEB_OUT[4] of <device *dll @[sx_dll,sy_dll]>>,
        <pin CLK_OSC of <device *iob @[sx,sy]>>              => <wire CLK_ADC_OUT of <device *dll @[sx_dll,sy_dll]>>
    );

}//end of function connect_iob_gs_input
