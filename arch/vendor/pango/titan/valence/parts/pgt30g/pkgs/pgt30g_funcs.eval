        <pin QUAD_0_REFCLK of <device PREGMUX_R @([PRE_GMUX_R_X*TILE_NUM_SITE_X+3, PRE_GMUX_R_Y*TILE_NUM_SITE_Y])>>  => <wire CLKDIVOUT of <device IOCLKDIV @[(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+1, IOCLKGEN_R_Y*TILE_NUM_SITE_Y]>> ,
        <pin QUAD_1_0_TX   of <device PREGMUX_R @([PRE_GMUX_R_X*TILE_NUM_SITE_X+3, PRE_GMUX_R_Y*TILE_NUM_SITE_Y])>>  => <wire CLKDIVOUT of <device IOCLKDIV @[(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+2, IOCLKGEN_R_Y*TILE_NUM_SITE_Y]>> ,
        <pin QUAD_2_0_RX   of <device PREGMUX_R @([PRE_GMUX_R_X*TILE_NUM_SITE_X+3, PRE_GMUX_R_Y*TILE_NUM_SITE_Y])>>  => <wire CLKDIVOUT of <device IOCLKDIV @[(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+3, IOCLKGEN_R_Y*TILE_NUM_SITE_Y]>>
        
        //from IOCLKDIV                                                                                                                                                                                 
        //<pin QUAD_0_0_RX of <device PREGMUX_L @([PRE_GMUX_R_X*TILE_NUM_SITE_X+3, PRE_GMUX_R_Y*TILE_NUM_SITE_Y])>>  => <wire CLKDIVOUT of <device IOCLKDIV @[IOCLKGEN_R_X*TILE_NUM_SITE_X+2, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+2]>>

    ); 

    // pre_gmux_t   FIXME
    connect
    (
        <pin PLLOUT3[0]    of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => 1'b1,
        <pin PLLOUT3[1]    of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => 1'b1,        
        <pin PLLOUT3[2]    of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => 1'b1,        
        <pin PLLOUT3[3]    of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => 1'b1,        

        <pin PLLOUT2[0]    of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => 1'b1,
        <pin PLLOUT2[1]    of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => 1'b1,
        <pin PLLOUT2[2]    of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => 1'b1,
        <pin PLLOUT2[3]    of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => 1'b1,
        
        <pin PLLOUT1       of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => <wire CLKOUT  of <device PLL @[PLL_TILE_X1*TILE_NUM_SITE_X+3, (PLL_TILE_Y1-4)*TILE_NUM_SITE_Y]>>, 
        <pin PLLOUT0       of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => <wire CLKOUT  of <device PLL @[PLL_TILE_X0*TILE_NUM_SITE_X+3, (PLL_TILE_Y0-4)*TILE_NUM_SITE_Y]>>, 

        <pin PCLK_1[0]     of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => <wire OUT of <device CLKDELAYMUX @[25*TILE_NUM_SITE_X+3, 81*TILE_NUM_SITE_Y]>>,
        <pin PCLK_1[1]     of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => 1'b1,  
        
        <pin PCLK_0[0]     of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => <wire OUT of <device CLKDELAYMUX @[22*TILE_NUM_SITE_X+3, 81*TILE_NUM_SITE_Y]>>,
        <pin PCLK_0[1]     of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => 1'b1,

        <pin SIG_FR_CIM[0] of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => <wire S_IRS  of <device CIM @[23*TILE_NUM_SITE_X+3, 81*TILE_NUM_SITE_Y+2]>>, 
        <pin SIG_FR_CIM[1] of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => <wire A_IBD  of <device CIM @[23*TILE_NUM_SITE_X+3, 81*TILE_NUM_SITE_Y+2]>>, 
        <pin SIG_FR_CIM[2] of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => <wire A_ICD  of <device CIM @[23*TILE_NUM_SITE_X+3, 81*TILE_NUM_SITE_Y+2]>>,
        
        //from IOCLKDIV                                                                                                                                                                                 
        <pin CLKDIV_TB[0] of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>  => <wire CLKDIVOUT of <device IOCLKDIV @[(IOCLKGEN_T_X+1)*TILE_NUM_SITE_X+0, IOCLKGEN_T_Y*TILE_NUM_SITE_Y]>>,
        <pin CLKDIV_TB[1] of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>  => <wire CLKDIVOUT of <device IOCLKDIV @[(IOCLKGEN_T_X+1)*TILE_NUM_SITE_X+1, IOCLKGEN_T_Y*TILE_NUM_SITE_Y]>>,
        <pin CLKDIV_TB[2] of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>  => <wire CLKDIVOUT of <device IOCLKDIV @[(IOCLKGEN_T_X+1)*TILE_NUM_SITE_X+2, IOCLKGEN_T_Y*TILE_NUM_SITE_Y]>>,
        <pin CLKDIV_TB[3] of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>  => <wire CLKDIVOUT of <device IOCLKDIV @[(IOCLKGEN_T_X+1)*TILE_NUM_SITE_X+3, IOCLKGEN_T_Y*TILE_NUM_SITE_Y]>>
        
      //<pin GLOGEN        of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => <wire GLOGEN  of <device CONFIG @[?*TILE_NUM_SITE_X+2, ?*TILE_NUM_SITE_Y]>>
    ); 

    // pre_gmux_b   FIXME
    connect
    (
        <pin PLLOUT3[0]    of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>     => 1'b1, 
        <pin PLLOUT3[1]    of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>     => 1'b1,         
        <pin PLLOUT3[2]    of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>     => 1'b1,         
        <pin PLLOUT3[3]    of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>     => 1'b1,         
         
        <pin PLLOUT2[0]    of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>     => 1'b1,
        <pin PLLOUT2[1]    of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>     => 1'b1,
        <pin PLLOUT2[2]    of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>     => 1'b1,
        <pin PLLOUT2[3]    of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>     => 1'b1,
                                                                                                                                       
        <pin PLLOUT1    of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>     => <wire CLKOUT  of <device PLL @[PLL_TILE_X3*TILE_NUM_SITE_X+3, (PLL_TILE_Y3+4)*TILE_NUM_SITE_Y]>>,                                                                                                              
        <pin PLLOUT0    of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>     => <wire CLKOUT  of <device PLL @[PLL_TILE_X2*TILE_NUM_SITE_X+3, (PLL_TILE_Y2+4)*TILE_NUM_SITE_Y]>>, 
                                                                                                                   
