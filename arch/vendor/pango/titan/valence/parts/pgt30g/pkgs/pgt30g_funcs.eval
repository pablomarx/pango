        <pin DIN of <device IOCLKGATE @([IOCLKGEN_R_X*TILE_NUM_SITE_X+1, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>  => <wire A_IM1 of <device CIM @[47*TILE_NUM_SITE_X+3, 38*TILE_NUM_SITE_Y+2]>> ,
        <pin DIN of <device IOCLKGATE @([IOCLKGEN_R_X*TILE_NUM_SITE_X+2, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>  => <wire A_IM2 of <device CIM @[47*TILE_NUM_SITE_X+3, 38*TILE_NUM_SITE_Y+2]>> ,
        <pin DIN of <device IOCLKGATE @([IOCLKGEN_R_X*TILE_NUM_SITE_X+3, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>  => <wire A_IM3 of <device CIM @[47*TILE_NUM_SITE_X+3, 38*TILE_NUM_SITE_Y+2]>> ,
   
        //IOCLKGATE4 - IOCLKGATE7   (BRIDGE GATE)                                                                                                                                                            
        <pin DIN of <device IOCLKGATE @([IOCLKGEN_R_X*TILE_NUM_SITE_X+0, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+2])>>  => <wire S_IM2 of <device CIM @[47*TILE_NUM_SITE_X+3, 40*TILE_NUM_SITE_Y+2]>> ,  
        <pin DIN of <device IOCLKGATE @([IOCLKGEN_R_X*TILE_NUM_SITE_X+1, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+2])>>  => <wire S_IM3 of <device CIM @[47*TILE_NUM_SITE_X+3, 40*TILE_NUM_SITE_Y+2]>> ,  
        <pin DIN of <device IOCLKGATE @([IOCLKGEN_R_X*TILE_NUM_SITE_X+2, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+2])>>  => <wire S_IM2 of <device CIM @[47*TILE_NUM_SITE_X+3, 39*TILE_NUM_SITE_Y+2]>> ,  
        <pin DIN of <device IOCLKGATE @([IOCLKGEN_R_X*TILE_NUM_SITE_X+3, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+2])>>  => <wire S_IM3 of <device CIM @[47*TILE_NUM_SITE_X+3, 39*TILE_NUM_SITE_Y+2]>> ,  

        //USCMMUX2TO1
        <pin IN0 of <device USCMMUX2TO1 @([(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+0, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>  => <wire BRGCLK_R_O[0] of <device IOCLK_BRIDGE @[IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y]>> ,
        <pin IN0 of <device USCMMUX2TO1 @([(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+1, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>  => <wire BRGCLK_R_O[1] of <device IOCLK_BRIDGE @[IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y]>> ,
        <pin IN0 of <device USCMMUX2TO1 @([(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+2, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>  => <wire BRGCLK_R_O[2] of <device IOCLK_BRIDGE @[IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y]>> ,
        <pin IN0 of <device USCMMUX2TO1 @([(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+3, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>  => <wire BRGCLK_R_O[3] of <device IOCLK_BRIDGE @[IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y]>> 
        
    );
    
    
    // IOCLK_BRIDGE
    connect
    (
        <pin SIG_FR_CIM[0] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire S_IA0 of <device CIM @[23*TILE_NUM_SITE_X+3, 40*TILE_NUM_SITE_Y+2]>> ,
        <pin SIG_FR_CIM[1] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire S_IB0 of <device CIM @[23*TILE_NUM_SITE_X+3, 40*TILE_NUM_SITE_Y+2]>> ,
        <pin SIG_FR_CIM[2] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire S_IDD of <device CIM @[23*TILE_NUM_SITE_X+3, 40*TILE_NUM_SITE_Y+2]>> ,
        <pin SIG_FR_CIM[3] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire A_IDD of <device CIM @[23*TILE_NUM_SITE_X+3, 40*TILE_NUM_SITE_Y+2]>> ,
        
        <pin BRGCLK_IN_T[0] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire OUT of <device IOCLKGATE @[IOCLKGEN_T_X*TILE_NUM_SITE_X+0, IOCLKGEN_T_Y*TILE_NUM_SITE_Y+2]>> ,
        <pin BRGCLK_IN_T[1] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire OUT of <device IOCLKGATE @[IOCLKGEN_T_X*TILE_NUM_SITE_X+1, IOCLKGEN_T_Y*TILE_NUM_SITE_Y+2]>> ,
        <pin BRGCLK_IN_T[2] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire OUT of <device IOCLKGATE @[IOCLKGEN_T_X*TILE_NUM_SITE_X+2, IOCLKGEN_T_Y*TILE_NUM_SITE_Y+2]>> ,
        <pin BRGCLK_IN_T[3] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire OUT of <device IOCLKGATE @[IOCLKGEN_T_X*TILE_NUM_SITE_X+3, IOCLKGEN_T_Y*TILE_NUM_SITE_Y+2]>> ,
       
        <pin BRGCLK_IN_B[0] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire OUT of <device IOCLKGATE @[IOCLKGEN_B_X*TILE_NUM_SITE_X+0, IOCLKGEN_B_Y*TILE_NUM_SITE_Y+2]>> ,
        <pin BRGCLK_IN_B[1] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire OUT of <device IOCLKGATE @[IOCLKGEN_B_X*TILE_NUM_SITE_X+1, IOCLKGEN_B_Y*TILE_NUM_SITE_Y+2]>> ,
        <pin BRGCLK_IN_B[2] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire OUT of <device IOCLKGATE @[IOCLKGEN_B_X*TILE_NUM_SITE_X+2, IOCLKGEN_B_Y*TILE_NUM_SITE_Y+2]>> ,
        <pin BRGCLK_IN_B[3] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire OUT of <device IOCLKGATE @[IOCLKGEN_B_X*TILE_NUM_SITE_X+3, IOCLKGEN_B_Y*TILE_NUM_SITE_Y+2]>> ,
        
        <pin BRGCLK_IN_R[0] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire OUT of <device IOCLKGATE @[IOCLKGEN_R_X*TILE_NUM_SITE_X+0, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+2]>> ,
        <pin BRGCLK_IN_R[1] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire OUT of <device IOCLKGATE @[IOCLKGEN_R_X*TILE_NUM_SITE_X+1, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+2]>> ,
        <pin BRGCLK_IN_R[2] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire OUT of <device IOCLKGATE @[IOCLKGEN_R_X*TILE_NUM_SITE_X+2, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+2]>> ,
        <pin BRGCLK_IN_R[3] of <device IOCLK_BRIDGE @([IO_CLK_BRIDGE_X*TILE_NUM_SITE_X, IO_CLK_BRIDGE_Y*TILE_NUM_SITE_Y])>>  => <wire OUT of <device IOCLKGATE @[IOCLKGEN_R_X*TILE_NUM_SITE_X+3, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+2]>> 
        
    );
    
};

/**VFunction******************************************************************************

  Author    [Zhang min]

  Abstract  [Connect the inputs of IO in the entire architecture]

  Revision History:

*****************************************************************************************/
function connect_io_input
(
    unsigned int IOCLKGEN_T_X ,
    unsigned int IOCLKGEN_T_Y , 
                 
    unsigned int IOCLKGEN_B_X ,
    unsigned int IOCLKGEN_B_Y ,
                  
    unsigned int IOCLKGEN_R_X ,
    unsigned int IOCLKGEN_R_Y ,
    
    unsigned int NUM_TILE_X ,
    unsigned int NUM_TILE_Y 
)
{
    unsigned int tx, ty;
    unsigned int uscm_mux_tx, uscm_mux_ty;
    
    //===== IOCLKGEN output to IOLs ( top and bottom ) =====//
    const unsigned int r_tile_x0[] = { [1:4, 6:13, 15:22, 25:32, 34:41, 43:46 +:4] };
    const unsigned int r_tile_y0[] = {0,81};

    foreach ty ( r_tile_y0 )
    {
        foreach tx ( r_tile_x0 )
        {
           if(ty == 0)
           {    
               uscm_mux_tx = IOCLKGEN_B_X;
               uscm_mux_ty = IOCLKGEN_B_Y;
           }
           else
           {
               uscm_mux_tx = IOCLKGEN_T_X;
               uscm_mux_ty = IOCLKGEN_T_Y;
           }
           
           connect
           (
                //TILE 0
                <pin IOCLK[0] of <device IOMUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+0, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[1] of <device IOMUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+1, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[2] of <device IOMUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+2, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[3] of <device IOMUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+3, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                
                <pin IOCLK[0] of <device IOMUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+0])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+0, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[1] of <device IOMUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+0])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+1, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[2] of <device IOMUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+0])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+2, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[3] of <device IOMUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+0])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+3, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                
                //TILE 1
                <pin IOCLK[0] of <device IOMUX @([(tx+1)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+0, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[1] of <device IOMUX @([(tx+1)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+1, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[2] of <device IOMUX @([(tx+1)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+2, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[3] of <device IOMUX @([(tx+1)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+3, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                
                <pin IOCLK[0] of <device IOMUX @([(tx+1)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+0])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+0, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[1] of <device IOMUX @([(tx+1)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+0])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+1, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[2] of <device IOMUX @([(tx+1)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+0])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+2, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[3] of <device IOMUX @([(tx+1)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+0])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+3, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                
                //TILE 3
                <pin IOCLK[0] of <device IOMUX @([(tx+3)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+0, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[1] of <device IOMUX @([(tx+3)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+1, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[2] of <device IOMUX @([(tx+3)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+2, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[3] of <device IOMUX @([(tx+3)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+3, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                
                <pin IOCLK[0] of <device IOMUX @([(tx+3)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+0])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+0, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[1] of <device IOMUX @([(tx+3)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+0])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+1, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[2] of <device IOMUX @([(tx+3)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+0])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+2, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[3] of <device IOMUX @([(tx+3)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+0])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+3, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> 
            );
                        
        }
    }
    
    //===== IOCLKGEN output to DQSs ( top and bottom ) =====//
    const unsigned int r_tile_x1[] = { 12, 21, 31, 40 };
    const unsigned int r_tile_y1[] = { 0, 81 };

    foreach ty ( r_tile_y1 )
    {
        if(ty == 0)
        {    
            uscm_mux_tx = IOCLKGEN_B_X;
            uscm_mux_ty = IOCLKGEN_B_Y;
        }
        else
        {
            uscm_mux_tx = IOCLKGEN_T_X;
            uscm_mux_ty = IOCLKGEN_T_Y;
        }                          
        
        foreach tx ( r_tile_x1 )
        {          
           connect
           (               
                <pin IOCLK[0] of <device IOMUX @([tx*TILE_NUM_SITE_X+2, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+0, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[1] of <device IOMUX @([tx*TILE_NUM_SITE_X+2, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+1, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[2] of <device IOMUX @([tx*TILE_NUM_SITE_X+2, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+2, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[3] of <device IOMUX @([tx*TILE_NUM_SITE_X+2, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+3, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                
                <pin IOCLK[0] of <device IOMUX @([tx*TILE_NUM_SITE_X+2, ty*TILE_NUM_SITE_Y+2])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+0, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[1] of <device IOMUX @([tx*TILE_NUM_SITE_X+2, ty*TILE_NUM_SITE_Y+2])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+1, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[2] of <device IOMUX @([tx*TILE_NUM_SITE_X+2, ty*TILE_NUM_SITE_Y+2])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+2, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[3] of <device IOMUX @([tx*TILE_NUM_SITE_X+2, ty*TILE_NUM_SITE_Y+2])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+3, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> 
            );
                        
        }
    }
    
    
    //======== IOCLKGEN output to IOLs ( right ) =========//
    const unsigned int r_tile_x2[] = { 47 };
    const unsigned int r_tile_y2[] = { [75:68, 65:62, 60:57, 54:47, 44:37, 34:27, 24:21, 19:16, 13:6 -:4] };
    
    uscm_mux_tx = IOCLKGEN_R_X;
    uscm_mux_ty = IOCLKGEN_R_Y;
            
    foreach tx ( r_tile_x2 )
    {
        foreach ty ( r_tile_y2 )
        {           
            connect
            (
                //TILE 0
                <pin IOCLK[0] of <device IOMUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+0, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
                <pin IOCLK[1] of <device IOMUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(uscm_mux_tx+1)*TILE_NUM_SITE_X+1, uscm_mux_ty*TILE_NUM_SITE_Y+1])>> ,
