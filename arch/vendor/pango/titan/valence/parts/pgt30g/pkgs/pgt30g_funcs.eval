        <pin SIG_FR_CIM[0] of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>  => <wire S_IRS  of <device CIM @[23*TILE_NUM_SITE_X+3, 0*TILE_NUM_SITE_Y+2]>>, 
        <pin SIG_FR_CIM[1] of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>  => <wire A_IBD  of <device CIM @[23*TILE_NUM_SITE_X+3, 0*TILE_NUM_SITE_Y+2]>>, 
        <pin SIG_FR_CIM[2] of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>  => <wire A_ICD  of <device CIM @[23*TILE_NUM_SITE_X+3, 0*TILE_NUM_SITE_Y+2]>>,
        
        //from IOCLKDIV                                                                                                                                                                                 
        <pin CLKDIV_TB[0] of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>  => <wire CLKDIVOUT of <device IOCLKDIV @[(IOCLKGEN_B_X+1)*TILE_NUM_SITE_X+0, IOCLKGEN_B_Y*TILE_NUM_SITE_Y]>>,
        <pin CLKDIV_TB[1] of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>  => <wire CLKDIVOUT of <device IOCLKDIV @[(IOCLKGEN_B_X+1)*TILE_NUM_SITE_X+1, IOCLKGEN_B_Y*TILE_NUM_SITE_Y]>>,
        <pin CLKDIV_TB[2] of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>  => <wire CLKDIVOUT of <device IOCLKDIV @[(IOCLKGEN_B_X+1)*TILE_NUM_SITE_X+2, IOCLKGEN_B_Y*TILE_NUM_SITE_Y]>>,
        <pin CLKDIV_TB[3] of <device PREGMUX_TB @([PRE_GMUX_B_X*TILE_NUM_SITE_X+3, PRE_GMUX_B_Y*TILE_NUM_SITE_Y])>>  => <wire CLKDIVOUT of <device IOCLKDIV @[(IOCLKGEN_B_X+1)*TILE_NUM_SITE_X+3, IOCLKGEN_B_Y*TILE_NUM_SITE_Y]>>
        
      //<pin GLOGEN of <device PREGMUX_TB @([PRE_GMUX_T_X*TILE_NUM_SITE_X+3, PRE_GMUX_T_Y*TILE_NUM_SITE_Y])>>   => <wire GLOGEN  of <device CONFIG @[?*TILE_NUM_SITE_X+2, ?*TILE_NUM_SITE_Y]>>
    ); 
    
    // post_gmux   FIXME
    connect
    (
        <pin CLKIN_L    of <device POST_GMUX @([POST_GMUX_X*TILE_NUM_SITE_X+3, POST_GMUX_Y*TILE_NUM_SITE_Y])>>   => <wire CLKIN  of <device PREGMUX_L @[PRE_GMUX_L_X*TILE_NUM_SITE_X+3, PRE_GMUX_L_Y*TILE_NUM_SITE_Y]>>,
        <pin CLKIN_R    of <device POST_GMUX @([POST_GMUX_X*TILE_NUM_SITE_X+3, POST_GMUX_Y*TILE_NUM_SITE_Y])>>   => <wire CLKIN  of <device PREGMUX_R @[PRE_GMUX_R_X*TILE_NUM_SITE_X+3, PRE_GMUX_R_Y*TILE_NUM_SITE_Y]>>,
