    (
        <pin CIM[0] of <device IOCLKMUX @([IOCLKGEN_T_X*TILE_NUM_SITE_X+1, IOCLKGEN_T_Y*TILE_NUM_SITE_Y])>>  => <wire A_IM2 of <device CIM @[21*TILE_NUM_SITE_X+3, 81*TILE_NUM_SITE_Y+2]>> ,
