               <pin DLL_CODE1 of <device DQS_MUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire DLL_CODE of <device DLL @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+3, 0*TILE_NUM_SITE_Y+1])>>             
            );  
        }
    }
    
    foreach ty ( r_tile_y4 )
    {
        foreach tx ( r_tile_x4 ) 
        {
            connect
            ( 
               <pin DLL_CODE2 of <device DQS_MUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>   => <wire DLL_CODE of <device DLL @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+3, (NUM_TILE_Y-1)*TILE_NUM_SITE_Y])>>   ,
               <pin DLL_CODE3 of <device DQS_MUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>   => <wire DLL_CODE of <device DLL @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+3, (NUM_TILE_Y-1)*TILE_NUM_SITE_Y+1])>> 
            );
        }
    }
    
    //======== DLL output to DQSs ( top & bottom ) =========//
    const unsigned int r_tile_x7[] = { 40, 31, 21, 12 };
    const unsigned int r_tile_y7[] = { 0, 81};
 
    foreach ty ( r_tile_y7 )
    {
        foreach tx ( r_tile_x7 ) 
        {
            connect
            ( 
                <pin DLL_CODE0 of <device DQS_MUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire DLL_CODE of <device DLL @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y])>>   ,
                <pin DLL_CODE1 of <device DQS_MUX @([tx*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  => <wire DLL_CODE of <device DLL @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y+1])>>  
            );            
        }
    }   
    

    
    //========= IOB output to IODELAY and IODELAYMUX ===========//
    connect
    (                                                                                  
        <pin DLYIN of <device CLKDELAY @[47*TILE_NUM_SITE_X+3, 62*TILE_NUM_SITE_Y+1]>>  => <wire DIN of <device IOB_A @[47*TILE_NUM_SITE_X+2, 62*TILE_NUM_SITE_Y+1]>>  ,  
        <pin DLYIN of <device CLKDELAY @[47*TILE_NUM_SITE_X+3, 60*TILE_NUM_SITE_Y+1]>>  => <wire DIN of <device IOB_LA @[47*TILE_NUM_SITE_X+2, 60*TILE_NUM_SITE_Y+1]>>  ,                          
        <pin DLYIN of <device CLKDELAY @[47*TILE_NUM_SITE_X+3, 21*TILE_NUM_SITE_Y+1]>>  => <wire DIN of <device IOB_A @[47*TILE_NUM_SITE_X+2, 21*TILE_NUM_SITE_Y+1]>>  , 
        <pin DLYIN of <device CLKDELAY @[47*TILE_NUM_SITE_X+3, 19*TILE_NUM_SITE_Y+1]>>  => <wire DIN of <device IOB_LA @[47*TILE_NUM_SITE_X+2, 19*TILE_NUM_SITE_Y+1]>>  ,

        <pin DLYIN of <device CLKDELAY @[25*TILE_NUM_SITE_X+3, 81*TILE_NUM_SITE_Y+1]>>   => <wire DIN of <device IOB_LA @[25*TILE_NUM_SITE_X+2, 81*TILE_NUM_SITE_Y+1]>> ,
        <pin DLYIN of <device CLKDELAY @[22*TILE_NUM_SITE_X+3, 81*TILE_NUM_SITE_Y+1]>>   => <wire DIN of <device IOB_A @[22*TILE_NUM_SITE_X+2, 81*TILE_NUM_SITE_Y+1]>> ,

        <pin DLYIN of <device CLKDELAY @[22*TILE_NUM_SITE_X+3, 0*TILE_NUM_SITE_Y+1]>>    => <wire DIN of <device IOB_A @[22*TILE_NUM_SITE_X+2, 0*TILE_NUM_SITE_Y+1]>>  ,                                                                       
        <pin DLYIN of <device CLKDELAY @[25*TILE_NUM_SITE_X+3, 0*TILE_NUM_SITE_Y+1]>>    => <wire DIN of <device IOB_LA @[25*TILE_NUM_SITE_X+2, 0*TILE_NUM_SITE_Y+1]>>  ,
        
        //                                                                              
        <pin IN1 of <device CLKDELAYMUX @[47*TILE_NUM_SITE_X+3, 62*TILE_NUM_SITE_Y]>>  => <wire DIN of <device IOB_A @[47*TILE_NUM_SITE_X+2, 62*TILE_NUM_SITE_Y+1]>>   ,  
        <pin IN1 of <device CLKDELAYMUX @[47*TILE_NUM_SITE_X+3, 60*TILE_NUM_SITE_Y]>>  => <wire DIN of <device IOB_LA @[47*TILE_NUM_SITE_X+2, 60*TILE_NUM_SITE_Y+1]>>   ,                          
        <pin IN1 of <device CLKDELAYMUX @[47*TILE_NUM_SITE_X+3, 21*TILE_NUM_SITE_Y]>>  => <wire DIN of <device IOB_A @[47*TILE_NUM_SITE_X+2, 21*TILE_NUM_SITE_Y+1]>>   , 
        <pin IN1 of <device CLKDELAYMUX @[47*TILE_NUM_SITE_X+3, 19*TILE_NUM_SITE_Y]>>  => <wire DIN of <device IOB_LA @[47*TILE_NUM_SITE_X+2, 19*TILE_NUM_SITE_Y+1]>>   ,

        <pin IN1 of <device CLKDELAYMUX @[25*TILE_NUM_SITE_X+3, 81*TILE_NUM_SITE_Y]>>  => <wire DIN of <device IOB_LA @[25*TILE_NUM_SITE_X+2, 81*TILE_NUM_SITE_Y+1]>>   ,
        <pin IN1 of <device CLKDELAYMUX @[22*TILE_NUM_SITE_X+3, 81*TILE_NUM_SITE_Y]>>  => <wire DIN of <device IOB_A @[22*TILE_NUM_SITE_X+2, 81*TILE_NUM_SITE_Y+1]>>   ,

        <pin IN1 of <device CLKDELAYMUX @[22*TILE_NUM_SITE_X+3, 0*TILE_NUM_SITE_Y]>>   => <wire DIN of <device IOB_A @[22*TILE_NUM_SITE_X+2, 0*TILE_NUM_SITE_Y+1]>>    ,                                                                       
        <pin IN1 of <device CLKDELAYMUX @[25*TILE_NUM_SITE_X+3, 0*TILE_NUM_SITE_Y]>>   => <wire DIN of <device IOB_LA @[25*TILE_NUM_SITE_X+2, 0*TILE_NUM_SITE_Y+1]>> 
        
    );   
    
    //========= IOB output to DQS(top and botom) ==========//
    const unsigned int r_tile_x9[] = { 12, 21, 31, 40 };
    const unsigned int r_tile_y9[] = { 0, 81 };
    
    unsigned int flag = 0;
    
    unsigned int dqs0 = 0;
    unsigned int dqs1 = 0;
    
    foreach ty ( r_tile_y9 )
    {
        flag = 0;
        
        foreach tx ( r_tile_x9 ) 
        {
            if(flag % 2 == 0)
            {
                dqs0 = tx;
            }
            else if(flag % 2 == 1)
            {
                dqs1 = tx;
                
                connect
                (
                    <pin DQSI[0] of <device DQS @([dqs0*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y])>>  => <wire DIN of <device IOB_A @([(dqs0+1)*TILE_NUM_SITE_X+2, ty*TILE_NUM_SITE_Y+1])>>   ,
                    <pin DQSI[1] of <device DQS @([dqs0*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y])>>  => <wire DIN of <device IOB_A @([(dqs1+1)*TILE_NUM_SITE_X+2, ty*TILE_NUM_SITE_Y+1])>>   ,
                    <pin DQSI[0] of <device DQS @([dqs1*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y])>>  => <wire DIN of <device IOB_A @([(dqs1+1)*TILE_NUM_SITE_X+2, ty*TILE_NUM_SITE_Y+1])>>   ,
                    <pin DQSI[1] of <device DQS @([dqs1*TILE_NUM_SITE_X+3, ty*TILE_NUM_SITE_Y])>>  => <wire DIN of <device IOB_A @([(dqs0+1)*TILE_NUM_SITE_X+2, ty*TILE_NUM_SITE_Y+1])>>   
                );  
            }
                                   
            flag = flag + 1;
        }
    } 
    
    //========= IOB output to DQS(right) ==========//
    const unsigned int r_tile_x10[] = { 47 };
    const unsigned int r_tile_y10[] = { 76, 66, 55, 45, 35, 25, 14, 4 };
    
    foreach tx ( r_tile_x10 )
    {
        flag = 0;
        
        foreach ty ( r_tile_y10 ) 
        {
            if(flag % 2 == 0)
            {
                dqs0 = ty;
            }
            else if(flag % 2 == 1)
            {
                dqs1 = ty;
                
                connect
                (
                    <pin DQSI[0] of <device DQS @([tx*TILE_NUM_SITE_X+3, dqs0*TILE_NUM_SITE_Y])>>  => <wire DIN of <device IOB_A  @([tx*TILE_NUM_SITE_X+2, (dqs0+1)*TILE_NUM_SITE_Y+1])>>   ,
                    <pin DQSI[1] of <device DQS @([tx*TILE_NUM_SITE_X+3, dqs0*TILE_NUM_SITE_Y])>>  => <wire DIN of <device IOB_LA @([tx*TILE_NUM_SITE_X+2, (dqs1+1)*TILE_NUM_SITE_Y+1])>>   ,
                    <pin DQSI[0] of <device DQS @([tx*TILE_NUM_SITE_X+3, dqs1*TILE_NUM_SITE_Y])>>  => <wire DIN of <device IOB_LA @([tx*TILE_NUM_SITE_X+2, (dqs1+1)*TILE_NUM_SITE_Y+1])>>   ,
                    <pin DQSI[1] of <device DQS @([tx*TILE_NUM_SITE_X+3, dqs1*TILE_NUM_SITE_Y])>>  => <wire DIN of <device IOB_A  @([tx*TILE_NUM_SITE_X+2, (dqs0+1)*TILE_NUM_SITE_Y+1])>>   
                );  
            }
                                   
            flag = flag + 1;
        }
    }
    
    //========== USCMMUX2TO1 to DLL ============//
    connect
    (
        <pin IOCLK[0] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, 0*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_B_X+1)*TILE_NUM_SITE_X+0, IOCLKGEN_B_Y*TILE_NUM_SITE_Y+1])>>   ,
        <pin IOCLK[1] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, 0*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_B_X+1)*TILE_NUM_SITE_X+1, IOCLKGEN_B_Y*TILE_NUM_SITE_Y+1])>>   ,
        <pin IOCLK[2] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, 0*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_B_X+1)*TILE_NUM_SITE_X+2, IOCLKGEN_B_Y*TILE_NUM_SITE_Y+1])>>   ,
        <pin IOCLK[3] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, 0*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_B_X+1)*TILE_NUM_SITE_X+3, IOCLKGEN_B_Y*TILE_NUM_SITE_Y+1])>>   ,
        
        <pin IOCLK[0] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, 0*TILE_NUM_SITE_Y+2])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+0, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>   ,
        <pin IOCLK[1] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, 0*TILE_NUM_SITE_Y+2])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+1, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>   ,
        <pin IOCLK[2] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, 0*TILE_NUM_SITE_Y+2])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+2, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>   ,
        <pin IOCLK[3] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, 0*TILE_NUM_SITE_Y+2])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+3, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>   
    );

    connect
    (
        <pin IOCLK[0] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, (NUM_TILE_Y-1)*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_T_X+1)*TILE_NUM_SITE_X+0, IOCLKGEN_T_Y*TILE_NUM_SITE_Y+1])>>   ,
        <pin IOCLK[1] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, (NUM_TILE_Y-1)*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_T_X+1)*TILE_NUM_SITE_X+1, IOCLKGEN_T_Y*TILE_NUM_SITE_Y+1])>>   ,
        <pin IOCLK[2] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, (NUM_TILE_Y-1)*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_T_X+1)*TILE_NUM_SITE_X+2, IOCLKGEN_T_Y*TILE_NUM_SITE_Y+1])>>   ,
        <pin IOCLK[3] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, (NUM_TILE_Y-1)*TILE_NUM_SITE_Y+1])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_T_X+1)*TILE_NUM_SITE_X+3, IOCLKGEN_T_Y*TILE_NUM_SITE_Y+1])>>   ,
        
        <pin IOCLK[0] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, (NUM_TILE_Y-1)*TILE_NUM_SITE_Y+2])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+0, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>   ,
        <pin IOCLK[1] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, (NUM_TILE_Y-1)*TILE_NUM_SITE_Y+2])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+1, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>   ,
        <pin IOCLK[2] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, (NUM_TILE_Y-1)*TILE_NUM_SITE_Y+2])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+2, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>   ,
        <pin IOCLK[3] of <device DLL_CLKMUX @([(NUM_TILE_X-1)*TILE_NUM_SITE_X+2, (NUM_TILE_Y-1)*TILE_NUM_SITE_Y+2])>>  => <wire OUT of <device USCMMUX2TO1 @([(IOCLKGEN_R_X+1)*TILE_NUM_SITE_X+3, IOCLKGEN_R_Y*TILE_NUM_SITE_Y+1])>>   
    );
};

/**VFunction******************************************************************************

  Author    [Zhang min]

  Abstract  [Connect the inputs of CLMA and CLMS in the entire architecture]

  Revision History:

*****************************************************************************************/
function connect_CLM_inputs
( 
    unsigned int MARGIN_TILE0_Y,
    unsigned int MARGIN_TILE1_Y,

    unsigned int X_TILES[],   // Entire 2-D array of X tiles
    unsigned int Y_TILES[]
)
{
    // Tile index variable
    unsigned int tx, ty;

    // Site index variable
    unsigned int sx, sy;
    
    string strVar0;          
    string strVar1;
    
    // Indexes of the tiles from which inputs are taken    
    int tile_n_1;
    int num_x = 1;
    
    foreach tx ( X_TILES )
    {
        if(num_x % 2 == 1)
        {
            sprintf(strVar0, "CLMA");
            sprintf(strVar1, "CLMA");
        }
        else
        {
            sprintf(strVar0, "CLMA");
            sprintf(strVar1, "CLMS");
        }
        
        num_x = num_x + 1;
        
        foreach ty ( Y_TILES )
        {
            tile_n_1 = (ty==MARGIN_TILE0_Y-1) || (ty==MARGIN_TILE1_Y-1)  ? ty+2 : ty+1;
            
            // The current tile
            sx = tx * TILE_NUM_SITE_X;
            sy = ty * TILE_NUM_SITE_Y;
            
            connect
            (
                <pin CIN     of <device *strVar0 @([sx+1,(tile_n_1*TILE_NUM_SITE_Y)+1])>>  =>  <wire COUT     of <device *strVar0 @[sx+1,sy+1]>> ,
                <pin CEIN    of <device *strVar0 @([sx+1,(tile_n_1*TILE_NUM_SITE_Y)+1])>>  =>  <wire CEOUT    of <device *strVar0 @[sx+1,sy+1]>> ,
                <pin RSIN    of <device *strVar0 @([sx+1,(tile_n_1*TILE_NUM_SITE_Y)+1])>>  =>  <wire RSOUT    of <device *strVar0 @[sx+1,sy+1]>> ,
                <pin SHIFTIN of <device *strVar0 @([sx+1,(tile_n_1*TILE_NUM_SITE_Y)+1])>>  =>  <wire SHIFTOUT of <device *strVar0 @[sx+1,sy+1]>> ,
                
                <pin CIN     of <device *strVar1 @([sx+2,(tile_n_1*TILE_NUM_SITE_Y)+1])>>  =>  <wire COUT     of <device *strVar1 @[sx+2,sy+1]>> ,
                <pin CEIN    of <device *strVar1 @([sx+2,(tile_n_1*TILE_NUM_SITE_Y)+1])>>  =>  <wire CEOUT    of <device *strVar1 @[sx+2,sy+1]>> ,
                <pin RSIN    of <device *strVar1 @([sx+2,(tile_n_1*TILE_NUM_SITE_Y)+1])>>  =>  <wire RSOUT    of <device *strVar1 @[sx+2,sy+1]>> ,
                <pin SHIFTIN of <device *strVar1 @([sx+2,(tile_n_1*TILE_NUM_SITE_Y)+1])>>  =>  <wire SHIFTOUT of <device *strVar1 @[sx+2,sy+1]>> ,
                
                <pin L7IN of <device *strVar1 @([sx+2,sy+1])>>  =>  <wire L7OUT of <device *strVar0 @[sx+1, (tile_n_1*TILE_NUM_SITE_Y)+1]>>           
            );
        }
    }
    
    
};

/**VFunction******************************************************************************

  Author    [Zhang min]

  Abstract  [Connect the inputs of SRB in the entire architecture]

  Revision History:

*****************************************************************************************/
function connect_SRB_inputs
( 
    unsigned int N_SRB,    // The IOB tile X/Y tile index
    unsigned int S_SRB,
    unsigned int W_SRB,
    unsigned int E_SRB,

    unsigned int MARGIN_TILE0_Y,
    unsigned int MARGIN_TILE1_Y,

    unsigned int X_TILES[],   // Entire 2-D array of X tiles
    unsigned int Y_TILES[],

    unsigned int CENTRAL_TILE_X,   // The central tile index
    
    unsigned int EMPTY_COLUMN_X     
)
{
    // Tile index variable
    unsigned int tx, ty;

    // Site index variable
    unsigned int sx, sy;
    
    string strVar0;          
    string strVar1;
    
    // Indexes of the tiles from which inputs are taken
    int tile_n_1, tile_n_2, tile_n_4, tile_n_8, tile_n_16; // North
    int tile_s_1, tile_s_2, tile_s_4, tile_s_8, tile_s_16; // South
    int tile_w_1, tile_w_2, tile_w_4, tile_w_8, tile_w_16; // West
    int tile_e_1, tile_e_2, tile_e_4, tile_e_8, tile_e_16; // East
    
    foreach tx ( X_TILES )
    {
        // Calculate the west/east source tile index. Adjust the X index for the sake of
        // central tile gap
        tile_w_1 = (tx==EMPTY_COLUMN_X+1) ? tx-2 : tx-1;
        tile_w_2 = (tx==EMPTY_COLUMN_X+1 ||
                    tx==EMPTY_COLUMN_X+2) ? tx-3 : tx-2;
        tile_w_4 = (tx==EMPTY_COLUMN_X+1 ||
                    tx==EMPTY_COLUMN_X+2 ||
                    tx==EMPTY_COLUMN_X+3 ||
                    tx==EMPTY_COLUMN_X+4) ? tx-5 : tx-4;
                    
        tile_w_8 = (tx>=EMPTY_COLUMN_X+1 &&
                    tx<=EMPTY_COLUMN_X+8) ? tx-9 : tx-8;
                    
        tile_w_16 = (tx>=EMPTY_COLUMN_X+1 &&
                     tx<=EMPTY_COLUMN_X+16) ? tx-17 : tx-16;

        tile_e_1 = (tx==EMPTY_COLUMN_X-1) ? tx+2 : tx+1;
        tile_e_2 = (tx==EMPTY_COLUMN_X-1 ||
                    tx==EMPTY_COLUMN_X-2) ? tx+3 : tx+2;
        tile_e_4 = (tx==EMPTY_COLUMN_X-1 ||
                    tx==EMPTY_COLUMN_X-2 ||
                    tx==EMPTY_COLUMN_X-3 ||
                    tx==EMPTY_COLUMN_X-4) ? tx+5 : tx+4;
                    
        tile_e_8 = (tx<=EMPTY_COLUMN_X-1 &&
                    tx>=EMPTY_COLUMN_X-8) ? tx+9 : tx+8;
                    
        tile_e_16 = (tx<=EMPTY_COLUMN_X-1 &&
                     tx>=EMPTY_COLUMN_X-16) ? tx+17 : tx+16;
        
        foreach ty ( Y_TILES )
        {
            
            
            // The current tile
            sx = tx * TILE_NUM_SITE_X;
            sy = ty * TILE_NUM_SITE_Y;



            // Calculate the north/south source tile index
            tile_s_1 = (ty==MARGIN_TILE0_Y+1) ||
                       (ty==MARGIN_TILE1_Y+1)  ? ty-2 : ty-1;
            tile_s_2 = (ty==MARGIN_TILE0_Y+1) ||
                       (ty==MARGIN_TILE1_Y+1) ||
                       (ty==MARGIN_TILE0_Y+2) ||
                       (ty==MARGIN_TILE1_Y+2)  ? ty-3 : ty-2;
            tile_s_4 = (ty==MARGIN_TILE0_Y+1) ||
                       (ty==MARGIN_TILE1_Y+1) ||
                       (ty==MARGIN_TILE0_Y+2) ||
                       (ty==MARGIN_TILE1_Y+2) ||
                       (ty==MARGIN_TILE0_Y+3) ||
                       (ty==MARGIN_TILE1_Y+3) ||
                       (ty==MARGIN_TILE0_Y+4) ||
                       (ty==MARGIN_TILE1_Y+4)  ? ty-5 : ty-4;
                       
            tile_s_8 = ((ty>=MARGIN_TILE0_Y+1 && ty<=MARGIN_TILE0_Y+8) ||
                        (ty>=MARGIN_TILE1_Y+1 && ty<=MARGIN_TILE1_Y+8)) ? ty-9 : ty-8;
                        
            tile_s_16 = ((ty>=MARGIN_TILE0_Y+1 && ty<=MARGIN_TILE0_Y+16) ||
