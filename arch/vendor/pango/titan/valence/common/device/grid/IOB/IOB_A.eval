*****************************************************************

  Device    [IOB_A]

  Author    [hzhang]

  Abstract  [IOB_A(TRUE PAD) is a grid device of the IOBs which are placed on the
            TOP/RIGHT/BOTTOM of Arch.]

  Revision History:

********************************************************************************/
grid
device IOB_A
{ 
    property
    (
        //DONT_USE means this iob unbond, and EMPTY means the layout dont have this iob ;NAME will assign PAD name later
        bool   DONT_USE = false,
        bool   EMPTY = false,
        string PAD_NAME = "",
        string PIN_NAME = "",
        string LOCATION = "",
        string HARDWARE_NAME = "",
        string IO_TYPE  = "",
        bool   SHARED_IO = false,
        bool   CLK_PAD   = false
    );

    parameter
    (
        config string IOB_MODE   = "UNUSED",    // "IBUF", "OBUF", "IOBUF", "OBUFT", "IBUFDS", "OBUFDS", "IOBUFDS"
        config string TERMINATED = "PULLUP",    /* "UNUSED" , "PULLUP" , "PULLDOWN" , "KEEPER" */
        config string HOT_SOCKET = "ON",        // ON; OFF
        config string POWER_MODE = "ON",        // ON; OFF; ON--> POWER GUARD
        config string VCCIO      = "3.3",       // "3.3" "2.5" "1.8" "1.5" "1.2"
        
        config string OPENDRAIN  = "OFF",       // OFF: NA; ON: OD
        // ibuf
        config string HYS_DRIVE_MODE   = "NOHYS" ,    // "NOHYS","SMHYS_I","SMHYS_II","LGHYS","OD","UD" 

        config string VREF_MODE  = "IN"    ,    // "IN": VREF_IN; "OUT": VREF_OUT
        config string VREF_IN_MODE  = "OFF",    // "OFF" "0.45" "0.50" "0.55"
        
        config string DDR_TERM_MODE = "ON",     // "TERM": "ON" "OFF" 
        
        config string VREF_OUT_MODE = "VREF1",  // "VREF1" "VREF2" "VREF3" "EXT_DRV" 
        
        config string DIFF_IN_TERM_MODE = "OFF", // "ON" "OFF" 
        
        // obuf
        config string SLEW_MODE         = "F",  // "F" "S"
        config string DRV_CURR_EN       = "TRUE",  // "TRUE" "FALSE" 
        config string DRV_CURR_VAL      = "0",  /*  LVCMOS33 : 4/8/12/16/20/24         (Driver Current) 
                                                    LVCMOS25 : 4/8/12/16/20            (Driver Current)
                                                    LVCMOS18 : 4/8/12/16               (Driver Current)
                                                    LVCMOS15 : 4/8/12                  (Driver Current)
                                                    LVCMOS12 : 2/6                     (Driver Current)
                                                    PCI33    : 1.5                        
                                                    LVTTL33  : 4/8/12/16/20/24         (Driver Current)
                                                    SSTL25_I : 8.1                     (Driver Current) 
                                                    SSTL25_II: 16.2                    (Driver Current)
                                                    SSTL18_I : 6.7                     (Driver Current)
                                                    SSTL18_II: 13.4                    (Driver Current)
                                                    SSTL15_I : 7.5                     (Driver Current)
                                                    SSTL15_II: 8.8                     (Driver Current)
                                                    HSTL18_I : 8                       (Driver Current)
                                                    HSTL18_II: 16                      (Driver Current)
                                                    HSTL15_I : 8                       (Driver Current)
                                                    LVDS     : 1.25/2.0/2.5/3.5/4.0/4.5(Driver Current)
                                                    RSDS     : 2.0                     (Driver Current)
                                                    PPDS     : 2.0                     (Driver Current)
                                                    DEFAULT  : 0                                       */
        
        // modify for bug-7123
        config string PMOS_FINGER       = "1", /*   SSTL25_I : 1/2/3/4/5/6             (Driver Current) 
                                                    SSTL25_II: 1/2/3/4/5/6             (Driver Current) 
                                                    SSTL18_I : 1/2/3/4/5/6             (Driver Current) 
                                                    SSTL18_II: 1/2/3/4/5/6             (Driver Current) 
                                                    SSTL15_I : 1/2/3/4/5/6             (Driver Current) 
                                                    SSTL15_II: 1/2/3/4/5/6             (Driver Current) 
                                                    HSTL18_I : 1/2/3/4/5/6             (Driver Current) 
                                                    HSTL18_II: 1/2/3/4/5/6             (Driver Current) 
                                                    HSTL15_I : 1/2/3/4/5/6             (Driver Current) 
                                                    DEFAULT  : 1                                       */
                                                    
        config string NMOS_FINGER       = "1", /*   SSTL25_I : 1/2/3/4/5/6             (Driver Current) 
                                                    SSTL25_II: 1/2/3/4/5/6             (Driver Current) 
                                                    SSTL18_I : 1/2/3/4/5/6             (Driver Current) 
                                                    SSTL18_II: 1/2/3/4/5/6             (Driver Current) 
                                                    SSTL15_I : 1/2/3/4/5/6             (Driver Current) 
                                                    SSTL15_II: 1/2/3/4/5/6             (Driver Current) 
                                                    HSTL18_I : 1/2/3/4/5/6             (Driver Current) 
                                                    HSTL18_II: 1/2/3/4/5/6             (Driver Current) 
                                                    HSTL15_I : 1/2/3/4/5/6             (Driver Current) 
                                                    DEFAULT  : 1                                       */                                            
        
        config string IOSTANDARD = "DEFAULT" 
        /*
        ibuf:
            LVCMOS33, LVCMOS25, LVCMOS18, LVCMOS15, LVCMOS12, LVTTL33, PCI33: NOHYS/SMHYS_I/SMHYS_II/LGHYS/ODXX/UDXX
            SSTL25_I/II,   SSTL18_I/II,   SSTL15_I/II,HSTL18_I/II, HSTL15_I/II: OFF/0.45/0.50/0.55(VREF_IN);VREF1/VREF2/VREF3/EXT_DRV/TERM (VREF_OUT)
            LVDS: NOTERM/TERM
            VREF: The STANDARD is for referent pin(IOB_A), and this time only set VREF_OUT_MODE = "EXT_DRV", others set val is 'b0 
            HSTL15D  : HSTL15        ibuf
            HSTL18D  : HSTL18        ibuf
            SSTL15D  : SSTL15        ibuf
            SSTL18D  : SSTL18        ibuf
            SSTL25D  : SSTL25        ibuf  
        obuf:
            LVCMOS33 : F/S(SLEW); 4/8/12/16/20/24         (Driver Current) 
            LVCMOS25 : F/S(SLEW); 4/8/12/16/20            (Driver Current)
            LVCMOS18 : F/S(SLEW); 4/8/12/16               (Driver Current)
            LVCMOS15 : F/S(SLEW); 4/8/12                  (Driver Current)
            LVCMOS12 : F/S(SLEW); 2/6                     (Driver Current)
            PCI33    : F/S(SLEW)                          
            LVTTL33  : F/S(SLEW); 4/8/12/16/20/24         (Driver Current)
            SSTL25_I : F/S(SLEW); 8.1                     (Driver Current) 
            SSTL25_II: F/S(SLEW); 16.2                    (Driver Current)
            SSTL18_I : F/S(SLEW); 6.7                     (Driver Current)
            SSTL18_II: F/S(SLEW); 13.4                    (Driver Current)
            SSTL15_I : F/S(SLEW); 7.5                     (Driver Current)
            SSTL15_II: F/S(SLEW); 8.8                     (Driver Current)
            HSTL18_I : F/S(SLEW); 8                       (Driver Current)
            HSTL18_II: F/S(SLEW); 16                      (Driver Current)
            HSTL15_I : F/S(SLEW); 8                       (Driver Current)
            LVDS     : ---------  1.25/2.0/2.5/3.5/4.0/4.5(Driver Current)
            RSDS     : ---------  2.0                     (Driver Current)
            PPDS     : ---------  2.0                     (Driver Current)
            LVDS25E / RSDS25E / PPDS25E : LVCMOS25         obuf
            LVPECL33E                   : LVCMOS33      obuf
            HSTL15D_I                   : HSTL15_I      obuf
            HSTL18D_I                   : HSTL18_I      obuf
            HSTL18D_II                  : HSTL18_II     obuf
            SSTL15D_I                   : SSTL15_I      obuf
            SSTL15D_II                  : SSTL15_II     obuf
            SSTL18D_I                   : SSTL18_I      obuf
            SSTL18D_II                  : SSTL18_II     obuf
            SSTL25D_I                   : SSTL25_I      obuf
            SSTL25D_II                  : SSTL25_II     obuf 
        iobuf:    
        */ 
        
                      
    );
    port
    (
        // These are the config ports.

        config input SC_DIFF_IN_EN      = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_ENINR" */,
        config input SC_DIFFO_OUT       = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_ENLVDS" */,
        config input SC_BUS_KEEPER[1:0] = 2'b00  /*pragma PAP_CFG_BIT_NAME = "SC_PU, SC_PD_N" */,
        config input SC_HOT_SOCKET      = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_CLAMP" */,
        config input SC_POWER_MODE      = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_PWRGRD_E" */,
        config input SC_3325_N          = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_3325_N" */,
        config input SC_DYNTERM         = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_DYNTERM" */, 
        
        // single_end_in
        config input SC_ENINS_N    = 1'b0  /*pragma PAP_CFG_BIT_NAME = "SC_ENINS_N" */,                                                
        config input SC_RD         = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_RD" */,      // resistance of input enable       
        config input SC_INSBT2     = 1'b0  /*pragma PAP_CFG_BIT_NAME = "SC_INSBT2 " */,
        config input SC_INSBT1     = 1'b0  /*pragma PAP_CFG_BIT_NAME = "SC_INSBT1  " */,
        config input SC_H2L        = 1'b0  /*pragma PAP_CFG_BIT_NAME = "SC_H2L    " */,
        
        // single_end_out 
        config input SC_PG[2:0]   = 3'b000  /*pragma PAP_CFG_BIT_NAME = "SC_PG1, SC_PG2_N ,SC_PG3 " */ ,
        config input SC_NG[2:0]   = 3'b000  /*pragma PAP_CFG_BIT_NAME = "SC_NG1, SC_NG2_N ,SC_NG3 " */, 
        config input SC_SLEW_N    = 1'b0    /*pragma PAP_CFG_BIT_NAME = "SC_SLEW_N  " */,  
        
        // BANK0~5
        config input SC_PU2_B                 = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_PU2_B"*/,
        config input SC_PU3_B                 = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_PU3_B"*/,
        config input SC_PD2                   = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_PD2"*/,
        config input SC_PD3                   = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_PD3"*/,
        config input SC_STDBY_INR             = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_STDBY_INR"*/,
        config input SC_INR_ON                = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_INR_ON"*/,
        config input SC_VRE[1:0]              = 2'b00  /*pragma PAP_CFG_BIT_NAME = "SC_VRE<1>, SC_VRE<0>"*/,
        config input SC_VREFPD_N              = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_VREFPD_N"*/,
        config input SC_DIFFMODE_SEL[1:0]     = 2'b00  /*pragma PAP_CFG_BIT_NAME = "SC_DIFFMODE_SEL<1>, SC_DIFFMODE_SEL<0>"*/,
        config input SC_DRV[2:0]              = 3'b000 /*pragma PAP_CFG_BIT_NAME = "SC_DRV<2>, SC_DRV<1>, SC_DRV<0>"*/,
        config input SC_RCON_EN               = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_RCON_EN"*/,
        config input SC_LVDS_ON               = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_LVDS_ON"*/,
        config input SC_STDBY_LVDS            = 1'b0   /*pragma PAP_CFG_BIT_NAME = "SC_STDBY_LVDS"*/,
        config input SC_HSPD_CTRL[1:0]        = 2'b00  /*pragma PAP_CFG_BIT_NAME = "SC_HSPD_CTRL<1>, SC_HSPD_CTRL<0>"*/,
        
        inout  PAD,
        input  TO  = 1'b1,
        input  DO  = 1'b1,
        output DIN,

        input  DIFFI_IN = 1'b1, 
        output DIFFI_OUT,
        output DIFFO_OUT
    );

}// end of device IOB_A

/*******************************************************************************

  Device    [IOB_A]

  Author    []

  Abstract  [The structure netlist of IOB_A is described in the similar fashion
             as in HDL. In unit instantiation statement, the formal pin may be
             connected to net which is declared explicitly.

             In Valence, the connection can also be made from formal pin to other
             pin or port, in which case Valence compiler shall create the net to
             fulfil the connection. The built-in naming convention is kicked in
             to name the automatically created net. However, the objects (net, instance)
             of this netlist are going to mapped from schematic. The compiler-generated
             names cause trouble for the mapping. Therefore, all wires are declared
             explicitly in the following description]

  Revision History:

********************************************************************************/
structure netlist of IOB_A
{
    // Wires for input ports
    wire ntTO, ntDO, ntDIFFI_IN;
    ntTO         <= TO;
    ntDO         <= DO;
    ntDIFFI_IN   <= DIFFI_IN;

   // Wires connecting to output ports

    wire ntPAD;
    wire ntINBUF;
    wire ntDIFFO_OUT;

    PAD         <= ntPAD;
    DIFFI_OUT   <= ntPAD;
    DIN         <= ntINBUF;
    DIFFO_OUT   <= ntDIFFO_OUT;

    device IOB_OBUFA OBUFA
        parameter map
        (
            IOB_MODE     => IOB_MODE,
            SLEW_MODE    => SLEW_MODE,
            DRV_CURR_VAL => DRV_CURR_VAL,
            IOSTANDARD   => IOSTANDARD
        )
        port map
        (
            TS           => ntTO,
            IN           => ntDO,
            OUT          => ntPAD,
            DIFFO_OUT    => ntDIFFO_OUT
        );

    device IOB_INBUFA INBUFA
        parameter map
        (
            IOB_MODE          => IOB_MODE,
            DDR_TERM_MODE     => DDR_TERM_MODE,
            DIFF_IN_TERM_MODE => DIFF_IN_TERM_MODE,
            IOSTANDARD        => IOSTANDARD
        )
        port map
        (
            IN          => ntPAD,
            DIFFI_IN    => ntDIFFI_IN,
            O           => ntINBUF
        );

    def_disable_timing <pin OUT of <device OBUFA>> -> <pin IN of <device INBUFA>>;

}; // end of structure netlist of IOB_A

/*******************************************************************************

  Device    [IOB_A]

  Author    []

  Abstract  [The floorplan structure]

  Revision History:

********************************************************************************/
floorplan
structure fp_struct of IOB_A
{
    device IOB_INBUFA  INBUFA ;
    device IOB_OBUFA   OBUFA ; 

}; // end of structure fp_struct of IOB_A



/*******************************************************************************

  Device    [IOB_A]

  Author    []

  Abstract  [Configure the configuration bits based on the value of config parameter]

  Revision History:

********************************************************************************/
configuration cfg of IOB_A
{

    // IO CONFIG PORT SETTING
    if (TERMINATED == "PULLUP")
    {
        SC_BUS_KEEPER := 2'b00;
    }
    else if (TERMINATED == "PULLDOWN")
    {
        SC_BUS_KEEPER := 2'b11;
    }
    else if (TERMINATED == "KEEPER")
    {
        SC_BUS_KEEPER := 2'b10;
    }
    else if (TERMINATED == "UNUSED")
    {
        SC_BUS_KEEPER := 2'b01;
    }

    if (HOT_SOCKET == "ON")
    {
        SC_HOT_SOCKET := 1'b0;
    }
    else // if (HOT_SOCKET == "OFF")
    {
        SC_HOT_SOCKET := 1'b1; 
    }
    
    if (POWER_MODE == "ON")
    {
        SC_POWER_MODE := 1'b0;
    }
    else // if (POWER_MODE == "OFF")
    {
        SC_POWER_MODE := 1'b1;
    }
    
    if (IOB_MODE == "IBUF" || IOB_MODE == "IBUFDS")
    {
        if ( IOSTANDARD == "LVCMOS33" || IOSTANDARD == "DEFAULT" || IOSTANDARD == "LVCMOS25" 
           || IOSTANDARD == "LVCMOS18" || IOSTANDARD == "LVCMOS15" 
           || IOSTANDARD == "LVCMOS12" || IOSTANDARD == "LVTTL33" 
           || IOSTANDARD == "PCI33")
        {
            if (HYS_DRIVE_MODE == "NOHYS")
            {
                SC_ENINS_N := 1'b0  ;
                SC_RD      := 1'b0  ;
                SC_INSBT2  := 1'b0  ;
                SC_INSBT1  := 1'b0  ;
                SC_H2L     := 1'b0  ;
            }
            else if (HYS_DRIVE_MODE == "SMHYS_I")
            {
                SC_ENINS_N := 1'b0  ;
                SC_RD      := 1'b1  ;
                SC_INSBT2  := 1'b0  ;
                SC_INSBT1  := 1'b0  ;
                SC_H2L     := 1'b0  ;    
            }
            else if (HYS_DRIVE_MODE == "SMHYS_II")
            {
                SC_ENINS_N := 1'b0  ;
                SC_RD      := 1'b0  ;
                SC_INSBT2  := 1'b0  ;
                SC_INSBT1  := 1'b0  ;
                SC_H2L     := 1'b1  ;    
            }
            else if (HYS_DRIVE_MODE == "LGHYS")
            {
                SC_ENINS_N := 1'b0  ;
                SC_RD      := 1'b1  ;
                SC_INSBT2  := 1'b0  ;
                SC_INSBT1  := 1'b0  ;
                SC_H2L     := 1'b1  ;    
            }
            else if (!strncmp(HYS_DRIVE_MODE,"OD",2))
            {
                SC_ENINS_N := 1'b0  ;
                SC_RD      := 1'b0  ;
                SC_INSBT2  := 1'b1  ;
                SC_INSBT1  := 1'b1  ;
                SC_H2L     := 1'b0  ;    
            }
            else // if (HYS_DRIVE_MODE == "UD")
            {
                SC_ENINS_N := 1'b0  ;
                SC_RD      := 1'b1  ;
                SC_INSBT2  := 1'b1  ;
                SC_INSBT1  := 1'b1  ;
                SC_H2L     := 1'b0  ;    
            }    
        }
        else if ( IOSTANDARD == "SSTL25_I"  || IOSTANDARD == "SSTL25_II" 
                || IOSTANDARD == "SSTL18_I" || IOSTANDARD == "SSTL18_II"
                || IOSTANDARD == "SSTL15_I" ||  IOSTANDARD == "SSTL15_II"
                || IOSTANDARD == "HSTL18_I" || IOSTANDARD == "HSTL18_II"
                || IOSTANDARD == "HSTL15_I" || IOSTANDARD == "HSTL15_II"

                || IOSTANDARD == "HSTL15D"    || IOSTANDARD == "HSTL18D"
                || IOSTANDARD == "SSTL15D"  || IOSTANDARD == "SSTL18D"
                || IOSTANDARD == "SSTL25D")
        {                  
            if (VREF_MODE == "OUT" )
            {        
                if (VREF_OUT_MODE == "VREF1")
                {
                    SC_ENINS_N := 1'b1  ;  
                    SC_RD      := 1'b0  ;  
                    SC_INSBT2  := 1'b0  ;  
                    SC_INSBT1  := 1'b0  ;  
                    SC_H2L     := 1'b1  ; 
                                    
                    if (DDR_TERM_MODE == "ON")
                    {
                        SC_DYNTERM := 1'b1  ;
                    }
                    else //if (DDR_TERM_MODE == "OFF")
                    {
                        SC_DYNTERM := 1'b0  ;
                    }
                }
                else if (VREF_OUT_MODE == "VREF2")
                {
                    SC_ENINS_N := 1'b1  ;
                    SC_RD      := 1'b0  ;
                    SC_INSBT2  := 1'b0  ;
                    SC_INSBT1  := 1'b1  ;
                    SC_H2L     := 1'b0  ;
                    
                    if (DDR_TERM_MODE == "ON")
                    {
                        SC_DYNTERM := 1'b1  ;
                    }
                    else //if (DDR_TERM_MODE == "OFF")
                    {
                        SC_DYNTERM := 1'b0  ;
                    }             
                }
                else if (VREF_OUT_MODE == "VREF3")
                {
                    SC_ENINS_N := 1'b1  ;  
                    SC_RD      := 1'b0  ;  
                    SC_INSBT2  := 1'b0  ;  
                    SC_INSBT1  := 1'b1  ;  
                    SC_H2L     := 1'b1  ; 
                    
                    if (DDR_TERM_MODE == "ON")
                    {
                        SC_DYNTERM := 1'b1  ;
                    }
                    else //if (DDR_TERM_MODE == "OFF")
                    {
                        SC_DYNTERM := 1'b0  ;
                    } 
                }
                else if (VREF_OUT_MODE == "EXT_DRV")
                {
                    SC_ENINS_N := 1'b1  ;
                    SC_RD      := 1'b0  ;
                    SC_INSBT2  := 1'b1  ;
                    SC_INSBT1  := 1'b0  ;
                    SC_H2L     := 1'b1  ;
                    SC_DYNTERM := 1'b0  ;     
                }
            }
            else
            {
                if (VREF_OUT_MODE == "VREF1")
                {
                    SC_ENINS_N := 1'b1  ;  
                    SC_RD      := 1'b0  ;  
                    SC_INSBT2  := 1'b0  ;  
                    SC_INSBT1  := 1'b0  ;  
                    SC_H2L     := 1'b1  ; 
                    SC_DYNTERM := 1'b0  ;
                }
                else if (VREF_OUT_MODE == "VREF2")
                {
                    SC_ENINS_N := 1'b1  ;
                    SC_RD      := 1'b0  ;
                    SC_INSBT2  := 1'b0  ;
                    SC_INSBT1  := 1'b1  ;
                    SC_H2L     := 1'b0  ;
                    SC_DYNTERM := 1'b0  ;             
                }
                else if (VREF_OUT_MODE == "VREF3")
                {
                    SC_ENINS_N := 1'b1  ;  
                    SC_RD      := 1'b0  ;  
                    SC_INSBT2  := 1'b0  ;  
                    SC_INSBT1  := 1'b1  ;  
                    SC_H2L     := 1'b1  ; 
                    SC_DYNTERM := 1'b0  ;
                }
                else
                {
                    SC_ENINS_N := 1'b1  ;  
                    SC_RD      := 1'b0  ;  
                    SC_INSBT2  := 1'b0  ;  
                    SC_INSBT1  := 1'b0  ;  
                    SC_H2L     := 1'b1  ;
                    SC_DYNTERM := 1'b0  ;
                }           
            }
        }
        else  if ( IOSTANDARD == "LVDS" )
        {
            if (DIFF_IN_TERM_MODE == "ON")
            {
                SC_ENINS_N := 1'b0  ;
                SC_RD      := 1'b1  ;
                SC_INSBT2  := 1'b0  ;
                SC_INSBT1  := 1'b0  ;
                SC_H2L     := 1'b0  ;
                SC_DYNTERM := 1'b0  ;      
            }
            else // if (DIFF_IN_TERM_MODE == "OFF")
            {
                SC_ENINS_N := 1'b0  ;
                SC_RD      := 1'b0  ;
                SC_INSBT2  := 1'b0  ;
                SC_INSBT1  := 1'b0  ;
                SC_H2L     := 1'b0  ;
                SC_DYNTERM := 1'b0  ;      
            }
        }
        else if (IOSTANDARD == "VREF")
        {
            SC_ENINS_N := 1'b1  ;
            SC_RD      := 1'b0  ;
            SC_INSBT2  := 1'b1  ;
            SC_INSBT1  := 1'b0  ;
            SC_H2L     := 1'b1  ;
            SC_DYNTERM := 1'b0  ;             
        }
        SC_PG := 3'b010;
        SC_NG := 3'b010;

    }
    else if (IOB_MODE == "OBUF" || IOB_MODE == "OBUFDS" || IOB_MODE == "OBUFT" )
    {   
        if ( IOSTANDARD == "LVCMOS33" || IOSTANDARD == "DEFAULT"
          || IOSTANDARD == "LVCMOS25"
          || IOSTANDARD == "LVCMOS18"
          || IOSTANDARD == "LVCMOS15"
          || IOSTANDARD == "LVCMOS12"
          || IOSTANDARD == "LVTTL33")
        { 
