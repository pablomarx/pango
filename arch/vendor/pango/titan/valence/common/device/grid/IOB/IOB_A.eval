    }
    else //if (VCCIO == "3.3") 
    {
        SC_PU2_B    := 1'b0    ;
        SC_PU3_B    := 1'b0    ;
        SC_PD2      := 1'b0    ;
        SC_PD3      := 1'b0    ;        
    }
    
    if ( IOSTANDARD == "SSTL25_I"  || IOSTANDARD == "SSTL25_II" 
      || IOSTANDARD == "SSTL18_I"  || IOSTANDARD == "SSTL18_II"
      || IOSTANDARD == "SSTL15_I"  || IOSTANDARD == "SSTL15_II"            
      || IOSTANDARD == "SSTL25D"   || IOSTANDARD == "SSTL18D"    || IOSTANDARD == "SSTL15D"  
      || IOSTANDARD == "SSTL25D_I" || IOSTANDARD == "SSTL25D_II"
      || IOSTANDARD == "SSTL18D_I" || IOSTANDARD == "SSTL18D_II" 
      || IOSTANDARD == "SSTL15D_I" || IOSTANDARD == "SSTL15D_II")
    {
        if (VREF_MODE == "IN")
        {
            if (VREF_IN_MODE == "0.45")
            {
                SC_VRE       := 2'b01   ;
                SC_VREFPD_N  := 1'b1    ;           
            }
            else if (VREF_IN_MODE == "0.5")
            {
                SC_VRE       := 2'b10   ; 
                SC_VREFPD_N  := 1'b1    ; 
            }
            else if (VREF_IN_MODE == "0.55")
            {
                SC_VRE       := 2'b11   ;   
                SC_VREFPD_N  := 1'b1    ;  
            }  
            else if (VREF_IN_MODE == "OFF")
            {
                SC_VRE       := 2'b00   ;
                SC_VREFPD_N  := 1'b0    ;
            }
        }
        else
        {
            SC_VREFPD_N  := 1'b1    ; 
        }        
        
        SC_STDBY_INR             := 1'b0    ;
    } 
    else if ( IOSTANDARD == "HSTL18_I"  || IOSTANDARD == "HSTL18_II"
           || IOSTANDARD == "HSTL15_I"  || IOSTANDARD == "HSTL15_II"            
           || IOSTANDARD == "HSTL18D"   || IOSTANDARD == "HSTL15D"
           || IOSTANDARD == "HSTL18D_I" || IOSTANDARD == "HSTL18D_II"
           || IOSTANDARD == "HSTL15D_I")
    {
        if (VREF_MODE == "IN")
        {
            if (VREF_IN_MODE == "0.45")
            {
                SC_VRE       := 2'b01   ;
                SC_VREFPD_N  := 1'b1    ; 
         
            }
            else if (VREF_IN_MODE == "0.5")
            {
                SC_VRE       := 2'b10   ; 
                SC_VREFPD_N  := 1'b1    ; 
            }
            else if (VREF_IN_MODE == "0.55")
            {
                SC_VRE       := 2'b11   ;   
                SC_VREFPD_N  := 1'b1    ;
            }  
            else if (VREF_IN_MODE == "OFF")
            {
                SC_VRE       := 2'b00   ;
                SC_VREFPD_N  := 1'b0    ;
            }
        }
        else
        {
            SC_VREFPD_N  := 1'b1    ; 
        }
           
        SC_STDBY_INR             := 1'b0    ;          
    }       
                
    if (IOB_MODE == "IBUF" || IOB_MODE == "IOBUF" || IOB_MODE == "IBUFDS" || IOB_MODE == "IOBUFDS")
    {
        // ibuf
        if ( IOSTANDARD == "SSTL25_I" || IOSTANDARD == "SSTL25_II" 
          || IOSTANDARD == "SSTL18_I" || IOSTANDARD == "SSTL18_II"
          || IOSTANDARD == "SSTL15_I" ||  IOSTANDARD == "SSTL15_II"                
          || IOSTANDARD == "SSTL25D"  || IOSTANDARD == "SSTL18D"   || IOSTANDARD == "SSTL15D"
          || IOSTANDARD == "HSTL18_I" || IOSTANDARD == "HSTL18_II" || IOSTANDARD == "HSTL18D"
          || IOSTANDARD == "HSTL15_I" ||  IOSTANDARD == "HSTL15_II"|| IOSTANDARD == "HSTL15D" 
          || IOSTANDARD == "LVDS")
        {
            SC_INR_ON                := 1'b1    ;    
        }  
        else 
        {
            SC_INR_ON                := 1'b0    ; 
        }  
    } 
         
    if ( IOB_MODE == "OBUF"  || IOB_MODE == "IOBUF" 
      || IOB_MODE == "OBUFT" || IOB_MODE == "OBUFDS"
      || IOB_MODE == "IOBUFDS") 
    {
        // obuf
        if (IOSTANDARD == "SSTL25_I"  || IOSTANDARD == "SSTL25_II"
         || IOSTANDARD == "SSTL25D_I" || IOSTANDARD == "SSTL25D_II")
        {
            SC_HSPD_CTRL             := 2'b00   ;
        }
        else if (IOSTANDARD == "SSTL18_I" || IOSTANDARD == "SSTL18_II"
              || IOSTANDARD == "SSTL18D_I"|| IOSTANDARD == "SSTL18D_II"
              || IOSTANDARD == "HSTL18_I" || IOSTANDARD == "HSTL18_II"
              || IOSTANDARD == "HSTL18D"  || IOSTANDARD == "HSTL18D_I" || IOSTANDARD == "HSTL18D_II")
        {
            SC_HSPD_CTRL             := 2'b10   ;    
        }
        else if (IOSTANDARD == "SSTL15_I"  || IOSTANDARD == "SSTL15_II"
              || IOSTANDARD == "SSTL15D_I" || IOSTANDARD == "SSTL15D_II" 
              || IOSTANDARD == "HSTL15_I"  || IOSTANDARD == "HSTL15_II"            
              || IOSTANDARD == "HSTL15D"   || IOSTANDARD == "HSTL15D_I")
        {
            SC_HSPD_CTRL             := 2'b01   ;    
        }
        else
        {
            SC_HSPD_CTRL             := 2'b00   ;    
        }
    }
    
    if (IOSTANDARD == "LVDS" && (IOB_MODE == "OBUFDS" || IOB_MODE == "OBUFT" || IOB_MODE == "IOBUFDS"))
    {   
        SC_RCON_EN  := 1'b1    ;
        if (DRV_CURR_VAL == "4.5")
        {
            SC_DRV  := 3'b010  ;
        }
        else if (DRV_CURR_VAL == "4")
        {
            SC_DRV  := 3'b001  ;
        }
        else if (DRV_CURR_VAL == "3.5")
        {
            SC_DRV  := 3'b000  ;
        }   
        else if (DRV_CURR_VAL == "2.5")
        {
            SC_DRV  := 3'b011  ;
        }    
        else if (DRV_CURR_VAL == "2")
        {
            SC_DRV  := 3'b100  ;
        }
        else if (DRV_CURR_VAL == "1.25")
        {
            SC_DRV  := 3'b101  ;
        }     
        else
        {
            SC_DRV  := 3'b000  ;
        } 
        SC_STDBY_LVDS            := 1'b0    ; 
        SC_LVDS_ON               := 1'b1    ;                              
    }
    else if (IOSTANDARD == "RSDS")
    {   
        SC_RCON_EN  := 1'b1    ;
        if (DRV_CURR_VAL == "2")
        {
            SC_DIFFMODE_SEL  := 2'b01   ;
            SC_DRV           := 3'b110  ;
        } 
        else
        {
            SC_DIFFMODE_SEL  := 2'b00   ;
            SC_DRV           := 3'b000  ;
        }  
        SC_STDBY_LVDS        := 1'b0    ; 
        SC_LVDS_ON           := 1'b1    ;                 
    }
    else if (IOSTANDARD == "PPDS")
    {
        SC_RCON_EN  := 1'b1    ;
        if (DRV_CURR_VAL == "2")
        {
            SC_DIFFMODE_SEL  := 2'b10   ;
            SC_DRV           := 3'b111  ;
        } 
        else
        {
            SC_DIFFMODE_SEL  := 2'b00   ;
            SC_DRV           := 3'b000  ;
        } 
        SC_STDBY_LVDS        := 1'b0    ; 
        SC_LVDS_ON           := 1'b1    ;       
    } 
    
}; // end of configuration cfg of IOB_A






