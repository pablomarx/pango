/*******************************************************************************

  Device    [IPAL]
  
  Author    [tangqiang]

  Abstract  [The floorplan symbol of IPAL]

  Revision History:

********************************************************************************/
symbol fpsym of IPAL // pragma PAP_ARC_COLOR="210:45:45"
{
    // The bounding box
    generate ( 10*8 # 40*8 );

    //
    // Poloygon declaration
    //
    shape
    [ 0, 0 ]  ->  [  0, 40*8 ]   
              ->  [ 10*8, 40*8 ]
              ->  [ 10*8,  0 ]   
              ->  [  0,  0 ]  ;

    //
    // Free-hanging text and edge
    //
    "IPAL" @ [5*8,20*8];


}; // end of symbol fpsym of IPAL
