symbol logsym of FGA // pragma PAP_ARC_COLOR="252:252:184"
{
    // The bounding box
    generate ( 16 # 28 );

    //
    // Poloygon declaration
    //
    shape
    [ 0, 0 ]  ->  [  0, 28 ]   < A0 @[,4], A1 @[,8], A2 @[,12], A3 @[,16], 
                                 A4 @[,20], A5 @[,24] >
              ->  [ 16, 28 ]   < COUT @[8,] >
              ->  [ 16,  0 ]   < Z @[,18] >
              ->  [  0,  0 ]   < CIN @[8,] >;

}; // end of symbol logsym of FGA


