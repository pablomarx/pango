symbol logsym of USCMDC // pragma PAP_ARC_COLOR="128:16:16"
{
    // The bounding box
    generate ( 20 # 20 );
    //
    // Poloygon declaration
    //
    shape
    [ 0, 0 ]  ->  [ , 20 ]
                        <
                            USCMDC_EN       @[ ,4 ],
                            CLK_IN          @[ ,10]
                        >
              ->  [ 20, ]
              
              ->  [ , 0 ]
                        <
                            CLK_OUT         @[ ,10]
                        >
              ->  [ 0, ] ;

}; // symbol logsym of USCMDC


