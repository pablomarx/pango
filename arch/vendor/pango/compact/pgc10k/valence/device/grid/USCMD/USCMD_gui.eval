symbol logsym of USCMD // pragma PAP_ARC_COLOR="210:45:45"
{
    // The bounding box
    generate ( 20 # 20 );
    //
    // Poloygon declaration
    //
    shape
    [ 0, 0 ]  ->  [ , 20 ]
                        <
                            USCMD_SEL       @[ ,4],
                            
                            CLK_A           @[ ,8 ],
                            CLK_B           @[ ,12]
                        >
              ->  [ 20, ]
              
              ->  [ , 0 ]
                        <
                            CLK_USCMD       @[ ,10]
                        >
              ->  [ 0, ] ;

}; // symbol logsym of USCMD


