use pgc_defs,pgc7km_defs;
//use pgc_IOB_TILES_input_inc,pgc_WLSR2_input_inc,pgc_PLL_input_inc;
use pgc_USCM_input_inc,pgc_IOCK_input_inc,pgc_PLL_input_inc;
use pgc_CLKDLYT_input_inc,pgc_CLKDLYB_input_inc;


package pgc7km_funcs_connect_clk
{
/****************************************************************************************************
*****************************************************************************************************/
function connect_uscm_input_7km
(
    unsigned int tx,
    unsigned int ty,
    unsigned int tx_ckeb2_l,
    unsigned int tx_ckeb2_r,
    unsigned int ty_ckeb2,

    string tile_ckeb2,
    string tile_uscm
)
{
    int sx = tx * NUM_GRID_X;
    int sy = ty * NUM_GRID_Y;
    int sx_ckeb2_l = tx_ckeb2_l * NUM_GRID_X;
    int sx_ckeb2_r = tx_ckeb2_r * NUM_GRID_X;
    int sy_ckeb2 = ty_ckeb2 * NUM_GRID_X;

    string ckeb2 = tile_ckeb2;
    string uscm = tile_uscm;

    connect
    (
        <pin CLK_IN[0]  of <device *uscm @[sx,sy]>> => <wire CLK_PIO_OUT[0]  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IN[1]  of <device *uscm @[sx,sy]>> => <wire CLK_PIO_OUT[1]  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IN[2]  of <device *uscm @[sx,sy]>> => <wire CLK_DLY_L_OUT   of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IN[3]  of <device *uscm @[sx,sy]>> => <wire CLK_DLY_M_OUT   of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IN[4]  of <device *uscm @[sx,sy]>> => <wire CLK_DLY_R_OUT   of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IN[5]  of <device *uscm @[sx,sy]>> => <wire CLK_PIO_OUT[1]  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IN[6]  of <device *uscm @[sx,sy]>> => <wire CLKOS_PLL0_OUT  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IN[7]  of <device *uscm @[sx,sy]>> => <wire CLK_CFGS_OUT    of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
//      <pin CLK_IN[8]  of <device *uscm @[sx,sy]>> => <wire TIEHI           of <device *uscm    @[sx,sy]>>,
//      <pin CLK_IN[9]  of <device *uscm @[sx,sy]>> => <wire TIEHI           of <device *uscm    @[sx,sy]>>,
        <pin CLK_IN[10] of <device *uscm @[sx,sy]>> => <wire CLK_DIV0_OUT[0] of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IN[11] of <device *uscm @[sx,sy]>> => <wire CLK_DIV0_OUT[1] of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IN[12] of <device *uscm @[sx,sy]>> => <wire CLK_DIV1_OUT[0] of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IN[13] of <device *uscm @[sx,sy]>> => <wire CLK_DIV1_OUT[1] of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IN[14] of <device *uscm @[sx,sy]>> => <wire CLK_DIV0_OUT[0] of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IN[15] of <device *uscm @[sx,sy]>> => <wire CLK_DIV0_OUT[1] of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IN[16] of <device *uscm @[sx,sy]>> => <wire CLK_DIV1_OUT[0] of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IN[17] of <device *uscm @[sx,sy]>> => <wire CLK_DIV1_OUT[1] of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IN[18] of <device *uscm @[sx,sy]>> => <wire CLK_PIO_OUT[0]  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IN[19] of <device *uscm @[sx,sy]>> => <wire CLKOP_PLL0_OUT  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IN[20] of <device *uscm @[sx,sy]>> => <wire TIEHI           of <device *uscm    @[sx,sy]>>,
        <pin CLK_IN[21] of <device *uscm @[sx,sy]>> => <wire CLKOS3_PLL0_OUT of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IN[22] of <device *uscm @[sx,sy]>> => <wire CLKOP_PLL1_OUT  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IN[23] of <device *uscm @[sx,sy]>> => <wire CLKOS_PLL1_OUT  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IN[24] of <device *uscm @[sx,sy]>> => <wire CLKOS2_PLL1_OUT of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IN[25] of <device *uscm @[sx,sy]>> => <wire CLKOS3_PLL1_OUT of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IN[26] of <device *uscm @[sx,sy]>> => <wire CLK_CFGS_OUT    of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
 
        <pin CLK_IN_CKEB[0]  of <device *uscm @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[0] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[1]  of <device *uscm @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[1] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[2]  of <device *uscm @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[2] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[3]  of <device *uscm @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[3] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[4]  of <device *uscm @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[4] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[5]  of <device *uscm @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[5] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[6]  of <device *uscm @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[6] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[7]  of <device *uscm @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[7] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[8]  of <device *uscm @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[0] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[9]  of <device *uscm @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[1] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[10] of <device *uscm @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[2] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[11] of <device *uscm @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[3] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[12] of <device *uscm @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[4] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[13] of <device *uscm @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[5] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[14] of <device *uscm @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[6] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IN_CKEB[15] of <device *uscm @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[7] of <device *ckeb2 @[sx_ckeb2_l,sy_ckeb2]>>,
 
        <pin CLK_IOCKBRGMUX_L0[1] of <device *uscm @[sx,sy]>> => <wire CLKOP_PLL0_OUT  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_L0[2] of <device *uscm @[sx,sy]>> => <wire CLKOS_PLL1_OUT  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IOCKBRGMUX_L0[3] of <device *uscm @[sx,sy]>> => <wire CLK_PIO_OUT[0]  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_L0[4] of <device *uscm @[sx,sy]>> => <wire CLK_PIO_OUT[1]  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_L0[5] of <device *uscm @[sx,sy]>> => <wire CLK_IOPRE2BRG_O[0]  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_L0[6] of <device *uscm @[sx,sy]>> => <wire CLK_IOPRE2BRG_O[0]  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,

        <pin CLK_IOCKBRGMUX_L1[1] of <device *uscm @[sx,sy]>> => <wire CLKOP_PLL0_OUT  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_L1[2] of <device *uscm @[sx,sy]>> => <wire CLKOS_PLL1_OUT  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>, 
        <pin CLK_IOCKBRGMUX_L1[3] of <device *uscm @[sx,sy]>> => <wire CLK_PIO_OUT[0]  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_L1[4] of <device *uscm @[sx,sy]>> => <wire CLK_PIO_OUT[1]  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_L1[5] of <device *uscm @[sx,sy]>> => <wire CLK_IOPRE2BRG_O[1]  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_L1[6] of <device *uscm @[sx,sy]>> => <wire CLK_IOPRE2BRG_O[1]  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,

        <pin CLK_IOCKBRGMUX_R0[1] of <device *uscm @[sx,sy]>> => <wire CLK_DLY_M_OUT   of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_R0[2] of <device *uscm @[sx,sy]>> => <wire CLKOS_PLL1_OUT  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_R0[3] of <device *uscm @[sx,sy]>> => <wire CLK_PIO_OUT[0]  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_R0[4] of <device *uscm @[sx,sy]>> => <wire CLK_PIO_OUT[1]  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_R0[5] of <device *uscm @[sx,sy]>> => <wire CLK_IOPRE2BRG_O[0]  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_R0[6] of <device *uscm @[sx,sy]>> => <wire CLK_IOPRE2BRG_O[0]  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,

        <pin CLK_IOCKBRGMUX_R1[1] of <device *uscm @[sx,sy]>> => <wire CLKOP_PLL1_OUT  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_R1[2] of <device *uscm @[sx,sy]>> => <wire CLK_CFGS_OUT    of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>, 
        <pin CLK_IOCKBRGMUX_R1[3] of <device *uscm @[sx,sy]>> => <wire CLKOP_PLL0_OUT  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_R1[4] of <device *uscm @[sx,sy]>> => <wire CLKOS_PLL0_OUT  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_R1[5] of <device *uscm @[sx,sy]>> => <wire CLK_IOPRE2BRG_O[1]  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IOCKBRGMUX_R1[6] of <device *uscm @[sx,sy]>> => <wire CLK_IOPRE2BRG_O[1]  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,

        <pin CLK_IOMUX2PLL_R[0]   of <device *uscm @[sx,sy]>> => <wire CLK_IOMUX2PLL_O[0]  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_IOMUX2PLL_R[1]   of <device *uscm @[sx,sy]>> => <wire CLK_IOMUX2PLL_O[1]  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,

        <pin CLKOP_PLL_D          of <device *uscm @[sx,sy]>> => <wire CLKOP_PLL0_OUT  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLKOP_PLL_U          of <device *uscm @[sx,sy]>> => <wire CLKOP_PLL1_OUT  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,
        <pin CLKOS_PLL_D          of <device *uscm @[sx,sy]>> => <wire CLKOS_PLL0_OUT  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLKOS_PLL_U          of <device *uscm @[sx,sy]>> => <wire CLKOS_PLL1_OUT  of <device *ckeb2   @[sx_ckeb2_l,sy_ckeb2]>>,

        <pin CLK_P_BL             of <device *uscm @[sx,sy]>> => <wire CLK_CFGS_OUT    of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_P_BM             of <device *uscm @[sx,sy]>> => <wire TIEHI  of <device *uscm   @[sx,sy]>>,
        <pin CLK_P_BR             of <device *uscm @[sx,sy]>> => <wire TIEHI  of <device *uscm   @[sx,sy]>>,
        <pin CLK_P_TL             of <device *uscm @[sx,sy]>> => <wire CLK_DLY_L_OUT   of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_P_TM             of <device *uscm @[sx,sy]>> => <wire CLK_DLY_M_OUT   of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_P_TR             of <device *uscm @[sx,sy]>> => <wire CLK_DLY_R_OUT   of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,

        <pin CLK_PIO2PLL_R_IN[0]  of <device *uscm @[sx,sy]>> => <wire CLK_PIO_OUT[0]   of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_PIO2PLL_R_IN[1]  of <device *uscm @[sx,sy]>> => <wire CLK_PIO_OUT[1]   of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_REF_0_BUF2       of <device *uscm @[sx,sy]>> => <wire CLKOS3_PLL0_OUT  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
        <pin CLK_REF_1_BUF2       of <device *uscm @[sx,sy]>> => <wire CLKOS3_PLL1_OUT  of <device *ckeb2   @[sx_ckeb2_r,sy_ckeb2]>>,
//      <pin CLK_RX_0_BUF2        of <device *uscm @[sx,sy]>> => <wire 
//      <pin CLK_RX_1_BUF2        of <device *uscm @[sx,sy]>> => <wire 
//      <pin CLK_TX_0_BUF2        of <device *uscm @[sx,sy]>> => <wire 
//      <pin CLK_TX_1_BUF2        of <device *uscm @[sx,sy]>> => <wire 
        <pin CLK_RX_0_DATA_BUF2   of <device *uscm @[sx,sy]>> => <wire TIEHI  of <device *uscm   @[sx,sy]>>,
        <pin CLK_RX_1_DATA_BUF2   of <device *uscm @[sx,sy]>> => <wire TIEHI  of <device *uscm   @[sx,sy]>>

    );


};//end of function connect_uscm_inpu_7km
/****************************************************************************************************
*****************************************************************************************************/
function connect_ckeb2_input_7km
(
    unsigned int tx,
    unsigned int ty,
    unsigned int tx_uscm,
    unsigned int ty_uscm,
    unsigned int ty_iock,

    string tile_uscm,
    string tile_iock,
    string tile_ckeb2
)
{
    int sx = tx * NUM_GRID_X;
    int sy = ty * NUM_GRID_Y;
    int sx_uscm = tx_uscm * NUM_GRID_X;
    int sy_uscm = ty_uscm * NUM_GRID_Y;
    int sx_iock = (tx < tx_uscm) ? (1 * NUM_GRID_X + 3) : ((NUM_TILE_X - 2) * NUM_GRID_X + 3);
    int sy_iock = ty_iock * NUM_GRID_Y;

    string ckeb2 = tile_ckeb2;
    string uscm = tile_uscm;
    string iock = tile_iock;

    if(tx < tx_uscm)
    {
        connect
        (
           <pin CLK_IN[0]     of <device *ckeb2 @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[0] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[1]     of <device *ckeb2 @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[1] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[2]     of <device *ckeb2 @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[2] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[3]     of <device *ckeb2 @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[3] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[4]     of <device *ckeb2 @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[4] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[5]     of <device *ckeb2 @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[5] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[6]     of <device *ckeb2 @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[6] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[7]     of <device *ckeb2 @[sx,sy]>> => <wire SIG_GLOBAL_OUTR[7] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[8]     of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[0] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[9]     of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[1] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[10]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[2] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[11]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[3] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[12]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[4] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[13]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[5] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[14]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[6] of <device *ckeb2 @[sx,sy]>>, 
           <pin CLK_IN[15]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_OUTR[7] of <device *ckeb2 @[sx,sy]>>, 

           <pin CLK_GLOBAL_IN[0]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_L[0] of <device *uscm @[sx_uscm,sy_uscm]>>, 
           <pin CLK_GLOBAL_IN[1]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_L[1] of <device *uscm @[sx_uscm,sy_uscm]>>, 
           <pin CLK_GLOBAL_IN[2]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_L[2] of <device *uscm @[sx_uscm,sy_uscm]>>, 
           <pin CLK_GLOBAL_IN[3]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_L[3] of <device *uscm @[sx_uscm,sy_uscm]>>, 
           <pin CLK_GLOBAL_IN[4]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_L[4] of <device *uscm @[sx_uscm,sy_uscm]>>, 
           <pin CLK_GLOBAL_IN[5]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_L[5] of <device *uscm @[sx_uscm,sy_uscm]>>, 
           <pin CLK_GLOBAL_IN[6]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_L[6] of <device *uscm @[sx_uscm,sy_uscm]>>, 
           <pin CLK_GLOBAL_IN[7]    of <device *ckeb2 @[sx,sy]>> => <wire CLK_GLOBAL_L[7] of <device *uscm @[sx_uscm,sy_uscm]>>, 

           <pin SIG_GLOBAL_IN[0]    of <device *ckeb2 @[sx,sy]>> => <wire SIG_GLOBAL_L[0] of <device *uscm @[sx_uscm,sy_uscm]>>, 
           <pin SIG_GLOBAL_IN[1]    of <device *ckeb2 @[sx,sy]>> => <wire SIG_GLOBAL_L[1] of <device *uscm @[sx_uscm,sy_uscm]>>, 
           <pin SIG_GLOBAL_IN[2]    of <device *ckeb2 @[sx,sy]>> => <wire SIG_GLOBAL_L[2] of <device *uscm @[sx_uscm,sy_uscm]>>, 
