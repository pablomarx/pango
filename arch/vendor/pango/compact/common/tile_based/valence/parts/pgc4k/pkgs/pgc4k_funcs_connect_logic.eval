    int sy_analog = ty_analog * NUM_GRID_Y;

    if(tx==0)
    {
     int ty_srb = ty + 16;
     int sy_srb = ty_srb * NUM_GRID_Y;
     int sx_srb = (tx+1)*NUM_GRID_X;

     string ccs = "CCS_4K_TILE";
     string analog = "ANALOG_4K_TILE";

       connect
         (
            <pin IPOLY10U of <device IOB0_4K_TILE @[sx,sy]>> => <wire IPOLY_10U[0] of <device *analog @ [sx_analog,sy_analog]>>,
            <pin ICONST3U[0] of <device IOB0_4K_TILE @[sx,sy]>> => <wire ICONST_3U[0] of <device *analog @ [sx_analog,sy_analog]>>,
