        <pin SEL_VREF of <device *analog @[sx,sy]>>           => <wire TIEHI of <device *analog @[sx,sy]>>,
        <pin OSC_EN of <device *analog @[sx,sy]>>             => <wire OSC_EN of <device *ccs @[sx,sy_ccs]>>,
        <pin MFG_SRAM_RETENTION of <device *analog @[sx,sy]>> => <wire MFG_SRAM_RETENTION of <device *ccs @[sx,sy_ccs]>>,

        <pin LVDS_VCM[1] of <device *analog @ [sx,sy]>>  => <wire VL of <device *iob0 @[sx_iob0,sy_iob0]>>,
        <pin LVDS_VCM[0] of <device *analog @ [sx,sy]>>  => <wire VH of <device *iob0 @[sx_iob0,sy_iob0]>>,
        <pin VREF of <device *analog @ [sx,sy]>>         => <wire VREF_TEST of <device *iob0 @[sx_iob0,sy_iob0]>>,
        <pin PLL_LDO_I[0] of <device *analog @[sx,sy]>>   => <wire LDO_TEST_OUT of <device PLL_TILE @[sx,sy_plld]>>,
        <pin PLL_LDO_I[1] of <device *analog @[sx,sy]>>   => <wire LDO_TEST_OUT of <device  PLL_TILE @[sx,sy_pllu]>>,
        <pin VREF_IN of <device *analog @ [sx,sy]>>  => <wire VREF_TEST of <device *iob1 @[sx,sy_iob1]>>,
//        <pin VREF_IN[1] of <device *analog @ [sx,sy]>>  => <wire VREF_TEST of <device *iob2 @[sx_iob2,sy_iob2]>>,
//        <pin VREF_IN[2] of <device *analog @ [sx,sy]>>  => <wire VREF_TEST of <device *iob3 @[sx_iob3,sy_iob3]>>,
