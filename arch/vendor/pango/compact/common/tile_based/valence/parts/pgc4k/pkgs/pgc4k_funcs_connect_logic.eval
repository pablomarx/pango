        <pin NM_DATA[19] of <device *analog @ [sx,sy]>> => <wire JRTI            of <device *ccs @ [sx,sy_ccs]>>,
        <pin NM_DATA[18] of <device *analog @ [sx,sy]>> => <wire CAPTUREDR       of <device *ccs @ [sx,sy_ccs]>>,
        <pin NM_DATA[17] of <device *analog @ [sx,sy]>> => <wire FLG_USER1       of <device *ccs @ [sx,sy_ccs]>>,
        <pin NM_DATA[16] of <device *analog @ [sx,sy]>> => <wire CLOCKDR1        of <device *ccs @ [sx,sy_ccs]>>,
        <pin NM_DATA[15] of <device *analog @ [sx,sy]>> => <wire FLG_USER2       of <device *ccs @ [sx,sy_ccs]>>,
        <pin NM_DATA[14] of <device *analog @ [sx,sy]>> => <wire CLOCKDR2        of <device *ccs @ [sx,sy_ccs]>>,
        <pin NM_DATA[13] of <device *analog @ [sx,sy]>> => <wire UID_DOUT        of <device *ccs @ [sx,sy_ccs]>>,
        <pin NM_DATA[12] of <device *analog @ [sx,sy]>> => <wire PCTLR_STOP      of <device *ccs @ [sx,sy_ccs]>>,
        <pin NM_DATA[11] of <device *analog @ [sx,sy]>> => <wire PCTLR_STDBY     of <device *ccs @ [sx,sy_ccs]>>,
        <pin NM_DATA[10] of <device *analog @ [sx,sy]>> => <wire PCTLR_STDBY_FLG of <device *ccs @ [sx,sy_ccs]>>,
