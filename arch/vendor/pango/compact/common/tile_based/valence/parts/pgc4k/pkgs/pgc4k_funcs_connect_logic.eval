        <pin NM_DATA[8:1] of <device *analog @ [sx,sy]>> => <wire PRDATA[7:0]     of <device *ccs @ [sx,sy_ccs]>>,
        <pin NM_DATA[0] of <device *analog @ [sx,sy]>> => <wire PREADY          of <device *ccs @ [sx,sy_ccs]>>

      );
}
/********************************************************************************
********************************************************************************/

function connect_ccs_input
(
  unsigned int tx,
  unsigned int ty,
  unsigned int ty_analog,
  unsigned int tx_cram,
  unsigned int ty_cram,
  unsigned int ty_idcode,
  unsigned int tx_esd,
  unsigned int ty_esd,
  unsigned int tx_iob0,
  unsigned int ty_iob0
)

{

    int sx = tx * NUM_GRID_X + 3;
    int sy = ty * NUM_GRID_Y;
    int sy_analog = ty_analog * NUM_GRID_Y;
    int sx_cram = tx_cram * NUM_GRID_X;
    int sy_cram = ty_cram * NUM_GRID_Y;
    int sy_idcode = ty_idcode * NUM_GRID_Y;
    int sx_esd = tx_esd * NUM_GRID_X;
    int sy_esd = ty_esd * NUM_GRID_Y;
    int sx_iob0 = tx_iob0 * NUM_GRID_X;
    int sy_iob0 = ty_iob0 * NUM_GRID_Y;

    string ccs = "CCS_4K_TILE";
    string analog = "ANALOG_4K_TILE";
    string cram_ctrl = "CRAM_CTRL_4K_TILE";
    string idcode = "IDCODE_4K_TILE";
    string esd = "ESD_4K_TILE";
    string iob0 = "IOB0_4K_TILE";

    connect
      (
        <pin PCTLR_BG_STABLE of <device *ccs @[sx,sy]>> => <wire BGSTABLE       of <device *analog     @[sx,sy_analog]>>,    
        <pin RBCRC_CLK       of <device *ccs @[sx,sy]>> => <wire CLK_SED        of <device *analog     @[sx,sy_analog]>>,    
        <pin MCLK            of <device *ccs @[sx,sy]>> => <wire CLK_CFGM       of <device *analog     @[sx,sy_analog]>>,    
        <pin CLK             of <device *ccs @[sx,sy]>> => <wire CLK_CFGS       of <device *analog     @[sx,sy_analog]>>,    
        <pin EFB_PCLK        of <device *ccs @[sx,sy]>> => <wire CLK_EFLASH     of <device *analog     @[sx,sy_analog]>>,    
        <pin POR_N           of <device *ccs @[sx,sy]>> => <wire POR_N          of <device *analog     @[sx,sy_analog]>>,    
        <pin EFB_VREF        of <device *ccs @[sx,sy]>> => <wire VBG_0P75V      of <device *analog     @[sx,sy_analog]>>,    
//        <pin POR1P06_N        of <device *ccs @[sx,sy]>> => <wire POR_N_1P06      of <device *analog     @[sx,sy_analog]>>,
                                                                                                                            
        <pin PLL_LOCK        of <device *ccs @[sx,sy]>> => <wire LOCKOUT_CAS    of <device PLL_TILE   @[sx,32*NUM_GRID_Y]>>,
                                                                                                                            
        <pin DATABACK        of <device *ccs @[sx,sy]>> => <wire DATA_BACK      of <device *cram_ctrl @[sx_cram,sy_cram]>>, 
        <pin NDRM_REGION_END of <device *ccs @[sx,sy]>> => <wire NONDRM_END     of <device *cram_ctrl @[sx_cram,sy_cram]>>, 
        <pin NDRM_COLUMN_INC of <device *ccs @[sx,sy]>> => <wire COLUMN_INC     of <device *cram_ctrl @[sx_cram,sy_cram]>>, 
        <pin GLOGEN_FB       of <device *ccs @[sx,sy]>> => <wire GLOGEN_OUTL[0] of <device *cram_ctrl @[sx_cram,sy_cram]>>, 
                                                                                                                            
        <pin IDCODE_H12      of <device *ccs @[sx,sy]>> => <wire IDCODE_H12     of <device *idcode    @[sx,sy_idcode]>>,    
        <pin IDCODE_L18      of <device *ccs @[sx,sy]>> => <wire IDCODE_L18     of <device *idcode    @[sx,sy_idcode]>>,    
        <pin DRM_COLUMN_NUM  of <device *ccs @[sx,sy]>> => <wire DRM_COLUMN_NUM of <device *idcode    @[sx,sy_idcode]>>,    
        <pin ROW_NUM         of <device *ccs @[sx,sy]>> => <wire ROW_NUM        of <device *idcode    @[sx,sy_idcode]>>,    
                                                                                                                            
        <pin TDI             of <device *ccs @[sx,sy]>> => <wire TDI_DIN        of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin TCK             of <device *ccs @[sx,sy]>> => <wire TCK_DIN        of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin TMS             of <device *ccs @[sx,sy]>> => <wire TMS_DIN        of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin DONE_I          of <device *ccs @[sx,sy]>> => <wire DONE_DIN       of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin INIT_N          of <device *ccs @[sx,sy]>> => <wire INITN_DIN      of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin RST_N           of <device *ccs @[sx,sy]>> => <wire RSTN           of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin JEN             of <device *ccs @[sx,sy]>> => <wire JTAGEN         of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin SPAL_CLK        of <device *ccs @[sx,sy]>> => <wire SPAL_CLK       of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin SPAL_DIN        of <device *ccs @[sx,sy]>> => <wire SPAL_DIN       of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin SPAL_CS_N       of <device *ccs @[sx,sy]>> => <wire SPAL_CS_N      of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin SPAL_RDWR_N     of <device *ccs @[sx,sy]>> => <wire SPAL_RDWR_N    of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin EFB_TCK         of <device *ccs @[sx,sy]>> => <wire EFB_CLK        of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin EFB_TDI         of <device *ccs @[sx,sy]>> => <wire EFB_DIN        of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin EFB_TSTR        of <device *ccs @[sx,sy]>> => <wire EFB_STR        of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin SCL             of <device *ccs @[sx,sy]>> => <wire SCL_DIN        of <device *iob0      @[sx_iob0,sy_iob0]>>, 
        <pin SDA_I           of <device *ccs @[sx,sy]>> => <wire SDA_DIN        of <device *iob0      @[sx_iob0,sy_iob0]>>,

//        <pin EFB_IREF of <device *ccs @[sx,sy]>>         => <wire ICONST_3U[11] of <device *analog @[sx,sy_analog]>>,
        <pin EFB_TP of <device *ccs @[sx,sy]>>     => <wire TP of <device *esd @[sx_esd,sy_esd]>>,
        <pin EFB_EN of <device *ccs @[sx,sy]>>     => <wire CP_EN of <device *esd @[sx_esd,sy_esd]>>

      );

} //end  of connect_ccs_tile

/********************************************************************************
********************************************************************************/
function connect_cram_ctrl_input(unsigned int tx,ty,tx_ccs,ty_ccs,tx_analog,ty_analog)
{   
    unsigned int sx, sy;
    
    sx = tx * NUM_GRID_X;
    sy = ty * NUM_GRID_Y;
    
    int sx_ccs = tx_ccs * NUM_GRID_X + 3;
    int sy_ccs = ty_ccs * NUM_GRID_Y;

    int sx_analog = tx_analog * NUM_GRID_X + 3;
    int sy_analog = ty_analog * NUM_GRID_Y;

    string ccs = "CCS_4K_TILE";
    string analog = "ANALOG_4K_TILE";
    string cram_ctrl = "CRAM_CTRL_4K_TILE";

    connect
      (
        <pin POR_N of <device *cram_ctrl @[sx,sy]>>         => <wire POR_N of <device *analog @[sx_analog,sy_analog]>>,

        <pin WL_ON of <device *cram_ctrl @[sx,sy]>>         => <wire WL_ON of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin READ of <device *cram_ctrl @[sx,sy]>>          => <wire READ of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin RDBK_CAP of <device *cram_ctrl @[sx,sy]>>      => <wire RDBK_CAP of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin ERASE_EN of <device *cram_ctrl @[sx,sy]>>      => <wire ERASE_EN of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin PROG_CAP of <device *cram_ctrl @[sx,sy]>>      => <wire PROG_CAP of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin PROG_ON of <device *cram_ctrl @[sx,sy]>>       => <wire PROG_ON of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin FRAME_ADDR of <device *cram_ctrl @[sx,sy]>>    => <wire FRAME_ADDR[5:0] of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin COLUMN_ADDR of <device *cram_ctrl @[sx,sy]>>   => <wire COLUMN[5:0] of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin PRECHG of <device *cram_ctrl @[sx,sy]>>        => <wire PRECHGE of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin REGION_RW_EN of <device *cram_ctrl @[sx,sy]>>  => <wire REGION_RW_EN of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin DATA_IN of <device *cram_ctrl @[sx,sy]>>       => <wire BLSRIN of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin MCLK of <device *cram_ctrl @[sx,sy]>>          => <wire CLK_CRAM of <device *ccs @[sx_ccs,sy_ccs]>>,

        <pin MCLKB of <device *cram_ctrl @[sx,sy]>>         => <wire CLK_DRM of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin MWR of <device *cram_ctrl @[sx,sy]>>           => <wire MWR of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin MCE of <device *cram_ctrl @[sx,sy]>>           => <wire MCE of <device *ccs @[sx_ccs,sy_ccs]>>,

        <pin GLOGEN_IN of <device *cram_ctrl @[sx,sy]>>     => <wire GLOGEN of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin GRS_N_IN of <device *cram_ctrl @[sx,sy]>>      => <wire GRS_N of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin GWEN_IN of <device *cram_ctrl @[sx,sy]>>       => <wire GWEN of <device *ccs @[sx_ccs,sy_ccs]>>,
        <pin PUP_REL_N_IN of <device *cram_ctrl @[sx,sy]>>  => <wire PUP_SEL_N of <device *ccs @[sx_ccs,sy_ccs]>>, 
        <pin TYPE of <device *cram_ctrl @[sx,sy]>>          => <wire CMEM_TYPE[0] of <device *ccs @[sx_ccs,sy_ccs]>>,

        <pin IREF of <device *cram_ctrl @[sx,sy]>>       => <wire ICONST_3U[8] of <device *analog @[sx_analog,sy_analog]>>,

        <pin MASK of <device *cram_ctrl @[sx,sy]>>     => <wire MASK_EN of <device *ccs @[sx_ccs,sy_ccs]>>

      );
    
      
}// end of function connect_cram_ctrl_input
/********************************************************************************
********************************************************************************/

function connect_pll_input
(
  unsigned int tx,
  unsigned int ty,
  unsigned int ty_analog,
  unsigned int tx_iob1,
  unsigned int ty_iob1,
  unsigned int tx_iob5,
  unsigned int ty_iob5,
  unsigned int ty_iock
)

{

    int sx = tx * NUM_GRID_X + 3;
    int sy = ty * NUM_GRID_Y;
    int sy_analog = ty_analog * NUM_GRID_Y;
    int sx_iob1 = tx_iob1 * NUM_GRID_X;
    int sy_iob1 = ty_iob1 * NUM_GRID_Y;
    int sx_iob5 = tx_iob5 * NUM_GRID_X;
    int sy_iob5 = ty_iob5 * NUM_GRID_Y;
    int sy_iock = ty_iock * NUM_GRID_Y;



    string analog = "ANALOG_4K_TILE";

    if(ty<ty_analog)
    {
    connect
      (

        <pin LOCKIN_CAS of <device PLL_TILE @ [sx,sy]>> => <wire TIEHI                   of <device PLL_TILE     @ [sx,sy]>>,
        <pin CLK_IN0[0] of <device PLL_TILE @ [sx,sy]>> => <wire TIEHI                   of <device PLL_TILE     @ [sx,sy]>>,
        <pin CLK_IN0[1] of <device PLL_TILE @ [sx,sy]>> => <wire IOLA_CLK                of <device SRB_TILE     @ [sx-3,sy]>>,
        <pin CLK_IN0[2] of <device PLL_TILE @ [sx,sy]>> => <wire IOLB_CLK                of <device SRB_TILE     @ [sx-3,sy]>>,
        <pin CLK_IN0[3] of <device PLL_TILE @ [sx,sy]>> => <wire DIN[8]                  of <device IOB5_4K_TILE @ [sx_iob5,sy_iob5]>>,
        <pin CLK_IN0[4] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_LD_BUF[0]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN0[5] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_LD_BUF[1]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN0[6] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_RD_BUF[0]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN0[7] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_RD_BUF[1]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN1[0] of <device PLL_TILE @ [sx,sy]>> => <wire TIEHI                   of <device PLL_TILE     @ [sx,sy]>>,
        <pin CLK_IN1[1] of <device PLL_TILE @ [sx,sy]>> => <wire S_CLK                   of <device SRB_TILE     @ [sx-3,sy+6]>>,
        <pin CLK_IN1[2] of <device PLL_TILE @ [sx,sy]>> => <wire A_CLK                   of <device SRB_TILE     @ [sx-3,sy+6]>>,
        <pin CLK_IN1[3] of <device PLL_TILE @ [sx,sy]>> => <wire DIN[8]                  of <device IOB5_4K_TILE @ [sx_iob5,sy_iob5]>>,
        <pin CLK_IN1[4] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_LD_BUF[0]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN1[5] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_LD_BUF[1]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN1[6] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_RD_BUF[0]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN1[7] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_RD_BUF[1]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_FB[0]  of <device PLL_TILE @ [sx,sy]>> => <wire DIN[4]                  of <device IOB5_4K_TILE @ [sx_iob5,sy_iob5]>>,
        <pin CLK_FB[1]  of <device PLL_TILE @ [sx,sy]>> => <wire IOLB_CLK                of <device SRB_TILE     @ [sx-3,sy+6]>>,
        <pin CLK_FB[2]  of <device PLL_TILE @ [sx,sy]>> => <wire IOLA_CLK                of <device SRB_TILE     @ [sx-3,sy+6]>>,
        <pin CLK_FB[3]  of <device PLL_TILE @ [sx,sy]>> => <wire CLK_IOMUX2PLLD[0]       of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_FB[4]  of <device PLL_TILE @ [sx,sy]>> => <wire CLK_IOMUX2PLLD[1]       of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_FB[5]  of <device PLL_TILE @ [sx,sy]>> => <wire CLK_IOMUX2PLL_RD_OUT[0] of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_FB[6]  of <device PLL_TILE @ [sx,sy]>> => <wire CLK_IOMUX2PLL_RD_OUT[1] of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_FB[7]  of <device PLL_TILE @ [sx,sy]>> => <wire CLK_GLOBAL_BUFD         of <device IOCK_7K_TILE @ [sx,sy_iock]>>,

        <pin IPOLY10U[1:0] of <device PLL_TILE @[sx,sy]>>    => <wire IPOLY_10U[9:8] of <device *analog @[sx,sy_analog]>>,
        <pin IPOLY10U[2] of <device PLL_TILE @[sx,sy]>>      => <wire IPOLY_10U[10] of <device *analog @[sx,sy_analog]>>
      );
    }
    else 
    {
    connect
      (
        <pin LOCKIN_CAS of <device PLL_TILE @ [sx,sy]>> => <wire LOCKOUT_CAS          of <device PLL_TILE     @ [sx,2*NUM_GRID_Y]>>,
        <pin CLK_IN0[0] of <device PLL_TILE @ [sx,sy]>> => <wire TIEHI                   of <device PLL_TILE     @ [sx,sy]>>,
        <pin CLK_IN0[1] of <device PLL_TILE @ [sx,sy]>> => <wire IOLA_CLK                of <device SRB_TILE     @ [sx-3,sy]>>,
        <pin CLK_IN0[2] of <device PLL_TILE @ [sx,sy]>> => <wire IOLB_CLK                of <device SRB_TILE     @ [sx-3,sy]>>,
        <pin CLK_IN0[3] of <device PLL_TILE @ [sx,sy]>> => <wire DIN[4]                  of <device IOB1_4K_TILE @ [sx_iob1,sy_iob1]>>,
        <pin CLK_IN0[4] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_LU_BUF[0]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN0[5] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_LU_BUF[1]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN0[6] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_RU_BUF[0]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN0[7] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_RU_BUF[1]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN1[0] of <device PLL_TILE @ [sx,sy]>> => <wire TIEHI                   of <device PLL_TILE     @ [sx,sy]>>,
        <pin CLK_IN1[1] of <device PLL_TILE @ [sx,sy]>> => <wire S_CLK                   of <device SRB_TILE     @ [sx-3,sy+6]>>,
        <pin CLK_IN1[2] of <device PLL_TILE @ [sx,sy]>> => <wire A_CLK                   of <device SRB_TILE     @ [sx-3,sy+6]>>,
        <pin CLK_IN1[3] of <device PLL_TILE @ [sx,sy]>> => <wire DIN[4]                  of <device IOB1_4K_TILE @ [sx_iob1,sy_iob1]>>,
        <pin CLK_IN1[4] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_LU_BUF[0]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN1[5] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_LU_BUF[1]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN1[6] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_RU_BUF[0]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_IN1[7] of <device PLL_TILE @ [sx,sy]>> => <wire CLK_PIO2PLL_RU_BUF[1]   of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_FB[0]  of <device PLL_TILE @ [sx,sy]>> => <wire DIN[0]                  of <device IOB1_4K_TILE @ [sx_iob1,sy_iob1]>>,
        <pin CLK_FB[1]  of <device PLL_TILE @ [sx,sy]>> => <wire IOLB_CLK                of <device SRB_TILE     @ [sx-3,sy+6]>>,
        <pin CLK_FB[2]  of <device PLL_TILE @ [sx,sy]>> => <wire IOLA_CLK                of <device SRB_TILE     @ [sx-3,sy+6]>>,
        <pin CLK_FB[3]  of <device PLL_TILE @ [sx,sy]>> => <wire CLK_IOMUX2PLLU[0]       of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_FB[4]  of <device PLL_TILE @ [sx,sy]>> => <wire CLK_IOMUX2PLLU[1]       of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_FB[5]  of <device PLL_TILE @ [sx,sy]>> => <wire CLK_IOMUX2PLL_RU_OUT[0] of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_FB[6]  of <device PLL_TILE @ [sx,sy]>> => <wire CLK_IOMUX2PLL_RU_OUT[1] of <device IOCK_7K_TILE @ [sx,sy_iock]>>,
        <pin CLK_FB[7]  of <device PLL_TILE @ [sx,sy]>> => <wire CLK_GLOBAL_BUFU         of <device IOCK_7K_TILE @ [sx,sy_iock]>>,

        <pin IPOLY10U[1:0] of <device PLL_TILE @[sx,sy]>>    => <wire IPOLY_10U[12:11] of <device *analog @[sx,sy_analog]>>,
        <pin IPOLY10U[2] of <device PLL_TILE @[sx,sy]>>      => <wire IPOLY_10U[13] of <device *analog @[sx,sy_analog]>>
      );
    }
}
/****************************************************************************************************
*****************************************************************************************************/
function connect_drm_4k_input
(
  unsigned int tx,
  unsigned int ty,
  unsigned int blank_y,
  unsigned int tx_uscm, 
  unsigned int min_y
)
{
  unsigned int sx,sy;
  sx = tx * NUM_GRID_X + 3;
  sy = ty * NUM_GRID_Y;


// Calculate the north source grid site
  unsigned int tile_s_1,site_s_1;
  tile_s_1 = (ty==blank_y+1) ? ty-4 : ty-3;
  site_s_1 = tile_s_1 * NUM_GRID_Y;

  

  if(ty!=min_y)
  {
    connect
     (
        <pin X1CASQI_A of <device DRM_TILE @[sx,sy]>>    => <wire X1CASQO_A of <device DRM_TILE @ [sx,site_s_1]>>,
        <pin X1CASQI_B of <device DRM_TILE @[sx,sy]>>    => <wire X1CASQO_B of <device DRM_TILE @ [sx,site_s_1]>>
      );
  }
  else
  {
    connect
     (
        <pin X1CASQI_A of <device DRM_TILE @[sx,sy]>>    => <wire TIEHI of <device DRM_TILE @ [sx,sy]>>,
        <pin X1CASQI_B of <device DRM_TILE @[sx,sy]>>    => <wire TIEHI of <device DRM_TILE @ [sx,sy]>>
     );
  }

  if(tx<tx_uscm)
  {
    if(ty>31)
    {
      connect
       (
         <pin NM_DATA_L of <device DRM_TILE @ [sx,sy]>> => <wire TIEHI of <device DRM_TILE @ [sx,sy]>>,
         <pin NM_DATA_R of <device DRM_TILE @ [sx,sy]>> => <wire TIEHI of <device DRM_TILE @ [sx,sy]>>
       );
    }
    else if(ty>min_y)
    {
      connect
       (
         <pin NM_DATA_L of <device DRM_TILE @ [sx,sy]>> => <wire TIEHI of <device  DRM_TILE @ [sx,sy]>>,
         <pin NM_DATA_M of <device DRM_TILE @ [sx,sy]>> => <wire TIEHI of <device  DRM_TILE @ [sx,sy]>>,
         <pin NM_DATA_R of <device DRM_TILE @ [sx,sy]>> => <wire TIEHI of <device  DRM_TILE @ [sx,sy]>>
       );
    }
  }
  else
  {
    connect
     (
       <pin NM_DATA_L of <device DRM_TILE @ [sx,sy]>> => <wire TIEHI of <device  DRM_TILE @ [sx,sy]>>,
       <pin NM_DATA_M of <device DRM_TILE @ [sx,sy]>> => <wire TIEHI of <device  DRM_TILE @ [sx,sy]>>,
       <pin NM_DATA_R of <device DRM_TILE @ [sx,sy]>> => <wire TIEHI of <device  DRM_TILE @ [sx,sy]>>
     );
  }

 
  
}//end of function connect_drm   
/********************************************************************************
********************************************************************************/
function connect_sfbudc_input(unsigned int tx,ty,tx_cram,ty_cram,tx_analog,ty_analog)
{   
    unsigned int sx, sy;
    
    sx = tx * NUM_GRID_X + 3;
    sy = ty * NUM_GRID_Y;
    
    int sx_cram = tx_cram * NUM_GRID_X;
    int sy_cram = ty_cram * NUM_GRID_Y;

    int sx_analog = tx_analog * NUM_GRID_X + 3;
    int sy_analog = ty_analog * NUM_GRID_Y;

    string analog = "ANALOG_4K_TILE";
    string cram_ctrl = "CRAM_CTRL_4K_TILE";

    if(ty<ty_cram)
    {
    connect
      (
        <pin LDO_VREF of <device SFBBC_4K_TILE @[sx,sy]>>         => <wire LDO_VREF of <device *analog @[sx_analog,sy_analog]>>,
        <pin IPOLY_LDO of <device SFBBC_4K_TILE @[sx,sy]>>         => <wire IPOLY_10U[1] of <device *analog @[sx_analog,sy_analog]>>
      );
    }
      
}// end of function connect_sfbudc_input
