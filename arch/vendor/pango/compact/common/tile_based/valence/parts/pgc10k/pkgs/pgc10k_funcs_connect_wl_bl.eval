********************************************************************************/
function connect_iobb_wl(unsigned int tx, ty, tx_cram, ty_cram, flag)
{
    unsigned int sx, sy; 
    sx = tx * NUM_GRID_X;
    sy = ty * NUM_GRID_Y;

    int sx_wl = tx_cram * NUM_GRID_X;
    int sy_wl = ty_cram * NUM_GRID_Y;

    string cram_ctrl= "CRAM_CTRL_10K_TILE";
    string iob = (flag==0) ? "IOB3_10K_TILE" : (flag==1) ? "IOB4_10K_TILE" : "IOB5_10K_TILE";

    if(flag==0)
    {
     connect
        (
          <pin ADDR[307:152] of <device *iob @[sx,sy]>>   => <wire ADDR[738:583] of <device *cram_ctrl @[sx_wl,sy_wl]>>,
          <pin ADDR[151] of <device *iob @[sx,sy]>>   => <wire ADDR[741] of <device *cram_ctrl @[sx_wl,sy_wl]>>,
          <pin ADDR[150:0] of <device *iob @[sx,sy]>>   => <wire ADDR[582:432] of <device *cram_ctrl @[sx_wl,sy_wl]>>
        );
    }
    else if(flag==1)
    {
       connect
       (
          <pin ADDR[177:126] of <device *iob @[sx,sy]>> => <wire ADDR[431:380] of <device *cram_ctrl @[sx_wl,sy_wl]>>,
          <pin ADDR[125] of <device *iob @[sx,sy]>> => <wire ADDR[740] of <device *cram_ctrl @[sx_wl,sy_wl]>>,
          <pin ADDR[124:0] of <device *iob @[sx,sy]>>   => <wire ADDR[379:255] of <device *cram_ctrl @[sx_wl,sy_wl]>>
       );
    }
    else
    {
     connect
        (
          <pin ADDR[255:178] of <device *iob @[sx,sy]>> => <wire ADDR[254:177] of <device *cram_ctrl @[sx_wl,sy_wl]>>,
          <pin ADDR[177] of <device *iob @[sx,sy]>> => <wire ADDR[739] of <device *cram_ctrl @[sx_wl,sy_wl]>>,
          <pin ADDR[176:0] of <device *iob @[sx,sy]>> => <wire ADDR[176:0] of <device *cram_ctrl @[sx_wl,sy_wl]>>
        );
    }
}   //end of connect_iobb_wl

