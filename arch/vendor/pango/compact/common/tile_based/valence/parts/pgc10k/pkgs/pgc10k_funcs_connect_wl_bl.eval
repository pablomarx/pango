/********************************************************************************
********************************************************************************/
function connect_iollr_wl_input
(
    unsigned int tx,
    unsigned int ty,
    unsigned int tx_wl,
    unsigned int ty_wl,
    unsigned int ty_ccs,
    unsigned int ty_analog
)
{
    int sx_wl = tx_wl * NUM_GRID_X;
    int sy_wl = ty_wl * NUM_GRID_Y;

    int sx,sy;
    sx = tx * NUM_GRID_X + 3;
    sy = ty * NUM_GRID_Y;

    string cram_ctrl = "CRAM_CTRL_10K_TILE";
    string ccs = "CCS_10K_TILE";
    string analog = "ANALOG_7K_TILE";

  if(tx==1)
  {
    if(ty==2 || ty==44)
    {
      connect
        (   
          <pin ADDR of <device PLL_TILE @ [sx,sy]>>     => <wire ADDR[25:22] of <device *cram_ctrl @ [sx_wl,sy_wl]>>,
          <pin GLOGEN of <device PLL_TILE @ [sx,sy]>>   => <wire GLOGEN_OUTL[0] of <device *cram_ctrl @ [sx_wl,sy_wl]>>
        );
    }
    else if(ty==ty_ccs)
    {
      connect
        (   
          <pin ADDR of <device *ccs @ [sx,sy]>>         => <wire ADDR[25:22] of <device *cram_ctrl @ [sx_wl,sy_wl]>>,
          <pin GLOGEN_SFB of <device *ccs @ [sx,sy]>>   => <wire GLOGEN_OUTL[0] of <device *cram_ctrl @ [sx_wl,sy_wl]>>

        );
    }
    else if(ty==ty_analog)
    {
      connect
        (   
          <pin ADDR of <device *analog @ [sx,sy]>>     => <wire ADDR[25:22] of <device *cram_ctrl @ [sx_wl,sy_wl]>>,
          <pin GLOGEN of <device *analog @ [sx,sy]>>   => <wire GLOGEN_OUTL[0] of <device *cram_ctrl @ [sx_wl,sy_wl]>>
//          <pin GWEN of <device *analog @ [sx,sy]>>     => <wire GWEN_OUTL[0] of <device *cram_ctrl @ [sx_wl,sy_wl]>>
        );
    }
    else
    {
      connect
        (   
          <pin ADDR of <device IOLL_TILE @ [sx,sy]>>     => <wire ADDR[25:22] of <device *cram_ctrl @ [sx_wl,sy_wl]>>,
          <pin GLOGEN of <device IOLL_TILE @ [sx,sy]>>   => <wire GLOGEN_OUTL[0] of <device *cram_ctrl @ [sx_wl,sy_wl]>>,
          <pin GRS_N of <device IOLL_TILE @ [sx,sy]>>    => <wire GRS_N_OUTL[0] of <device *cram_ctrl @ [sx_wl,sy_wl]>>
        );
    }
  }
  else
  {
      connect
        ( 
      <pin ADDR   of <device IOLR_TILE @ [30*NUM_GRID_X+3,sy]>> => <wire ADDR[736:733]   of <device *cram_ctrl @ [sx_wl,sy_wl]>>,
      <pin GLOGEN of <device IOLR_TILE @ [30*NUM_GRID_X+3,sy]>> => <wire GLOGEN_OUTR[13] of <device *cram_ctrl @ [sx_wl,sy_wl]>>,
      <pin GRS_N  of <device IOLR_TILE @ [30*NUM_GRID_X+3,sy]>> => <wire GRS_N_OUTR[13]  of <device *cram_ctrl @ [sx_wl,sy_wl]>>
        );
  }

}//end of connect_iollr_wl_input
