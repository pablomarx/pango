/********************************************************************************
********************************************************************************/
function connect_clm_mask_data
(
    unsigned int tx,
    unsigned int ty,
    unsigned int tx_wl,
    unsigned int ty_wl
)
{
    unsigned int sx,sy;
    sx = tx * NUM_GRID_X + 3;
    sy = ty * NUM_GRID_X;

    int sx_wl = tx_wl * NUM_GRID_X;
    int sy_wl = ty_wl * NUM_GRID_Y;

    int tx_b = tx - 1 ;
    int tx_bb = tx - 2 ;
    int tx_f = tx + 1 ;
    int tx_ff = tx + 2 ; 

    int sx_b = tx_b * NUM_GRID_X + 3;
    int sx_bb = tx_bb * NUM_GRID_X + 3 ;
    int sx_f = tx_f * NUM_GRID_X + 3 ;
    int sx_ff = tx_ff * NUM_GRID_X + 3 ;

    string clm = (tx==4 || tx==11 || tx==18 || tx==22 || tx==27) ? "CLMSS_TILE" : "CLMS_TILE";
    string clmb = (tx_b==4 || tx_b==11 || tx_b==18 || tx_b==22 || tx_b==27 ) ? "CLMSS_TILE" : "CLMS_TILE";
    string clmbb = (tx_bb==4 || tx_bb==11 || tx_bb==18 || tx_bb==22 || tx_bb==27 ) ? "CLMSS_TILE" : "CLMS_TILE";
    string clmf = (tx_f==4 || tx_f==11 || tx_f==18 || tx_f==22 || tx_f==27 ) ? "CLMSS_TILE": "CLMS_TILE";
    string clmff = (tx_ff==4 || tx_ff==11 || tx_ff==18 || tx_ff==22 || tx_ff==27 ) ? "CLMSS_TILE" : "CLMS_TILE";

    string cram_ctrl = "CRAM_CTRL_10K_TILE";

    if(tx==2||tx==29)
    {
        connect(<pin MASK_DATA_CIN of <device *clm @[sx,sy]>> => <wire TIELO of <device *clm @[sx,sy]>>);
    }
    else if(tx==8)
    {
        connect(<pin MASK_DATA_CIN of <device *clm @[sx,sy]>> => <wire MASK_DATA_COUT of <device *clmbb @[sx_bb,sy]>>);
    }
    else if(tx==23)
    {
        connect(<pin MASK_DATA_CIN of <device *clm @[sx,sy]>> => <wire MASK_DATA_COUT of <device *clmff @[sx_ff,sy]>>);
    }
    else if(tx<16)
    {
        connect(<pin MASK_DATA_CIN of <device *clm @[sx,sy]>> => <wire MASK_DATA_COUT of <device *clmb @[sx_b,sy]>>);
    }
    else
    {
        connect(<pin MASK_DATA_CIN of <device *clm @[sx,sy]>> => <wire MASK_DATA_COUT of <device *clmf @[sx_f,sy]>>);
    }

}
