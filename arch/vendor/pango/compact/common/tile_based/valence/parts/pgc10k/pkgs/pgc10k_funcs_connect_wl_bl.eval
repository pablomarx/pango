function connect_ioblr_wl(unsigned int tx, ty, tx_cram, ty_cram)
{
    unsigned int sx, sy; 
    sx = tx * NUM_GRID_X;
    sy = ty * NUM_GRID_Y;

    int sx_wl = tx_cram * NUM_GRID_X;
    int sy_wl = ty_cram * NUM_GRID_Y;

    string cram_ctrl="CRAM_CTRL_10K_TILE";

    string iob = (tx==0) ? "IOB0_10K_TILE" : "IOB2_10K_TILE";
    
    if(tx==0)
    {
    connect
      (
        <pin ADDR of  <device *iob @ [sx,sy]>>  => <wire ADDR[1:0] of <device *cram_ctrl @ [sx_wl,sy_wl]>>
      );
    }
    else
    {
    connect
      (
        <pin ADDR of  <device *iob @ [sx,sy]>>  => <wire ADDR[738:737] of <device *cram_ctrl @ [sx_wl,sy_wl]>>
      );
    }
}
