/********************************************************************************
********************************************************************************/
function connect_clm_mask_data_7km
(
    unsigned int tx,
    unsigned int ty,
    unsigned int tx_wl
)
{
    int sx = tx * NUM_GRID_X + 3;
    int sy = ty * NUM_GRID_Y;

    int tx_w = 2;
    int tx_e = NUM_TILE_X - 3;


    int tx_l = tx - 1;
    int tx_ll = tx - 2;
    int tx_r = tx + 1;
    int tx_rr = tx + 2;

    int sx_l = tx_l * NUM_GRID_X + 3;
    int sx_ll = tx_ll * NUM_GRID_X + 3;
    int sx_r = tx_r * NUM_GRID_X + 3;
    int sx_rr = tx_rr * NUM_GRID_X + 3;

    string clm    = (tx==3    || tx==7    || tx==11    || tx==16    || tx==22)    ? "CLMSS_TILE" : "CLMS_TILE";
    string clm_l  = (tx_l==3  || tx_l==7  || tx_l==11  || tx_l==16  || tx_l==22)  ? "CLMSS_TILE" : "CLMS_TILE";
    string clm_ll = (tx_ll==3 || tx_ll==7 || tx_ll==11 || tx_ll==16 || tx_ll==22) ? "CLMSS_TILE" : "CLMS_TILE";
    string clm_r  = (tx_r==3  || tx_r==7  || tx_r==11  || tx_r==16  || tx_r==22)  ? "CLMSS_TILE" : "CLMS_TILE";
    string clm_rr = (tx_rr==3 || tx_rr==7 || tx_rr==11 || tx_rr==16 || tx_rr==22) ? "CLMSS_TILE" : "CLMS_TILE";

    if((tx==tx_w) || (tx==tx_e))
    {
        connect( <pin MASK_DATA_CIN of <device *clm @[sx,sy]>> => <wire TIELO of <device *clm @[sx,sy]>> );
    }
    else if((tx==6) || (tx==10))
    {
        connect( <pin MASK_DATA_CIN of <device *clm @[sx,sy]>> => <wire MASK_DATA_COUT of <device *clm_ll @[sx_ll,sy]>> );
    }
    else if(tx==18)
    {
        connect( <pin MASK_DATA_CIN of <device *clm @[sx,sy]>> => <wire MASK_DATA_COUT of <device *clm_rr @[sx_rr,sy]>> );
    }
    else if(tx < tx_wl)
    {
        connect( <pin MASK_DATA_CIN of <device *clm @[sx,sy]>> => <wire MASK_DATA_COUT of <device *clm_l @[sx_l,sy]>> );
    }
    else
    {
        connect( <pin MASK_DATA_CIN of <device *clm @[sx,sy]>> => <wire MASK_DATA_COUT of <device *clm_r @[sx_r,sy]>> );
    }

};//end of function connect_clm_mask_data_7km

/****************************************************************************************************
*****************************************************************************************************/
function connect_drm_mask_input_7km
(
  unsigned int ty,
  unsigned int tx_wl,
  unsigned int ty_wl,

  string device_wl,
  string tile_name
)
{
  int sy = ty * NUM_GRID_Y;
  int sx_wl = tx_wl * NUM_GRID_X;
  int sy_wl = ty_wl * NUM_GRID_Y;

  string cram_ctrl = device_wl;
  string drm = tile_name;

  connect
  (
    <pin MASK_EN of <device *drm @[5*NUM_GRID_X + 3,sy]>>     => (ty <= ty_wl) ? <wire MASK_OD[0] of <device *cram_ctrl @[sx_wl,sy_wl] >>
                                                                               : <wire MASK_OU[0] of <device *cram_ctrl @[sx_wl,sy_wl] >>,

    <pin MASK_EN of <device *drm @[13*NUM_GRID_X + 3,sy]>>    => (ty <= ty_wl) ? <wire MASK_OD[1] of <device *cram_ctrl @[sx_wl,sy_wl] >>
                                                                               : <wire MASK_OU[1] of <device *cram_ctrl @[sx_wl,sy_wl] >>
  );
};//end of function connect_drm_mask_input_7km



