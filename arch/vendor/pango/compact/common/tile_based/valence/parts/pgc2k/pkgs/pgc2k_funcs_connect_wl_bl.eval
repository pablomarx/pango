        <pin PU_N of <device SRB_TILE @ [sx+5*NUM_GRID_X,sy]>>   => <wire PUP_REL_N_OUTL[4] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin GLOGEN of <device SRB_TILE @ [sx+5*NUM_GRID_X,sy]>> => <wire GLOGEN_OUTL[4] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,

        <pin ADDR of <device SRB_TILE @ [sx+6*NUM_GRID_X,sy]>>   => <wire ADDR[149:130] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
