        <pin ADDR of <device SRB_TILE @ [sx+10*NUM_GRID_X,sy]>>  => <wire ADDR[222:203] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin PU_N of <device SRB_TILE @ [sx+10*NUM_GRID_X,sy]>>   => <wire PUP_REL_N_OUTR[0] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin GLOGEN of <device SRB_TILE @ [sx+10*NUM_GRID_X,sy]>> => <wire GLOGEN_OUTR[0] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,

        <pin ADDR of <device SRB_TILE @ [sx+11*NUM_GRID_X,sy]>>  => <wire ADDR[248:229] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin PU_N of <device SRB_TILE @ [sx+11*NUM_GRID_X,sy]>>   => <wire PUP_REL_N_OUTR[1] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin GLOGEN of <device SRB_TILE @ [sx+11*NUM_GRID_X,sy]>> => <wire GLOGEN_OUTR[1] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,

        <pin ADDR of <device SRB_TILE @ [sx+12*NUM_GRID_X,sy]>>  => <wire ADDR[274:255] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin PU_N of <device SRB_TILE @ [sx+12*NUM_GRID_X,sy]>>   => <wire PUP_REL_N_OUTR[2] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin GLOGEN of <device SRB_TILE @ [sx+12*NUM_GRID_X,sy]>> => <wire GLOGEN_OUTR[2] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,

        <pin ADDR of <device SRB_TILE @ [sx+13*NUM_GRID_X,sy]>>  => <wire ADDR[300:281] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin PU_N of <device SRB_TILE @ [sx+13*NUM_GRID_X,sy]>>   => <wire PUP_REL_N_OUTR[3] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin GLOGEN of <device SRB_TILE @ [sx+13*NUM_GRID_X,sy]>> => <wire GLOGEN_OUTR[3] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,

        <pin ADDR of <device SRB_TILE @ [sx+14*NUM_GRID_X,sy]>>  => <wire ADDR[326:307] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin PU_N of <device SRB_TILE @ [sx+14*NUM_GRID_X,sy]>>   => <wire PUP_REL_N_OUTR[4] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
