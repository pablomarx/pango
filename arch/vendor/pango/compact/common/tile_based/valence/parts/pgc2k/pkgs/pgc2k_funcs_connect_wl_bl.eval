/********************************************************************************
********************************************************************************/
function connect_clm_mask_data
(
    unsigned int tx,
    unsigned int ty,
    unsigned int tx_wl
)
{
    unsigned int sx,sy;
    sx = tx * NUM_GRID_X + 3;
    sy = ty * NUM_GRID_X;

    int tx_b = tx - 1 ;
    int tx_f = tx + 1 ;


    int sx_b = tx_b * NUM_GRID_X + 3;
    int sx_f = tx_f * NUM_GRID_X + 3 ;


    if(tx==2 || tx==14)
    {
     connect
       (
        <pin MASK_DATA_CIN of <device CLMS_TILE @[sx,sy]>> => <wire TIELO of <device CLMS_TILE @[sx,sy]>>
       );
     } 
     else if(tx<tx_wl)
     {
     connect
     (
       <pin MASK_DATA_CIN of <device CLMS_TILE @[sx,sy]>> => <wire MASK_DATA_COUT of <device CLMS_TILE @[sx_b,sy]>>
     ); 
     } 
     else
     {
      connect
       (
         <pin MASK_DATA_CIN of <device CLMS_TILE @[sx,sy]>> => <wire MASK_DATA_COUT of <device CLMS_TILE @[sx_f,sy]>>
       ); 
     }
} // end of connect_clm_mask_data

/********************************************************************************
********************************************************************************/
function connect_iollr_wl_input
(
    unsigned int tx,
    unsigned int ty,
    unsigned int tx_wl,
    unsigned int ty_wl
)
{
    int sx_wl = tx_wl * NUM_GRID_X;
    int sy_wl = ty_wl * NUM_GRID_Y;

    int sx,sy;
    sx = tx * NUM_GRID_X + 3;
    sy = ty * NUM_GRID_Y;


  if(tx==1)
  {
    if(ty==PLL_TILE_Y)
    {
      connect
        (   
          <pin ADDR of <device PLL_TILE @ [sx,sy]>>     => <wire ADDR[25:22] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
          <pin GLOGEN of <device PLL_TILE @ [sx,sy]>>   => <wire GLOGEN_OUTL[0] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>
        );
    }
    else if(ty==CCS_TILE_Y)
    {
      connect
        (   
          <pin ADDR of <device CCS_TILE @ [sx,sy]>>         => <wire ADDR[25:22] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
          <pin GLOGEN_SFB of <device CCS_TILE @ [sx,sy]>>   => <wire GLOGEN_OUTL[0] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>

        );
    }
    else if(ty==ANALOG_2K_TILE_Y)
    {
      connect
        (   
          <pin ADDR of <device ANALOG_2K_TILE @ [sx,sy]>>     => <wire ADDR[25:22] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
          <pin GLOGEN of <device ANALOG_2K_TILE @ [sx,sy]>>   => <wire GLOGEN_OUTL[0] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>
//          <pin GWEN of <device ANALOG_2K_TILE @ [sx,sy]>>     => <wire GWEN_OUTL[0] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>
        );
    }
    else
    {
      connect
        (   
          <pin ADDR of <device IOLL_TILE @ [sx,sy]>>     => <wire ADDR[25:22] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
          <pin GLOGEN of <device IOLL_TILE @ [sx,sy]>>   => <wire GLOGEN_OUTL[0] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
          <pin GRS_N of <device IOLL_TILE @ [sx,sy]>>    => <wire GRS_N_OUTL[0] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>
        );
    }
  }
  else
  {
      connect
        ( 
          <pin ADDR of <device IOLR_TILE @ [sx,sy]>>   => <wire ADDR[356:353] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
          <pin GLOGEN of <device IOLR_TILE @ [sx,sy]>> => <wire GLOGEN_OUTR[5] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
          <pin GRS_N of <device IOLR_TILE @ [sx,sy]>>  => <wire GRS_N_OUTR[5] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>
        );
  }

}//end of connect_iollr_wl_input
