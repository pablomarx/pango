/********************************************************************************
********************************************************************************/
function connect_ccs_input
(
    unsigned int tx,
    unsigned int ty,
    unsigned int tx_pll,
    unsigned int ty_pll,
    unsigned int tx_iob,
    unsigned int ty_iob
)
{
    unsigned int sx,sy;
    sx = tx * NUM_GRID_X + 3;
    sy = ty * NUM_GRID_X;

    int sx_pll = tx_pll * NUM_GRID_X + 3;
    int sy_pll = ty_pll * NUM_GRID_Y;

    int sx_iob = tx_iob * NUM_GRID_X;
    int sy_iob = ty_iob * NUM_GRID_Y;



    connect
      (
        <pin PLL_LOCK of <device CCS_TILE @[sx,sy]>> => <wire LOCKOUT_CAS of <device PLL_TILE @[sx_pll,sy_pll]>>

      );
}


