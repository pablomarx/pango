        <pin GLOGEN of <device SRB_TILE @ [sx+15*NUM_GRID_X,sy]>> => <wire GLOGEN_OUTR[5] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>
      );

}//end of connect_srb_wl_input
/********************************************************************************
********************************************************************************/
function connect_ckeb_wl_input
(
    unsigned int ty,
    unsigned int tx_wl,
    unsigned int ty_wl
)
{
    int sx_wl = tx_wl * NUM_GRID_X;
    int sy_wl = ty_wl * NUM_GRID_Y;

    int sy;
    sy = ty * NUM_GRID_Y;


    connect
      (   
        <pin ADDR of <device IOCK_TILE @ [NUM_GRID_X+3,sy-NUM_GRID_Y]>>  => <wire ADDR[25:2] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin GRS_N of <device IOCK_TILE @ [NUM_GRID_X+3,sy-NUM_GRID_Y]>>  => <wire GRS_N_OUTL[0] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin GLOGEN of <device IOCK_TILE @ [NUM_GRID_X+3,sy-NUM_GRID_Y]>> => <wire GLOGEN_OUTL[0] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,

        <pin ADDR of <device CKEB_TILE @ [2*NUM_GRID_X,sy]>>   => <wire ADDR[51:26] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin ADDR of <device CKEB_TILE @ [3*NUM_GRID_X,sy]>>   => <wire ADDR[77:52] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin ADDR of <device CKEB_TILE @ [4*NUM_GRID_X,sy]>>   => <wire ADDR[103:78] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin ADDR of <device CKEB2_TILE @ [5*NUM_GRID_X,sy]>>   => <wire ADDR[129:104] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin ADDR of <device CKEB_TILE @ [6*NUM_GRID_X,sy]>>   => <wire ADDR[155:130] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin ADDR of <device CKEB_TILE @ [7*NUM_GRID_X,sy]>>   => <wire ADDR[181:156] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin ADDR[20:0] of <device USCM_TILE @ [9*NUM_GRID_X,sy+NUM_GRID_Y]>> => <wire ADDR[202:182] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin ADDR[21] of <device USCM_TILE @ [9*NUM_GRID_X,sy+NUM_GRID_Y]>>   => <wire ADDR[359] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin GLOGEN of <device USCM_TILE @ [9*NUM_GRID_X,sy+NUM_GRID_Y]>>     => <wire GLOGEN_OUTL[7] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,

        <pin ADDR of <device CKEB_TILE @ [10*NUM_GRID_X,sy]>>   => <wire ADDR[228:203] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin ADDR of <device CKEB_TILE @ [11*NUM_GRID_X,sy]>>   => <wire ADDR[254:229] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin ADDR of <device CKEB2_TILE @ [12*NUM_GRID_X,sy]>>   => <wire ADDR[280:255] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin ADDR of <device CKEB_TILE @ [13*NUM_GRID_X,sy]>>   => <wire ADDR[306:281] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin ADDR of <device CKEB_TILE @ [14*NUM_GRID_X,sy]>>   => <wire ADDR[332:307] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,

        <pin ADDR of <device IOCK_TILE @ [15*NUM_GRID_X+3,sy-NUM_GRID_Y]>>   => <wire ADDR[356:333] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin GRS_N of <device IOCK_TILE @ [15*NUM_GRID_X+3,sy-NUM_GRID_Y]>>  => <wire GRS_N_OUTR[5] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>,
        <pin GLOGEN of <device IOCK_TILE @ [15*NUM_GRID_X+3,sy-NUM_GRID_Y]>> => <wire GLOGEN_OUTR[5] of <device CRAM_CTRL_2K_TILE @ [sx_wl,sy_wl]>>
      );

}//end of connect_ckeb_wl_input
