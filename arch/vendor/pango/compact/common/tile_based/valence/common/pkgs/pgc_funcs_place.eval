    sprintf(strVar,"ntCLK_P_TM_BUF_%d_%d",sx,sy);
    &wire ntRef_ntCLK_P_TM_BUF = wire *strVar      /*pragma PAP_ARC_ITC_TYPE = "LOCAL"*/;

