                      CLK_OUTB        => ntRef_ntCLK_OUTB,     
                      TIEHI           => ntRef_ntTIEHI,
                      CLKOP_PLL_D_BUF => ntRef_ntCLKOP_PLL_D_BUF,
                      CLKOS_PLL_D_BUF => ntRef_ntCLKOS_PLL_D_BUF,
