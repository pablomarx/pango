timing iobr_tnl of IOBR
{
    if (CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI")
    {
        if (CP_IOB_MODE == "IBUF_DS" || CP_IOB_MODE == "IOBUF_MIPI")
        {
            operator V_INBUF_MIPI ibfmipi_ob_lp
                parameter map
                (
                    IOSTANDARD => "SLVS",
                    MIPI_MODE  => "LP_MIPI"  // RX OB_LP
                )
                port map
                (
                    IB => PAD,
                    I  => DIFFI_IN,
                    TS => 1'b1,
                    M  => MIPI_SW_DYN,
                    O  => DIFFI_OUT
                );
        }
    }
    else
    {
        operator V_BUF V_BUF
            port map
            (
                I         => PAD,
                Z         => DIFFI_OUT
            );
    }

////////////////////////////////////////////////////////////
////////////////////////IBUFR///////////////////////////////
////////////////////////////////////////////////////////////
    wire ntDI;
    if (CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI")
    {
        if (CP_IOB_MODE == "IBUF_DS" || CP_IOB_MODE == "IOBUF_MIPI")
        {
            operator V_INBUF_MIPI ibfmipi_hs
                parameter map
                (
                    IOSTANDARD => "SLVS",
                    MIPI_MODE  => "HS_MIPI"  // RX O_HS
                )
                port map
                (
                    I  => PAD,
                    IB => DIFFI_IN,
                    M  => MIPI_SW_DYN,
                    O  => ntDI
                );

            operator V_INBUF_MIPI ibfmipi_lp
                parameter map
                (
                    IOSTANDARD => "SLVS",
                    MIPI_MODE  => "LP_MIPI"  // RX O_LP
                )
                port map
                (
                    I  => PAD,
                    IB => DIFFI_IN,
                    TS => (CP_IOB_MODE == "IOBUF_MIPI") ? TS : 1'b1,
                    M  => MIPI_SW_DYN,
                    O  => DIN_MIPI_LP
                );
        }
    }
    else if (CP_LP_EN == "OFF")
    {
        if (CP_IOB_MODE == "IBUF" || CP_IOB_MODE == "IOBUF")
        {
            operator V_IBUF ibuf
                parameter map
                (
                    IOSTANDARD => CP_IO_STANDARD
                )
                port map
                (
                    I => PAD,
                    O => ntDI
                );
        }
        else if (CP_IOB_MODE == "IBUF_DS" || CP_IOB_MODE == "IOBUF_DS")
        {
            operator V_IBUFDS ibufds
                parameter map
                (
                    IOSTANDARD => CP_IO_STANDARD
                )
                port map
                (
                    I  => PAD,
                    IB => DIFFI_IN,
                    O  => ntDI
                );
        }
    }
    else
    {
        if (CP_IOB_MODE == "IBUF" || CP_IOB_MODE == "IOBUF")
        {
            operator V_INBUFE V_INBUFE
                parameter map
                (
                    IOSTANDARD => CP_IO_STANDARD
                )
                port map
                (
                    I       => PAD,
                    EN      => INBUF_DYN_DIS_N,
                    O       => ntDI
                );
        }
        else if (CP_IOB_MODE == "IBUF_DS" || CP_IOB_MODE == "IOBUF_DS")
        {
            operator V_INBUFEDS V_INBUFEDS
                parameter map
                (
                    IOSTANDARD => CP_IO_STANDARD
                )
                port map
                (
                    I       => PAD,
                    IB      => DIFFI_IN,
                    EN      => INBUF_DYN_DIS_N,
                    O       => ntDI
                );
        }
    }

    DIN <= ((CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI") && (CP_IOB_MODE == "IBUF_DS" || CP_IOB_MODE == "IOBUF_MIPI")) ? ntDI :
            (CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI") ? 1'b1 :
            (CP_IOB_MODE == "IBUF_DS" || CP_IOB_MODE == "IOBUF_DS" || CP_IOB_MODE == "IBUF" || CP_IOB_MODE == "IOBUF") ? ntDI : 1'b1;

////////////////////////////////////////////////////////////
////////////////////////////OBUFR///////////////////////////
////////////////////////////////////////////////////////////
    wire ntOBFOUT;
    if(CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI")
    {
        if (CP_IOB_MODE == "IOBUF_MIPI")
        {
            operator V_OUTBUFT_MIPI_SLAVE TX_I_LP
                parameter map
                (
                    IOSTANDARD => "SLVS"
                )
                port map
                (
                    IB => DO,
                    I  => 1'bz,
                    TB => TS,
                    M  => MIPI_SW_DYN,
                    OB => PAD
                );
            //DIFFO_OUT <= 1'bz;
        }
    }
    else if (CP_IOB_MODE == "OBUF")
    {
        operator V_OBUF obuf
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I => DO,
                O => PAD
            );
    }
    else if (CP_IOB_MODE == "OBUF_T" || CP_IOB_MODE == "IOBUF" || CP_IOB_MODE == "IOBUF_DS" || CP_IOB_MODE == "IOBUF_MIPI")
    {
        operator V_OBUFT obuft
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I => DO,
                T => TS,
                O => PAD
            );
    }
    else if (CP_IOB_MODE == "UNUSED")
    {
        //PAD <= 1'bz;
    }
};
