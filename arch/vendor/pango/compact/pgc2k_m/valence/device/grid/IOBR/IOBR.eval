tarted
grid
device IOBR
{
    // grid_property_def_start
    property
    (
        //DONT_USE means this iob unbond, and EMPTY means the layout dont have this iob ;NAME will assign PAD name later
        bool   DONT_USE = false,
        bool   EMPTY = false,
        string PAD_NAME = "",
        string PIN_NAME = "",
        string LOCATION = "",
        string HARDWARE_NAME = "",
        string IO_TYPE  = "",
        bool   SHARED_IO = false,
        bool   CLK_PAD   = false,
        string PLL_PAD   = "",        
        string DIFF_PAIR   = ""
    );
    // grid_property_def_end

    parameter
    (
        config string CP_IOB_MODE = "UNUSED",
        config string CP_PORT_STATUS = "PULLDW",
        config string CP_IO_STANDARD = "DEFAULT",
        config string CP_DIFFIN_TERM = "OFF",
        config string CP_W_VOLT_R_IN = "NOHYS",
        config string CP_DRV_STRENGTH = "8",
        config string CP_SLEW_MODE = "F",
        config string CP_HOT_SOCKET = "ON",
        config string CP_OPENDRAIN = "OFF",
        config string CP_LP_EN = "OFF",
        config string CP_VCCIO = "3.3"
    );
    port
    (
// configuration_body_def_on


    config input SC_PU = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_PU" */,

    config input SC_PD_N = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_PD_N" */,

    config input SC_CLAMP = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_CLAMP" */,

    config input SC_SIGEND_EN_N = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SIGEND_EN_N" */,

    config input SC_DIFFIN_EN = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_DIFFIN_EN" */,

    config input SC_VREF_EN = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_VREF_EN" */,

    config input SC_OUT_EN = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_OUT_EN" */,

    config input SC_MIXSIG_IN2 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_MIXSIG_IN2" */,

    config input SC_MIXSIG_IN1 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_MIXSIG_IN1" */,

    config input SC_RES_EN = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_RES_EN" */,

    config input SC_SLEW_N = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SLEW_N" */,

    config input SC_SRC1 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SRC1" */,

    config input SC_SRC2 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SRC2" */,

    config input SC_SRC3 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SRC3" */,

    config input SC_SINK1 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SINK1" */,

    config input SC_SINK2 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SINK2" */,

    config input SC_SINK3 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SINK3" */,

    config input SC_MIPI_EN = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_MIPI_EN" */,

    config input SC_INBUF_DYN_DIS = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_INBUF_DYN_DIS" */,

// configuration_body_def_end

        input TS = 1'b1,
        input DO = 1'b1,
        input DIFFI_IN = 1'b1,
        input MIPI_SW_DYN = 1'b1,
        input INBUF_DYN_DIS_N = 1'b1,
        output DIN,
        output DIN_MIPI_LP,
        output DIFFI_OUT,
        inout PAD
    );
};
//grid device end

//grid device structure netlist started
structure netlist of IOBR
{
    wire ntTS;
    wire ntDO;
    wire ntDIFFI_IN;
    wire ntMIPI_SW_DYN;
    wire ntINBUF_DYN_DIS_N;
    wire ntDIN;
    wire ntPAD;
    wire ntDIN_MIPI_LP;

    // the connection of all input
    ntTS               <=  TS;
    ntINBUF_DYN_DIS_N  <=  INBUF_DYN_DIS_N;
    ntDIFFI_IN         <=  DIFFI_IN;
    ntMIPI_SW_DYN      <=  MIPI_SW_DYN;
    ntDO               <=  DO;

    // the connection of all output
    DIN         <=  ntDIN;
    DIN_MIPI_LP <=  ntDIN_MIPI_LP;
    PAD         <=  ntPAD;
    DIFFI_OUT   <=  ntPAD;
    
    device IBUFR IBUFR
        parameter map
        (
            CP_IOB_MODE  =>  CP_IOB_MODE,
            CP_LP_EN    =>  CP_LP_EN,
            CP_IO_STANDARD  =>  CP_IO_STANDARD
        )
        port map
       (
            OUT              =>  ntDIN,
            OUT_MIPI_LP      =>  ntDIN_MIPI_LP,
            IN               =>  ntPAD,
            TS               =>  ntTS,
            INBUF_DYN_DIS_N  =>  ntINBUF_DYN_DIS_N,
            DIFFI_IN         =>  ntDIFFI_IN,
            MIPI_SW_DYN      =>  ntMIPI_SW_DYN
       );

    device OBUFR OBUFR
        parameter map
        (
            CP_IOB_MODE     =>  CP_IOB_MODE,
            CP_OPENDRAIN    =>  CP_OPENDRAIN,
            CP_IO_STANDARD  =>  CP_IO_STANDARD
        )
        port map
       (
            OUT          =>  ntPAD,
            DO           =>  ntDO,
            TS           =>  ntTS,
            MIPI_SW_DYN  =>  ntMIPI_SW_DYN
       );

};
//grid device structure netlist end

//grid device configure body started

configuration cfg of IOBR
{

//initial config body0 begin
    if(CP_PORT_STATUS == "PULLUP") //PULL-UP
    {
        SC_PU := 1'b1;
        SC_PD_N := 1'b1;
    }
    else if(CP_PORT_STATUS == "PULLDW")  //PULL_DOWN
    {
        SC_PU := 1'b0;
        SC_PD_N := 1'b0;
    }
    else if(CP_PORT_STATUS == "HOLD") //BUS_KPR
    {
        SC_PU := 1'b1;
        SC_PD_N := 1'b0;
    }
    else if(CP_PORT_STATUS == "NONE") // FLOATING
    {
        SC_PU := 1'b0;
        SC_PD_N := 1'b1;
    }
    else
    {
        error("illegal setting for CP_PORT_STATUS := %s",CP_PORT_STATUS);
    }
    if(CP_HOT_SOCKET == "ON")
    {
        SC_CLAMP  :=  1'b0;
    }
    else if(CP_HOT_SOCKET == "OFF")
    {
        SC_CLAMP  :=  1'b1;
    }
    else
    {
        error("illegal setting for CP_HOT_SOCKET := %s", CP_HOT_SOCKET);
    }
