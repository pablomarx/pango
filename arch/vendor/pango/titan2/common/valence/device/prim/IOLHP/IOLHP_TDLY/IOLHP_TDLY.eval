prim
device IOLHP_TDLY
{
    parameter
    (
        config string    CP_TDLY_EN       = "FALSE"     ,
        config string    CP_TDYN_EN       = "FALSE"     ,
        config bit       CP_TDLY_SET[7:0] = 8'b00000000
    );
    port
    (
        input            TDLY_LTH_EN_B                  ,
        input            TDLY_DYN_SET[7:0]              ,
        input            TDLY_SEL                       ,
        input            DI                             ,
        output           D_DLY
    );

}; // end of device IOLHP_TDLY


