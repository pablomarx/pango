symbol logsym of BKCLHP // pragma PAP_ARC_COLOR="210:45:45"
{
    // The bounding box
    generate ( 20 # 20 );
    //
    // Poloygon declaration
    //
    shape
    [ 0, 0 ]  ->  [   ,20 ] 
                            <
                                CAL_ACT_N           @[ , 10]
                            >
              ->  [ 20,   ]
              ->  [   , 0 ] 
              ->  [  0,   ]  ;

}; // symbol logsym of BKCLHP

