/*******************************************************************************

  Device    [IOBS]

  Author    []

  Abstract  [The structure netlist of IOBS is described in the similar fashion
             as in HDL. In unit instantiation statement, the formal pin may be
             connected to net which is declared explicitly.

             In Valence, the connection can also be made from formal pin to other
             pin or port, in which case Valence compiler shall create the net to
             fulfil the connection. The built-in naming convention is kicked in
             to name the automatically created net. However, the objects (net, instance)
             of this netlist are going to mapped from schematic. The compiler-generated
             names cause trouble for the mapping. Therefore, all wires are declared
             explicitly in the following description]

  Revision History:

********************************************************************************/
//grid device structure netlist started
structure netlist of IOBD
{
    //wire for input ports
    wire  ntIN_DYN_EN    ;
    wire  ntDO           ;
    wire  ntDO_NDRV      ;
    wire  ntDIFFI_IN     ;
    wire  ntMIPI_SW_DYN  ;
    
    //wire for output ports
    wire ntDIFFO_OUT     ;
    
    wire ntIBUFD         ;
    wire ntIBUFD_N       ;
    wire ntDIN_MIPI_LP   ;
    wire ntPAD           ;
    
    //assign port to wire
    ntIN_DYN_EN   <=    IN_DYN_EN  ;
    ntDO          <=    DO         ;
    ntDO_NDRV     <=    DO_NDRV    ;
    ntDIFFI_IN    <=    DIFFI_IN   ;
    ntMIPI_SW_DYN <=    MIPI_DYN_SW;
    
    PAD           <=   ntPAD         ;
    DIFFO_OUT     <=   ntDIFFO_OUT   ; 
    DIN           <=   ntIBUFD       ;
    DIN_N         <=   ntIBUFD_N     ;    
    DIN_MIPI_LP   <=   ntDIN_MIPI_LP ;
    
    
    device IBUFD IBUFD
    parameter map
    (
        CP_IOB_MODE    => CP_IOB_MODE     ,
        CP_DYN_IN      => CP_DYN_IN       ,
        CP_IO_STANDARD => CP_IO_STANDARD
    )
    port map 
    (    
        OUT         => ntIBUFD       ,
        OUT_N       => ntIBUFD_N     ,          
        MIPI_SW_DYN => ntMIPI_SW_DYN ,
        DIN_MIPI_LP => ntDIN_MIPI_LP ,
        DYN_EN      => ntIN_DYN_EN   ,
        IN          => ntPAD         ,
        DIFFI_IN    => ntDIFFI_IN          
    );
    
    device OBUFD OBUFD
    parameter map
    (
        CP_IOB_MODE    => CP_IOB_MODE   ,
        CP_PORT_STATUS => CP_PORT_STATUS,
        CP_OPENDRAIN   => CP_OPENDRAIN 
    )
    port map 
    (   
        OUT       => ntPAD       ,  
        DIFFO_OUT => ntDIFFO_OUT ,
        IN        => ntPAD       ,   
        DO        => ntDO        ,
        DO_NDRV   => ntDO_NDRV   
    );   
    
};// end of structure netlist of IOBD
//grid device structure netlist end
