            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I  => (CP_IOB_MODE == "IBUFHR_DS") ? ntDI : 1'b1,
                EN => IN_DYN_EN,
                O  => DIN
            );

            operator V_INBUFE INBUFE_LP
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I  => (CP_IOB_MODE == "IBUFHR_DS") ? ntDI_LP : 1'b1,
                EN => IN_DYN_EN,
                O  => DIN_MIPI_LP
            );
        }
        else
        {
            DIN <= (CP_IOB_MODE == "IBUFHR_DS") ? ntDI : 1'b1;
            DIN_MIPI_LP <= (CP_IOB_MODE == "IBUFHR_DS") ? ntDI_LP : 1'b1;
        }
    }
    else
    {
        if (CP_DYN_IN == "ON" && (CP_IOB_MODE == "IBUFHR" || CP_IOB_MODE == "IOBUFHR"))
        {
            operator V_INBUFE INBUFE
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I  => PAD,
                EN => IN_DYN_EN,
                O  => DIN
            );
        }
        else if (CP_DYN_IN == "ON" && (CP_IOB_MODE == "IBUFHR_DS" || CP_IOB_MODE == "IOBUFHR_DS"))
        {
            operator V_INBUFEDS INBUFEDS
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I  => PAD,
                IB => DIFFI_IN,
                EN => IN_DYN_EN,
                O  => DIN
            );
        }
        else if (CP_DYN_IN == "OFF" && (CP_IOB_MODE == "IBUFHR" || CP_IOB_MODE == "IOBUFHR"))
        {
            operator V_IBUF INBUF
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I  => PAD,
                O  => DIN
            );
        }
        else if (CP_DYN_IN == "OFF" && (CP_IOB_MODE == "IBUFHR_DS" || CP_IOB_MODE == "IOBUFHR_DS"))
        {
            operator V_IBUFDS INBUFDS
            parameter map
            (
                IOSTANDARD => CP_IO_STANDARD
            )
            port map
            (
                I  => PAD,
                IB => DIFFI_IN,
                O  => DIN
            );
        }
        else
        {
            DIN <= 1'b1;
        }
    }

    operator V_INV DIN_N_INV
    port map
    (
        I  => DIN,
        Z  => DIN_N
    );

    if (CP_IOB_MODE == "ADC")
    {
        ADC_IN <= PAD;
    }

    if (CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI")
    {
        if (CP_IOB_MODE == "OBUFHR_T" || CP_IOB_MODE == "OBUFHR_DS" || CP_IOB_MODE == "IBUFHR_DS")
        {
            operator V_OUTBUFT_MIPI obftmipi
            parameter map
            (
                IOSTANDARD => "SLVS"
            )
            port map
            (
                I  => DO,
                T  => ntTS,
                M  => MIPI_DYN_SW,
                O  => PAD,
                OB => DIFFO_OUT
            );
        }
    }
    else if (CP_IOB_MODE == "OBUFHR" || CP_IOB_MODE == "OBUFHR_T" || CP_IOB_MODE == "IOBUFHR")
    {
        operator V_OBUFT obuft
        parameter map
        (
            IOSTANDARD => CP_IO_STANDARD
        )
        port map
        (
            I => DO,
            T => ntTS,
            O => PAD
        );
    }
    else if (CP_IOB_MODE == "OBUFHR_DS" || CP_IOB_MODE == "OBUFHR_T_DS" || CP_IOB_MODE == "IOBUFHR_DS")
    {
        operator V_OBUFTDS obufds
        parameter map
        (
            IOSTANDARD => CP_IO_STANDARD
        )
        port map
        (
            I  => DO,
            T  => DO_NDRV,
            O  => PAD,
            OB => DIFFO_OUT
        );
    }
    else
    {
        DIFFO_OUT <= 1'bz;
    }
};
