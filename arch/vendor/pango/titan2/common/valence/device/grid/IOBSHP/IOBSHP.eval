structure netlist of IOBSHP
{
    //for inter connect net
    wire            ntOBUFO_IN              ;

    //for input port net
    wire            ntS11_ENINR_DYN         ;
    wire            ntS11_USR_DO_P          ;
    wire            ntS11_USR_DO_N          ;
    wire            ntDIFFO_IN              ;


    //for output port
    wire            ntDIN                   ;
    wire            ntDIFFI_OUT             ;
    wire            ntPAD                   ;


    //connect to input port
    ntS11_ENINR_DYN    <= S11_ENINR_DYN     ;
    ntS11_USR_DO_P     <= S11_USR_DO_P      ;
    ntS11_USR_DO_N     <= S11_USR_DO_N      ;
    ntDIFFO_IN         <= DIFFO_IN          ;


    //connect to output port
    DIN                <= ntDIN             ;
    DIFFI_OUT          <= ntDIFFI_OUT       ;
    PAD                <= ntPAD             ;


    device IOBHP_IBUFS IOBHP_IBUFS
    parameter map
    (
        CP_IOB_MODE    => CP_IOB_MODE       ,
        CP_DYN_IN      => CP_DYN_IN
    )
    port map
    (
        OUT            => ntDIN             ,
        IN_DYN_EN      => ntS11_ENINR_DYN   ,
        DIFFI_OUT      => ntDIFFI_OUT       ,
        IN             => ntPAD
    );

    device IOBHP_OBUFS IOBHP_OBUFS
    parameter map
    (
        CP_IOB_MODE    => CP_IOB_MODE       ,
        CP_OPENDRAIN   => CP_OPENDRAIN
    )
    port map
    (
        OUT            => ntOBUFO_IN        ,
        PAD            => ntPAD             ,
        DO_P           => ntS11_USR_DO_P    ,
        DO_N           => ntS11_USR_DO_N    ,
        IN             => ntPAD
    );

    device IOBHP_DIFFOSMUX IOBHP_DIFFOSMUX
    parameter map
    (
        CP_IOB_MODE    => CP_IOB_MODE       ,
        CP_PORT_STATUS => CP_PORT_STATUS
    )
    port map
    (
        OUT            => ntPAD             ,
        DO_P           => ntS11_USR_DO_P    ,
        DO_N           => ntS11_USR_DO_N    ,
        IN             => ntPAD             ,
        DIFFO_IN       => ntDIFFO_IN        ,
        OBUFO_IN       => ntOBUFO_IN
    );

};
