use pg2l_defs;

package pg2l_funcs_connect_io_bank
{
/****************************************************************************************************
*****************************************************************************************************/
function connect_iol_dqs_input
(
    unsigned int rx,
    unsigned int tx,
    unsigned int tx_pll,
    unsigned int loc_iol,         //loc_iol = 0,bottom half region;loc_iol = 1, top half region
    string device_iol
)
{
    int sx,sx_pll;
    sx = tx * NUM_GRID_X + 3;
    sx_pll = tx_pll * NUM_GRID_X;
    int sy_pll = (loc_iol==0) ? rx*NUM_TILES_REGION*NUM_GRID_Y : (rx*NUM_TILES_REGION+MID_TILE_REGION+1) * NUM_GRID_Y;  
    string pll = (loc_iol==0) ? "PPLL_TILE" : "GPLL_TILE";
    string iol = device_iol;

    int l_ty_dqs0[] = (loc_iol==0) ? {[13:24]} : {[38:50]};
    int l_ty_dqs1[] = (loc_iol==0) ? {[0:12]} : {[26:37]};
    int ty_dqs0_0 = (loc_iol==0) ? 19 : 44;
    int ty_dqs0_1 = (loc_iol==0) ? 20 : 45;
    int ty_dqs1_0 = (loc_iol==0) ? 7 : 32;
    int ty_dqs1_1 = (loc_iol==0) ? 8 : 33;

    int sy_dqs0_0 = (rx*NUM_TILES_REGION+ty_dqs0_0)* NUM_GRID_Y;
    int sy_dqs0_1 = (rx*NUM_TILES_REGION+ty_dqs0_1)* NUM_GRID_Y;
    int sy_dqs1_0 = (rx*NUM_TILES_REGION+ty_dqs1_0)* NUM_GRID_Y;
    int sy_dqs1_1 = (rx*NUM_TILES_REGION+ty_dqs1_1)* NUM_GRID_Y;

    connect
      (
        <pin DQSI00 of <device *pll @[sx_pll,sy_pll]>>   => <wire DI_TO_CLK of <device *iol @[sx,sy_dqs0_1]>>,
        <pin DQSIB00 of <device *pll @[sx_pll,sy_pll]>>  => <wire DI_TO_CLKB of <device *iol @[sx,sy_dqs0_1]>>,

        <pin DQSI01 of <device *pll @[sx_pll,sy_pll]>>   => <wire DI_TO_CLK of <device *iol @[sx,sy_dqs1_1]>>,
        <pin DQSIB01 of <device *pll @[sx_pll,sy_pll]>>  => <wire DI_TO_CLKB of <device *iol @[sx,sy_dqs1_1]>>
      );

   
    int ty,sy;
    foreach ty( l_ty_dqs0 )
    {
      sy = (rx*NUM_TILES_REGION+ty) * NUM_GRID_Y;
      if(ty==50)
      {
        connect
          (
            <pin IFIFO_WADDR[2] of <device *iol @[sx,sy]>>   => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin IFIFO_WADDR[1] of <device *iol @[sx,sy]>>   => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin IFIFO_WADDR[0] of <device *iol @[sx,sy]>>   => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin IFIFO_RADDR[2] of <device *iol @[sx,sy]>>   => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin IFIFO_RADDR[1] of <device *iol @[sx,sy]>>   => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin IFIFO_RADDR[0] of <device *iol @[sx,sy]>>   => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin DQSI_DELAY of <device *iol @[sx,sy]>>       => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin DQSI_DELAY_N of <device *iol @[sx,sy]>>     => <wire TIEHI of <device *iol @[sx,sy]>>,
            <pin WCLK of <device *iol @[sx,sy]>>             => <wire CLK_W0     of <device *pll @[sx_pll,sy_pll]>>,    //BUG: L2HW-85
            <pin WCLK_DELAY of <device *iol @[sx,sy]>>       => <wire CLK_W_DEL0 of <device *pll @[sx_pll,sy_pll]>>,    //BUG: L2HW-85

            <pin T_BYTEIN of <device *iol @[sx,sy]>>         => <wire TO1 of <device *pll @[sx_pll,sy_pll]>>,
            <pin TERM_BYTEIN of <device *iol @[sx,sy]>>      => <wire TERM1 of <device *pll @[sx_pll,sy_pll]>>,

            <pin VCC_REG of <device *iol @[sx,sy]>>          => <wire VCCREG0 of <device *pll @[sx_pll,sy_pll]>>,
            <pin PLLCLK0 of <device *iol @[sx,sy]>>          => <wire PHY_CLK0_2X_B_N of <device *pll @[sx_pll,sy_pll]>>,  
            <pin PLLCLK0_N of <device *iol @[sx,sy]>>        => <wire PHY_CLK0_2X_B_P of <device *pll @[sx_pll,sy_pll]>>,  
            <pin PLLCLK1 of <device *iol @[sx,sy]>>          => <wire SYSCLK0_B_N of <device *pll @[sx_pll,sy_pll]>> 
          );
      }
      else
      {
        connect
          (
            <pin IFIFO_WADDR of <device *iol @[sx,sy]>>   => <wire IFIFO_WADDR0 of <device *pll @[sx_pll,sy_pll]>>,
            <pin IFIFO_RADDR of <device *iol @[sx,sy]>>   => <wire IFIFO_RADDR0 of <device *pll @[sx_pll,sy_pll]>>,
            <pin DQSI_DELAY of <device *iol @[sx,sy]>>    => <wire CLK_DQSI_DEL0 of <device *pll @[sx_pll,sy_pll]>>,
            <pin DQSI_DELAY_N of <device *iol @[sx,sy]>>  => <wire CLK_DQSIB_DEL0 of <device *pll @[sx_pll,sy_pll]>>,
            <pin WCLK of <device *iol @[sx,sy]>>          => <wire CLK_W0 of <device *pll @[sx_pll,sy_pll]>>,
            <pin WCLK_DELAY of <device *iol @[sx,sy]>>    => <wire CLK_W_DEL0 of <device *pll @[sx_pll,sy_pll]>>,

            <pin T_BYTEIN of <device *iol @[sx,sy]>>       => (ty==ty_dqs0_0) ? <wire TO0 of <device *pll @[sx_pll,sy_pll]>> :
                                                              (ty==ty_dqs0_1) ? <wire TO0 of <device *pll @[sx_pll,sy_pll]>> 
                                                                              : <wire TO1 of <device *pll @[sx_pll,sy_pll]>>,
            <pin TERM_BYTEIN of <device *iol @[sx,sy]>>    => (ty==ty_dqs0_0) ? <wire TERM0 of <device *pll @[sx_pll,sy_pll]>> :
                                                              (ty==ty_dqs0_1) ? <wire TERM0 of <device *pll @[sx_pll,sy_pll]>> 
                                                                              : <wire TERM1 of <device *pll @[sx_pll,sy_pll]>>,

            <pin VCC_REG of <device *iol @[sx,sy]>>       => <wire VCCREG0 of <device *pll @[sx_pll,sy_pll]>>,
            <pin PLLCLK0 of <device *iol @[sx,sy]>>       => <wire PHY_CLK0_2X_B_N of <device *pll @[sx_pll,sy_pll]>>,  
            <pin PLLCLK0_N of <device *iol @[sx,sy]>>     => <wire PHY_CLK0_2X_B_P of <device *pll @[sx_pll,sy_pll]>>,  
            <pin PLLCLK1 of <device *iol @[sx,sy]>>       => <wire SYSCLK0_B_N of <device *pll @[sx_pll,sy_pll]>> 
          );
      }
    }

    foreach ty( l_ty_dqs1 )
    {
      sy = (rx*NUM_TILES_REGION+ty) * NUM_GRID_Y;
      if(ty==0)
      {
        connect
          (
            <pin IFIFO_WADDR[2] of <device *iol @[sx,sy]>>   => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin IFIFO_WADDR[1] of <device *iol @[sx,sy]>>   => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin IFIFO_WADDR[0] of <device *iol @[sx,sy]>>   => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin IFIFO_RADDR[2] of <device *iol @[sx,sy]>>   => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin IFIFO_RADDR[1] of <device *iol @[sx,sy]>>   => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin IFIFO_RADDR[0] of <device *iol @[sx,sy]>>   => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin DQSI_DELAY of <device *iol @[sx,sy]>>       => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin DQSI_DELAY_N of <device *iol @[sx,sy]>>     => <wire TIEHI of <device *iol @[sx,sy]>>,
            <pin WCLK of <device *iol @[sx,sy]>>             => <wire CLK_W1 of <device *pll @[sx_pll,sy_pll]>>,    //BUG: L2HW-85
            <pin WCLK_DELAY of <device *iol @[sx,sy]>>       => <wire CLK_W_DEL1 of <device *pll @[sx_pll,sy_pll]>>,    //BUG: L2HW-85
            <pin VCC_REG of <device *iol @[sx,sy]>>          => <wire VCCREG1 of <device *pll @[sx_pll,sy_pll]>>,

            <pin T_BYTEIN of <device *iol @[sx,sy]>>         => <wire TO3 of <device *pll @[sx_pll,sy_pll]>>,
            <pin TERM_BYTEIN of <device *iol @[sx,sy]>>      => <wire TERM3 of <device *pll @[sx_pll,sy_pll]>>,

            <pin PLLCLK0 of <device *iol @[sx,sy]>>          => <wire PHY_CLK1_2X_B_N of <device *pll @[sx_pll,sy_pll]>>,  
            <pin PLLCLK0_N of <device *iol @[sx,sy]>>        => <wire PHY_CLK1_2X_B_P of <device *pll @[sx_pll,sy_pll]>>,  
            <pin PLLCLK1 of <device *iol @[sx,sy]>>          => <wire SYSCLK1_B_N of <device *pll @[sx_pll,sy_pll]>> 
          );
      }
      else
      {
        connect
          (
            <pin IFIFO_WADDR of <device *iol @[sx,sy]>>   => <wire IFIFO_WADDR1 of <device *pll @[sx_pll,sy_pll]>>,
            <pin IFIFO_RADDR of <device *iol @[sx,sy]>>   => <wire IFIFO_RADDR1 of <device *pll @[sx_pll,sy_pll]>>,
            <pin DQSI_DELAY of <device *iol @[sx,sy]>>    => <wire CLK_DQSI_DEL1 of <device *pll @[sx_pll,sy_pll]>>,
            <pin DQSI_DELAY_N of <device *iol @[sx,sy]>>  => <wire CLK_DQSIB_DEL1 of <device *pll @[sx_pll,sy_pll]>>,
            <pin WCLK of <device *iol @[sx,sy]>>          => <wire CLK_W1 of <device *pll @[sx_pll,sy_pll]>>,
            <pin WCLK_DELAY of <device *iol @[sx,sy]>>    => <wire CLK_W_DEL1 of <device *pll @[sx_pll,sy_pll]>>,
            <pin VCC_REG of <device *iol @[sx,sy]>>       => <wire VCCREG1 of <device *pll @[sx_pll,sy_pll]>>,

            <pin T_BYTEIN of <device *iol @[sx,sy]>>      => (ty==ty_dqs1_0) ? <wire TO2 of <device *pll @[sx_pll,sy_pll]>> :
                                                             (ty==ty_dqs1_1) ? <wire TO2 of <device *pll @[sx_pll,sy_pll]>> 
                                                                             : <wire TO3 of <device *pll @[sx_pll,sy_pll]>>,                                                                                 
            <pin TERM_BYTEIN of <device *iol @[sx,sy]>>   => (ty==ty_dqs1_0) ? <wire TERM2 of <device *pll @[sx_pll,sy_pll]>> :
                                                             (ty==ty_dqs1_1) ? <wire TERM2 of <device *pll @[sx_pll,sy_pll]>> 
                                                                             : <wire TERM3 of <device *pll @[sx_pll,sy_pll]>>,

            <pin PLLCLK0 of <device *iol @[sx,sy]>>       => <wire PHY_CLK1_2X_B_N of <device *pll @[sx_pll,sy_pll]>>,  
            <pin PLLCLK0_N of <device *iol @[sx,sy]>>     => <wire PHY_CLK1_2X_B_P of <device *pll @[sx_pll,sy_pll]>>,  
            <pin PLLCLK1 of <device *iol @[sx,sy]>>       => <wire SYSCLK1_B_N of <device *pll @[sx_pll,sy_pll]>> 
          );
      }
    }
}
/****************************************************************************************************
*****************************************************************************************************/
function connect_iol_cascade
(
    unsigned int tx,
    unsigned int rx,
    unsigned int loc_iol,       //loc_srb = 0,bottom half region ; loc_srb = 1,top half region
    string device_iol
)
{
    int sx = tx * NUM_GRID_X + 3;
    int l_ty[] = (loc_iol==0) ? {[0:24]} : {[26:50]};
    string iol = device_iol;

    int ty,sy,sy_source;
    foreach ty( l_ty )
    {
      sy = (rx*NUM_TILES_REGION+ty) * NUM_GRID_Y;
      if(loc_iol==0)
      {
        sy_source = (ty%2==0) ? sy-NUM_GRID_Y : sy+NUM_GRID_Y;
      }
      else
      {
        sy_source = (ty%2==1) ? sy-NUM_GRID_Y : sy+NUM_GRID_Y;
      }
      if((ty==0)||(ty==50))
      {
        connect
          (
            <pin DO_CAS_IN of <device *iol@[sx,sy]>>    => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin TO_CAS_IN of <device *iol@[sx,sy]>>    => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin TERM_CAS_IN of <device *iol@[sx,sy]>>  => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin ISHIFTIN0 of <device *iol@[sx,sy]>>    => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin ISHIFTIN1 of <device *iol@[sx,sy]>>    => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin OSHIFTIN0 of <device *iol@[sx,sy]>>    => <wire TIELO of <device *iol @[sx,sy]>>,
            <pin OSHIFTIN1 of <device *iol@[sx,sy]>>    => <wire TIELO of <device *iol @[sx,sy]>>
          );
        
      }
      else
      {
        connect
          (
            <pin DO_CAS_IN of <device *iol@[sx,sy]>>    => <wire DO_CAS_OUT of <device *iol @[sx,sy_source]>>,
            <pin TO_CAS_IN of <device *iol@[sx,sy]>>    => <wire TO_CAS_OUT of <device *iol @[sx,sy_source]>>,
            <pin TERM_CAS_IN of <device *iol@[sx,sy]>>  => <wire TERM_CAS_OUT of <device *iol @[sx,sy_source]>>,
            <pin ISHIFTIN0 of <device *iol@[sx,sy]>>    => <wire ISHIFTOUT0 of <device *iol @[sx,sy_source]>>,
            <pin ISHIFTIN1 of <device *iol@[sx,sy]>>    => <wire ISHIFTOUT1 of <device *iol @[sx,sy_source]>>,
            <pin OSHIFTIN0 of <device *iol@[sx,sy]>>    => <wire OSHIFTOUT0 of <device *iol @[sx,sy_source]>>,
            <pin OSHIFTIN1 of <device *iol@[sx,sy]>>    => <wire OSHIFTOUT1 of <device *iol @[sx,sy_source]>>
          );
        
      }  
    }
    
}
/****************************************************************************************************
*****************************************************************************************************/
function connect_io_data
(
    unsigned int tx,
    unsigned int rx,
    string device_iob,
    string device_iol
)
{
    unsigned int sx_iol;
    int sx = tx * NUM_GRID_X;
    sx_iol = (tx==0) ? sx+15 : sx-9;    
    int ty = rx * NUM_TILES_REGION;
    int sy = ty * NUM_GRID_Y;
    string iob = device_iob;
    string iol = device_iol;
 
    connect
      (
         <pin DO_PDRV[49] of <device *iob @[sx,sy]>>          => <wire DO_P of <device *iol @ [sx_iol,ty*NUM_GRID_Y]>>,
         <pin DO_NDRV[49] of <device *iob @[sx,sy]>>          => <wire DO_N of <device *iol @ [sx_iol,ty*NUM_GRID_Y]>>,
         <pin S09_TERM_DYN_EN[49] of <device *iob @[sx,sy]>>  => <wire TERM of <device *iol @ [sx_iol,ty*NUM_GRID_Y]>>,
         <pin MIPI_DYN_SW[49] of <device *iob @[sx,sy]>>      => <wire MIPI_SW_DYN_O of <device *iol @ [sx_iol,ty*NUM_GRID_Y]>>,
         <pin IN_DYN_EN[49] of <device *iob @[sx,sy]>>        => <wire INBUF_DYN_DIS_N_O of <device *iol @ [sx_iol,ty*NUM_GRID_Y]>>,

         <pin DO_PDRV[48] of <device *iob @[sx,sy]>>          => <wire DO_P of <device *iol @ [sx_iol,(ty+1)*NUM_GRID_Y]>>,
         <pin DO_NDRV[48] of <device *iob @[sx,sy]>>          => <wire DO_N of <device *iol @ [sx_iol,(ty+1)*NUM_GRID_Y]>>,
         <pin S09_TERM_DYN_EN[48] of <device *iob @[sx,sy]>>  => <wire TERM of <device *iol @ [sx_iol,(ty+1)*NUM_GRID_Y]>>,
         <pin MIPI_DYN_SW[48] of <device *iob @[sx,sy]>>      => <wire MIPI_SW_DYN_O of <device *iol @ [sx_iol,(ty+1)*NUM_GRID_Y]>>,
         <pin IN_DYN_EN[48] of <device *iob @[sx,sy]>>        => <wire INBUF_DYN_DIS_N_O of <device *iol @ [sx_iol,(ty+1)*NUM_GRID_Y]>>,

         <pin DO_PDRV[47] of <device *iob @[sx,sy]>>          => <wire DO_P of <device *iol @ [sx_iol,(ty+2)*NUM_GRID_Y]>>,
         <pin DO_NDRV[47] of <device *iob @[sx,sy]>>          => <wire DO_N of <device *iol @ [sx_iol,(ty+2)*NUM_GRID_Y]>>,
         <pin S09_TERM_DYN_EN[47] of <device *iob @[sx,sy]>>  => <wire TERM of <device *iol @ [sx_iol,(ty+2)*NUM_GRID_Y]>>,
         <pin MIPI_DYN_SW[47] of <device *iob @[sx,sy]>>      => <wire MIPI_SW_DYN_O of <device *iol @ [sx_iol,(ty+2)*NUM_GRID_Y]>>,
         <pin IN_DYN_EN[47] of <device *iob @[sx,sy]>>        => <wire INBUF_DYN_DIS_N_O of <device *iol @ [sx_iol,(ty+2)*NUM_GRID_Y]>>,

         <pin DO_PDRV[46] of <device *iob @[sx,sy]>>          => <wire DO_P of <device *iol @ [sx_iol,(ty+3)*NUM_GRID_Y]>>,
         <pin DO_NDRV[46] of <device *iob @[sx,sy]>>          => <wire DO_N of <device *iol @ [sx_iol,(ty+3)*NUM_GRID_Y]>>,
         <pin S09_TERM_DYN_EN[46] of <device *iob @[sx,sy]>>  => <wire TERM of <device *iol @ [sx_iol,(ty+3)*NUM_GRID_Y]>>,
         <pin MIPI_DYN_SW[46] of <device *iob @[sx,sy]>>      => <wire MIPI_SW_DYN_O of <device *iol @ [sx_iol,(ty+3)*NUM_GRID_Y]>>,
         <pin IN_DYN_EN[46] of <device *iob @[sx,sy]>>        => <wire INBUF_DYN_DIS_N_O of <device *iol @ [sx_iol,(ty+3)*NUM_GRID_Y]>>,

         <pin DO_PDRV[45] of <device *iob @[sx,sy]>>          => <wire DO_P of <device *iol @ [sx_iol,(ty+4)*NUM_GRID_Y]>>,
         <pin DO_NDRV[45] of <device *iob @[sx,sy]>>          => <wire DO_N of <device *iol @ [sx_iol,(ty+4)*NUM_GRID_Y]>>,
         <pin S09_TERM_DYN_EN[45] of <device *iob @[sx,sy]>>  => <wire TERM of <device *iol @ [sx_iol,(ty+4)*NUM_GRID_Y]>>,
         <pin MIPI_DYN_SW[45] of <device *iob @[sx,sy]>>      => <wire MIPI_SW_DYN_O of <device *iol @ [sx_iol,(ty+4)*NUM_GRID_Y]>>,
         <pin IN_DYN_EN[45] of <device *iob @[sx,sy]>>        => <wire INBUF_DYN_DIS_N_O of <device *iol @ [sx_iol,(ty+4)*NUM_GRID_Y]>>,

         <pin DO_PDRV[44] of <device *iob @[sx,sy]>>          => <wire DO_P of <device *iol @ [sx_iol,(ty+5)*NUM_GRID_Y]>>,
         <pin DO_NDRV[44] of <device *iob @[sx,sy]>>          => <wire DO_N of <device *iol @ [sx_iol,(ty+5)*NUM_GRID_Y]>>,
         <pin S09_TERM_DYN_EN[44] of <device *iob @[sx,sy]>>  => <wire TERM of <device *iol @ [sx_iol,(ty+5)*NUM_GRID_Y]>>,
         <pin MIPI_DYN_SW[44] of <device *iob @[sx,sy]>>      => <wire MIPI_SW_DYN_O of <device *iol @ [sx_iol,(ty+5)*NUM_GRID_Y]>>,
         <pin IN_DYN_EN[44] of <device *iob @[sx,sy]>>        => <wire INBUF_DYN_DIS_N_O of <device *iol @ [sx_iol,(ty+5)*NUM_GRID_Y]>>,

         <pin DO_PDRV[43] of <device *iob @[sx,sy]>>          => <wire DO_P of <device *iol @ [sx_iol,(ty+6)*NUM_GRID_Y]>>,
         <pin DO_NDRV[43] of <device *iob @[sx,sy]>>          => <wire DO_N of <device *iol @ [sx_iol,(ty+6)*NUM_GRID_Y]>>,
         <pin S09_TERM_DYN_EN[43] of <device *iob @[sx,sy]>>  => <wire TERM of <device *iol @ [sx_iol,(ty+6)*NUM_GRID_Y]>>,
         <pin MIPI_DYN_SW[43] of <device *iob @[sx,sy]>>      => <wire MIPI_SW_DYN_O of <device *iol @ [sx_iol,(ty+6)*NUM_GRID_Y]>>,
         <pin IN_DYN_EN[43] of <device *iob @[sx,sy]>>        => <wire INBUF_DYN_DIS_N_O of <device *iol @ [sx_iol,(ty+6)*NUM_GRID_Y]>>,

         <pin DO_PDRV[42] of <device *iob @[sx,sy]>>          => <wire DO_P of <device *iol @ [sx_iol,(ty+7)*NUM_GRID_Y]>>,
         <pin DO_NDRV[42] of <device *iob @[sx,sy]>>          => <wire DO_N of <device *iol @ [sx_iol,(ty+7)*NUM_GRID_Y]>>,
         <pin S09_TERM_DYN_EN[42] of <device *iob @[sx,sy]>>  => <wire TERM of <device *iol @ [sx_iol,(ty+7)*NUM_GRID_Y]>>,
         <pin MIPI_DYN_SW[42] of <device *iob @[sx,sy]>>      => <wire MIPI_SW_DYN_O of <device *iol @ [sx_iol,(ty+7)*NUM_GRID_Y]>>,
         <pin IN_DYN_EN[42] of <device *iob @[sx,sy]>>        => <wire INBUF_DYN_DIS_N_O of <device *iol @ [sx_iol,(ty+7)*NUM_GRID_Y]>>,

         <pin DO_PDRV[41] of <device *iob @[sx,sy]>>          => <wire DO_P of <device *iol @ [sx_iol,(ty+8)*NUM_GRID_Y]>>,
         <pin DO_NDRV[41] of <device *iob @[sx,sy]>>          => <wire DO_N of <device *iol @ [sx_iol,(ty+8)*NUM_GRID_Y]>>,
         <pin S09_TERM_DYN_EN[41] of <device *iob @[sx,sy]>>  => <wire TERM of <device *iol @ [sx_iol,(ty+8)*NUM_GRID_Y]>>,
         <pin MIPI_DYN_SW[41] of <device *iob @[sx,sy]>>      => <wire MIPI_SW_DYN_O of <device *iol @ [sx_iol,(ty+8)*NUM_GRID_Y]>>,
         <pin IN_DYN_EN[41] of <device *iob @[sx,sy]>>        => <wire INBUF_DYN_DIS_N_O of <device *iol @ [sx_iol,(ty+8)*NUM_GRID_Y]>>,

         <pin DO_PDRV[40] of <device *iob @[sx,sy]>>          => <wire DO_P of <device *iol @ [sx_iol,(ty+9)*NUM_GRID_Y]>>,
