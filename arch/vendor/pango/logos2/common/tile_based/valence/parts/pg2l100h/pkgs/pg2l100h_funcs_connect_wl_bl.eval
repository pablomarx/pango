          tile_w_1 = tile_w;
          ckeb_w = ((tile_w==tx_ckeb2_l)||(tile_w==tx_ckeb2_r)) ? "CKEB2_TILE" : "CKEB_TILE";
        }
      }

      int site_e_1 = tile_e_1 * NUM_GRID_X;
      int site_w_1 = tile_w_1 * NUM_GRID_X;

