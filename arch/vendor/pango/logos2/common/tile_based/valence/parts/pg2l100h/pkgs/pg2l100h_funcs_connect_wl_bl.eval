        rst_n =((tx>=tx_l1)&&(tx<=tx_bl)) ? "RST_N_OL_R0[0]" :
               ((tx>=tx_l2)&&(tx<tx_l1))  ? "RST_N_OL_R0[1]" :
               ((tx>=tx_l3)&&(tx<tx_l2))  ? "RST_N_OL_R0[2]" :
               ((tx>=tx_l4)&&(tx<tx_l3))  ? "RST_N_OL_R0[3]" :
                             (tx<tx_l4)   ? "RST_N_OL_R0[4]" :
               ((tx>=tx_bl)&&(tx<=tx_r1)) ? "RST_N_OR_R0[0]" :
