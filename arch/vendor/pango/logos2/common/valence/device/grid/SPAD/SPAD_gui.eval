symbol logsym of SPAD // pragma PAP_ARC_COLOR="210:45:45"
{
    // The bounding box
    generate ( 20 # 20 );
    //
    // Poloygon declaration
    //
    shape
    [ 0, 0 ]  ->  [   ,20 ] <
                                SPAD             @[ , 10]
                            >
              ->  [ 20,   ]
              ->  [   , 0 ] <
                                PAD            @[ , 10]
                            >
              ->  [  0,   ]  ;

}; // symbol logsym of SPAD

