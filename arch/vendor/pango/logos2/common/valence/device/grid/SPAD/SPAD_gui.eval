symbol fpsym of SPAD  // pragma PAP_ARC_COLOR="210:45:45"
{
    // The bounding box
    generate ( 40 # 50 );

    shape
    [ 0, 0 ]  ->  [ 40,  0]
              ->  [ 40, 50]
              ->  [  0, 50]
              ->  [  0,  0];


    "SPAD" @ [20,25];
};// symbol logsym of SPAD

