*****************************************************************

  Device    [IOBS]

  Author    [clwang]

  Abstract  [IOBS  is a grid device of the IOBs which are placed on the
            TOP/RIGHT/BOTTOM of Arch.]

  Revision History:

********************************************************************************/
//grid device started
grid
device IOBS
{
    // grid_property_def_start
    property
    (
        //DONT_USE means this iob unbond, and EMPTY means the layout dont have this iob ;NAME will assign PAD name later
        bool   DONT_USE = false,
        bool   EMPTY = false,
        string PAD_NAME = "",
        string PIN_NAME = "",
        string LOCATION = "",
        string HARDWARE_NAME = "",
        string IO_TYPE  = "",
        bool   SHARED_IO = false,
        bool   CLK_PAD   = false,
        string PLL_PAD   = "",
        string DIFF_PAIR = ""
    );
    // grid_property_def_end

    parameter
    (
        config string CP_IOB_MODE = "UNUSED",
        config string CP_PORT_STATUS = "NONE",
        config string CP_IO_STANDARD = "DEFAULT",
        config string CP_DDRIN_TERM = "OFF",
        config string CP_DYN_TERM = "OFF",
        config string CP_IN_MAG = "UNUSED",
        config string CP_HYS = "OFF",
        config string CP_DRV_STRENGTH = "12",
        config string CP_SLEW_MODE = "F",
        config string CP_OPENDRAIN = "OFF",
        config string CP_TERM_VAL = "50",
        config string CP_DYN_IN = "OFF",
        config string CP_VCCIO = "3.3"
    );
    port
    (
// configuration_body_def_on


    config input SC_COMP_IN[1:0] = 2'h0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_COMP_IN[1],SC_COMP_IN[0]" */,

    config input SC_DRVN[2:0] = 3'h0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_DRVN[2],SC_DRVN[1],SC_DRVN[0]" */,

    config input SC_DRVP[2:0] = 3'h0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_DRVP[2],SC_DRVP[1],SC_DRVP[0]" */,

    config input SC_ADC_IN = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_ADC_IN" */,

    config input SC_HYS_SINGLE = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_HYS_SINGLE" */,

    config input SC_INTVREF = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_INTVREF" */,

    config input SC_IN_DYN_EN = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_IN_DYN_EN" */,

    config input SC_MIPI_EN = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_MIPI_EN" */,

    config input SC_OUT_EN = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_OUT_EN" */,

    config input SC_PD = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_PD" */,

    config input SC_PU = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_PU" */,

    config input SC_SIGEND_IN = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SIGEND_IN" */,

    config input SC_SINGIN12_15 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SINGIN12_15" */,

    config input SC_SINGIN18 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SINGIN18" */,

    config input SC_SINGIN25_33 = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SINGIN25_33" */,

    config input SC_SINGLE_VBIAS[1:0] = 2'h0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SINGLE_VBIAS[1],SC_SINGLE_VBIAS[0]" */,

    config input SC_SLEW_N[2:0] = 3'h0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SLEW_N[2],SC_SLEW_N[1],SC_SLEW_N[0]" */,

    config input SC_SLEW_P[2:0] = 3'h0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_SLEW_P[2],SC_SLEW_P[1],SC_SLEW_P[0]" */,

    config input SC_TERM_DDR = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_TERM_DDR" */,

    config input SC_TERM_ADJ[3:0] = 4'h0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_TERM_ADJ[3],SC_TERM_ADJ[2],SC_TERM_ADJ[1],SC_TERM_ADJ[0]" */,

    config input SC_TERM_DYN_EN = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_TERM_DYN_EN" */,

    config input SC_COMPIN_HYS = 1'b0
    /* pragma  PAP_CFG_BIT_NAME  = "SC_COMPIN_HYS" */,

// configuration_body_def_end

        input DO = 1'b1,
        input DO_NDRV = 1'b1,
        input DIFFO_IN = 1'b1,
        input IN_DYN_EN = 1'b1,
        output DIN,
        output DIFFI_OUT,
        inout ADC_IN,
        inout PAD
    );
};
//grid device end
/*******************************************************************************

  Device    [IOBS]

  Author    []

  Abstract  [The structure netlist of IOBS is described in the similar fashion
             as in HDL. In unit instantiation statement, the formal pin may be
             connected to net which is declared explicitly.

             In Valence, the connection can also be made from formal pin to other
             pin or port, in which case Valence compiler shall create the net to
             fulfil the connection. The built-in naming convention is kicked in
             to name the automatically created net. However, the objects (net, instance)
             of this netlist are going to mapped from schematic. The compiler-generated
             names cause trouble for the mapping. Therefore, all wires are declared
             explicitly in the following description]

  Revision History:

********************************************************************************/
//grid device structure netlist started
structure netlist of IOBS
{
    // input net
    wire ntDO;
    wire ntDO_NDRV;
    wire ntDIFFO_IN;
    //wire ntMIPI_SW_DYN;
    wire ntIN_DYN_EN;
    
    // inout net
    wire ntPAD;
    
    // 
    wire ntIBUFS;
    wire ntOBUFS;
    wire ntDIFFI_OUT;
    
    // assign net to port
    ntDO          <=  DO          ;
    ntDO_NDRV     <=  DO_NDRV     ;
    ntDIFFO_IN    <=  DIFFO_IN    ;
    //ntMIPI_SW_DYN <=  MIPI_SW_DYN ;
    
    PAD           <=  ntPAD       ;
    DIN           <=  ntIBUFS     ;
    //DIFFI_OUT     <=  ntPAD       ;
    DIFFI_OUT     <=  ntDIFFI_OUT ;
    ntIN_DYN_EN   <= IN_DYN_EN;
    
    
    
    device IBUFS IBUFR
        parameter map
        (
            CP_IOB_MODE    => CP_IOB_MODE,
            CP_DYN_IN      => CP_DYN_IN  ,
            CP_IO_STANDARD => CP_IO_STANDARD
        )
        port map 
        (
            OUT       => ntIBUFS  , 
            IN_DYN_EN => ntIN_DYN_EN,  
            IN        => ntPAD      
        );     
    
    device OBUFS OBUFR
       parameter map
       ( 
           CP_IOB_MODE    => CP_IOB_MODE    ,     
           CP_OPENDRAIN   => CP_OPENDRAIN      
       )
       port map
       (   
           OUT         => ntOBUFS       ,  
           PAD         => ntPAD         , 
           DIFFI_OUT   => ntDIFFI_OUT   , 
           DO_NDRV     => ntDO_NDRV     ,              
           DO          => ntDO          
       ); 
    device DIFFOSMUX DIFFOSMUX
       parameter map
       ( 
           CP_IO_STANDARD => CP_IO_STANDARD ,
           CP_IOB_MODE    => CP_IOB_MODE    ,
           CP_PORT_STATUS => CP_PORT_STATUS 
       )
       port map
       ( 
           OUT         => ntPAD          ,
           DO          => ntDO           ,
           DO_NDRV     => ntDO_NDRV      ,
           IN          => ntPAD        ,
           DIFFO_IN    => ntDIFFO_IN     ,
           OBUFO_IN    => ntOBUFS       
       );
         
}; // end of struct netlist of IOBS
//grid device structure netlist end

