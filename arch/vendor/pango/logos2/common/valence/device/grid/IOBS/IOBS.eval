//grid device configure body end

timing iobs_tnl of IOBS
{
    wire ntTS;
    ntTS <= DO_NDRV;

    wire ntDI;
    if (CP_DYN_IN == "ON")
    {
        operator V_INBUFE INBUFE
        parameter map
        (
            IOSTANDARD => CP_IO_STANDARD
        )
        port map
        (
            I  => PAD,
            EN => IN_DYN_EN,
            O  => ntDI
        );
    }
    else
    {
        operator V_IBUF ibuf
        parameter map
        (
            IOSTANDARD => CP_IO_STANDARD
        )
        port map
        (
            I => PAD,
            O => ntDI
        );
    }

    DIN <= (CP_IOB_MODE == "IBUFHR" || CP_IOB_MODE == "IOBUFHR") ? ntDI : 1'b1;
    DIFFI_OUT <= PAD;

    if (CP_IOB_MODE == "ADC")
    {
        ADC_IN <= PAD;
    }

    if (CP_IO_STANDARD == "SLVS" || CP_IO_STANDARD == "MIPI")
    {
        if (CP_IOB_MODE == "OBUFHR_T" || CP_IOB_MODE == "IOBUFHR")
        {
            operator V_OUTBUFT_MIPI_SLAVE V_OUTBUFT_MIPI_SLAVE
            parameter map
            (
                IOSTANDARD => "SLVS"
            )
            port map
            (
                IB => DO,
                I  => DIFFO_IN,
                TB => ntTS,
                M  => 1'b1,
                OB => PAD
            );
        }
    }
    else if (CP_IOB_MODE == "OBUFHR" || CP_IOB_MODE == "OBUFHR_T" || CP_IOB_MODE == "IOBUFHR")
    {
        operator V_OBUFT obuft
        parameter map
        (
            IOSTANDARD => CP_IO_STANDARD
        )
        port map
        (
            I => DO,
            T => ntTS,
            O => PAD
        );
    }
    else if (CP_IOB_MODE == "UNUSED")
    {
        PAD <= DIFFO_IN;
    }
};
