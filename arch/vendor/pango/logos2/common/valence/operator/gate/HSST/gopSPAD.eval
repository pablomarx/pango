gate
operator gopSPAD
{
    port
    (
        inout  PAD,
        output SPAD
    );
};


implementation impl_spad of gopSPAD
{
    device SPAD spad_inst
        port map 
        (
            PAD     => PAD,
            SPAD    => SPAD
        );
}; // end of implementation impl_spad of gopSPAD











